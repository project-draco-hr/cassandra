{
  boolean isInCondition=operator.equals(Relation.Type.IN);
  if (column.type instanceof CollectionType) {
    if (collectionElement == null)     return isInCondition ? new CollectionInBound(this,options) : new CollectionBound(this,options);
 else     return isInCondition ? new ElementAccessInBound(this,options) : new ElementAccessBound(this,options);
  }
  return isInCondition ? new SimpleInBound(this,options) : new SimpleBound(this,options);
}
