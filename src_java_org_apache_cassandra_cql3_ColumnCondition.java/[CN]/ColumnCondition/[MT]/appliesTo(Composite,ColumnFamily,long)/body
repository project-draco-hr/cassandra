{
  if (column.type instanceof CollectionType)   return collectionAppliesTo((CollectionType)column.type,rowPrefix,current,now);
  Cell c=current.getColumn(current.metadata().comparator.create(rowPrefix,column));
  ByteBuffer v=value.bindAndGet(variables);
  return v == null ? c == null || !c.isLive(now) : c != null && c.isLive(now) && c.value().equals(v);
}
