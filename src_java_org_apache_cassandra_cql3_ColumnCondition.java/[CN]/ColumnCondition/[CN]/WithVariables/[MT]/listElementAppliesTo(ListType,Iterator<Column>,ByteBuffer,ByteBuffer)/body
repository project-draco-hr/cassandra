{
  int idx=ByteBufferUtil.toInt(element);
  if (idx < 0)   throw new InvalidRequestException(String.format("Invalid negative list index %d",idx));
  int adv=Iterators.advance(iter,idx);
  if (adv != idx || !iter.hasNext())   throw new InvalidRequestException(String.format("List index %d out of bound, list has size %d",idx,adv));
  return type.elements.compare(iter.next().value(),value) == 0;
}
