{
  CollectionType type=(CollectionType)column.type;
  CFMetaData cfm=current.metadata();
  ColumnNameBuilder collectionPrefix=copyOrUpdatePrefix(cfm,rowPrefix).add(column.name.key);
  Iterator<Column> iter=collectionColumns(collectionPrefix,current,now);
  if (value == null)   return !iter.hasNext();
switch (type.kind) {
case LIST:
    return listAppliesTo((ListType)type,cfm,iter,((Lists.Value)value).elements);
case SET:
  return setAppliesTo((SetType)type,cfm,iter,((Sets.Value)value).elements);
case MAP:
return mapAppliesTo((MapType)type,cfm,iter,((Maps.Value)value).map);
}
throw new AssertionError();
}
