{
  List<CFMetaData> cfDefs=Arrays.asList(CFMetaData.BatchlogCf,CFMetaData.RangeXfersCf,CFMetaData.LocalCf,CFMetaData.PeersCf,CFMetaData.PeerEventsCf,CFMetaData.HintsCf,CFMetaData.IndexCf,CFMetaData.SchemaKeyspacesCf,CFMetaData.SchemaColumnFamiliesCf,CFMetaData.SchemaColumnsCf,CFMetaData.SchemaTriggersCf,CFMetaData.SchemaUserTypesCf,CFMetaData.CompactionLogCf,CFMetaData.CompactionHistoryCf,CFMetaData.PaxosCf,CFMetaData.SSTableActivityCF,CFMetaData.SizeEstimatesCf);
