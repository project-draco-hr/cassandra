{
  StringBuilder sb=new StringBuilder();
  sb.append(name).append(", rep strategy:").append(strategyClass.getSimpleName()).append("{").append(StringUtils.join(cfMetaData.values(),", ")).append("}").append(", strategy_options: ").append(strategyOptions.toString()).append(", durable_writes: ").append(durableWrites);
  return sb.toString();
}
