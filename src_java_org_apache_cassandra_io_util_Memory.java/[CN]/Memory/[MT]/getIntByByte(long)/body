{
  if (bigEndian) {
    return (((int)unsafe.getByte(address)) << 24) | (((int)unsafe.getByte(address + 1) & 0xff) << 16) | (((int)unsafe.getByte(address + 2) & 0xff) << 8)| (((int)unsafe.getByte(address + 3) & 0xff));
  }
 else {
    return (((int)unsafe.getByte(address + 3)) << 24) | (((int)unsafe.getByte(address + 2) & 0xff) << 16) | (((int)unsafe.getByte(address + 1) & 0xff) << 8)| (((int)unsafe.getByte(address) & 0xff));
  }
}
