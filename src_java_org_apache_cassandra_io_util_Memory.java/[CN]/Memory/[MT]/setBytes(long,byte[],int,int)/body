{
  if (buffer == null)   throw new NullPointerException();
 else   if (bufferOffset < 0 || count < 0 || bufferOffset + count > buffer.length)   throw new IndexOutOfBoundsException();
 else   if (count == 0)   return;
  checkPosition(memoryOffset);
  long end=memoryOffset + count;
  checkPosition(end - 1);
  while (memoryOffset < end)   unsafe.putByte(peer + memoryOffset++,buffer[bufferOffset++]);
}
