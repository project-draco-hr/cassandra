{
switch (nametype()) {
case SIMPLE_DENSE:
case SIMPLE_SPARSE:
    return get(0);
case COMPOUND_DENSE:
case COMPOUND_SPARSE_STATIC:
case COMPOUND_SPARSE:
  ByteBuffer result=ByteBuffer.allocate(cellDataSize());
if (isStatic()) ByteBufferUtil.writeShortLength(result,CompositeType.STATIC_MARKER);
for (int i=0; i < size(); i++) {
ByteBuffer bb=get(i);
ByteBufferUtil.writeShortLength(result,bb.remaining());
result.put(bb);
result.put((byte)0);
}
result.flip();
return result;
default :
throw new AssertionError();
}
}
