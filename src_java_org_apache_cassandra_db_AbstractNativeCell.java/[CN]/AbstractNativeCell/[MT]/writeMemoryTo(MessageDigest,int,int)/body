{
  int i=0, batchEnd=count - 16;
  if (i < batchEnd) {
    byte[] buffer=BUFFER.get();
    while (i < batchEnd) {
      int transfer=Math.min(count - i,256);
      getBytes(from + i,buffer,0,transfer);
      digest.update(buffer,0,transfer);
      i+=transfer;
    }
  }
  while (i < count)   digest.update(getByte(from + i++));
}
