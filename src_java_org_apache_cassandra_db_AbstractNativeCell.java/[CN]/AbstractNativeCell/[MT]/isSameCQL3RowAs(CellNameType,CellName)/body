{
switch (nametype()) {
case SIMPLE_DENSE:
case COMPOUND_DENSE:
    return type.compare(this,other) == 0;
case COMPOUND_SPARSE_STATIC:
case COMPOUND_SPARSE:
  int clusteringSize=clusteringSize();
if (clusteringSize != other.clusteringSize() || other.isStatic() != isStatic()) return false;
for (int i=0; i < clusteringSize; i++) if (type.subtype(i).compare(get(i),other.get(i)) != 0) return false;
return true;
case SIMPLE_SPARSE:
return true;
default :
throw new AssertionError();
}
}
