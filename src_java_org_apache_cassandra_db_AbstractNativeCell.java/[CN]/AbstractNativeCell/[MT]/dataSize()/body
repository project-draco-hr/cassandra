{
switch (nametype()) {
case SIMPLE_DENSE:
case SIMPLE_SPARSE:
    return valueStartOffset() - nameDeltaOffset(size());
case COMPOUND_DENSE:
case COMPOUND_SPARSE_STATIC:
case COMPOUND_SPARSE:
  int size=size();
return valueStartOffset() - nameDeltaOffset(size) + 3 * size + (isStatic() ? 2 : 0);
default :
throw new AssertionError();
}
}
