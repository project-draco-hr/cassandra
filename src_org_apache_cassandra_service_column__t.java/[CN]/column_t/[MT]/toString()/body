{
  StringBuilder sb=new StringBuilder("column_t(");
  boolean first=true;
  if (!first)   sb.append(", ");
  sb.append("columnName:");
  sb.append(this.columnName);
  first=false;
  if (!first)   sb.append(", ");
  sb.append("value:");
  sb.append(this.value);
  first=false;
  if (!first)   sb.append(", ");
  sb.append("timestamp:");
  sb.append(this.timestamp);
  first=false;
  sb.append(")");
  return sb.toString();
}
