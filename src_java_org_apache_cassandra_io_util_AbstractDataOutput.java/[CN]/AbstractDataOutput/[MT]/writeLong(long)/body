{
  byte[] buffer=new byte[8];
  int t=(int)(val >> 32);
  buffer[0]=(byte)(t >> 24);
  buffer[1]=(byte)(t >> 16);
  buffer[2]=(byte)(t >> 8);
  buffer[3]=(byte)t;
  buffer[4]=(byte)(val >> 24);
  buffer[5]=(byte)(val >> 16);
  buffer[6]=(byte)(val >> 8);
  buffer[7]=(byte)val;
  write(buffer,0,buffer.length);
}
