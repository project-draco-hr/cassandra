{
  byte newBytes[]=new byte[str.length() * 2];
  for (int index=0; index < str.length(); index++) {
    int newIndex=index == 0 ? index : index * 2;
    newBytes[newIndex]=(byte)((str.charAt(index) >> 8) & 0xFF);
    newBytes[newIndex + 1]=(byte)(str.charAt(index) & 0xFF);
  }
  write(newBytes);
}
