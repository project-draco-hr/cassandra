{
  StringBuilder sb=new StringBuilder();
  Set<InetAddress> eps=tokenToEndPointMap.inverse().keySet();
  for (  InetAddress ep : eps) {
    sb.append(ep);
    sb.append(":");
    sb.append(tokenToEndPointMap.inverse().get(ep));
    sb.append(System.getProperty("line.separator"));
  }
  return sb.toString();
}
