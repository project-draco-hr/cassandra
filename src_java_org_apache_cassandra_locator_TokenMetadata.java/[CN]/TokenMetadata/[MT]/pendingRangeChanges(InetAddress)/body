{
  int n=0;
  Collection<Range<Token>> sourceRanges=getPrimaryRangesFor(getTokens(source));
  lock.readLock().lock();
  try {
    for (    Token token : bootstrapTokens.keySet())     for (    Range<Token> range : sourceRanges)     if (range.contains(token))     n++;
  }
  finally {
    lock.readLock().unlock();
  }
  return n;
}
