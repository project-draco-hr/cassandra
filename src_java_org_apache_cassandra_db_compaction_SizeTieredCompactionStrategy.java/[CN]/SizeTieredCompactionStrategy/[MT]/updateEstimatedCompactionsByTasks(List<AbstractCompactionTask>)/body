{
  int n=0;
  for (  AbstractCompactionTask task : tasks) {
    if (!(task instanceof CompactionTask))     continue;
    Collection<SSTableReader> sstablesToBeCompacted=task.getSSTables();
    if (sstablesToBeCompacted.size() >= cfs.getMinimumCompactionThreshold())     n+=Math.ceil((double)sstablesToBeCompacted.size() / cfs.getMaximumCompactionThreshold());
  }
  estimatedRemainingTasks=n;
}
