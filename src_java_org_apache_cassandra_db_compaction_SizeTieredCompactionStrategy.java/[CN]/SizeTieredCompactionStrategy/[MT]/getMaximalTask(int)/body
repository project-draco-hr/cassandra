{
  Iterable<SSTableReader> filteredSSTables=filterSuspectSSTables(sstables);
  if (Iterables.isEmpty(sstables))   return null;
  if (!cfs.getDataTracker().markCompacting(filteredSSTables))   return null;
  return Arrays.<AbstractCompactionTask>asList(new CompactionTask(cfs,filteredSSTables,gcBefore,false));
}
