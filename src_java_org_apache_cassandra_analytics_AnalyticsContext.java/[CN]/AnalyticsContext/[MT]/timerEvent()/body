{
  if (isMonitoring) {
    Collection<IAnalyticsSource> myUpdaters;
    myUpdaters=new ArrayList<IAnalyticsSource>(updaters);
    for (    IAnalyticsSource updater : myUpdaters) {
      try {
        updater.doUpdates(this);
      }
 catch (      Throwable throwable) {
        throwable.printStackTrace();
      }
    }
    emitRecords();
  }
}
