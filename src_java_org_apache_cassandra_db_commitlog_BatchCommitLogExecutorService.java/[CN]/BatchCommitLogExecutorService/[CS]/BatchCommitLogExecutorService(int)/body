{
  queue=new LinkedBlockingQueue<CheaterFutureTask>(queueSize);
  Runnable runnable=new WrappedRunnable(){
    public void runMayThrow() throws Exception {
      while (true) {
        processWithSyncBatch();
        completedTaskCount++;
      }
    }
  }
;
  new Thread(runnable,"COMMIT-LOG-WRITER").start();
  registerMBean(this);
}
