{
  if (!hasParent())   return true;
  MBeanServer mbs=ManagementFactory.getPlatformMBeanServer();
  try {
    return !(mbs.queryNames(new ObjectName(name),null).isEmpty());
  }
 catch (  MalformedObjectNameException e) {
    return false;
  }
catch (  NullPointerException e) {
    return false;
  }
}
