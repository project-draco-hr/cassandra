{
  Table table=Table.open(tableName);
  logger.info("Flushing memtables for {}...",tableName);
  for (  Future<?> f : table.flush()) {
    try {
      f.get();
    }
 catch (    InterruptedException e) {
      throw new RuntimeException(e);
    }
catch (    ExecutionException e) {
      throw new RuntimeException(e);
    }
  }
  return table;
}
