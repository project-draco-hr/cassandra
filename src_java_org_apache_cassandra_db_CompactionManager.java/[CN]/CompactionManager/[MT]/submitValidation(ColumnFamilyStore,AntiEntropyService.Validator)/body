{
  Callable<Object> callable=new Callable<Object>(){
    public Object call() throws IOException {
      compactionLock.lock();
      try {
        if (!cfStore.isInvalid())         doValidationCompaction(cfStore,validator);
        return this;
      }
  finally {
        compactionLock.unlock();
      }
    }
  }
;
  return executor.submit(callable);
}
