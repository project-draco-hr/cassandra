{
  if (descriptor.isLatestVersion && !forceDeserialize)   return new EchoedRow(row);
  return row.dataSize > DatabaseDescriptor.getInMemoryCompactionLimit() ? new LazilyCompactedRow(cfs,Arrays.asList(row),false,getDefaultGcBefore(cfs),forceDeserialize) : new PrecompactedRow(cfs,Arrays.asList(row),false,getDefaultGcBefore(cfs),forceDeserialize);
}
