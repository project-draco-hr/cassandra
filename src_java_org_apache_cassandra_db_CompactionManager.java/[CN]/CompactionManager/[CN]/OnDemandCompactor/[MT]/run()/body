{
  if (logger_.isDebugEnabled())   logger_.debug("Started  Major compaction for " + columnFamilyStore_.columnFamily_);
  try {
    columnFamilyStore_.doMajorCompaction(skip_);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  if (logger_.isDebugEnabled())   logger_.debug("Finished Major compaction for " + columnFamilyStore_.columnFamily_);
}
