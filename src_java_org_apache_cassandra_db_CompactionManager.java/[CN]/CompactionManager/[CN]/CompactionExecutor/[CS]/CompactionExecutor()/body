{
  super(getThreadCount(),60,TimeUnit.SECONDS,new LinkedBlockingQueue<Runnable>(),new NamedThreadFactory("CompactionExecutor",DatabaseDescriptor.getCompactionThreadPriority()));
  Map<CompactionInfo.Holder,Boolean> cmap=new IdentityHashMap<CompactionInfo.Holder,Boolean>();
  compactions=Collections.synchronizedSet(Collections.newSetFromMap(cmap));
}
