{
  if (logger_.isDebugEnabled())   logger_.debug("Started  compaction ..." + columnFamilyStore_.columnFamily_);
  try {
    columnFamilyStore_.doCleanupCompaction();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  if (logger_.isDebugEnabled())   logger_.debug("Finished compaction ..." + columnFamilyStore_.columnFamily_);
}
