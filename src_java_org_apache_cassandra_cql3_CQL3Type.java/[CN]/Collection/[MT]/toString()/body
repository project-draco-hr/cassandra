{
switch (type.kind) {
case LIST:
    return "list<" + ((ListType)type).elements.asCQL3Type() + ">";
case SET:
  return "set<" + ((SetType)type).elements.asCQL3Type() + ">";
case MAP:
MapType mt=(MapType)type;
return "map<" + mt.keys.asCQL3Type() + ", "+ mt.values.asCQL3Type()+ ">";
}
throw new AssertionError();
}
