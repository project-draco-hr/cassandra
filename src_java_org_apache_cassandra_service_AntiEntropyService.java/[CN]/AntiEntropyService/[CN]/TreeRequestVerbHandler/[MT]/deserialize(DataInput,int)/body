{
  String sessId=dis.readUTF();
  InetAddress endpoint=CompactEndpointSerializationHelper.deserialize(dis);
  CFPair cfpair=new CFPair(dis.readUTF(),dis.readUTF());
  Range<Token> range;
  if (version > MessagingService.VERSION_07)   range=(Range<Token>)AbstractBounds.serializer().deserialize(dis,version);
 else   range=new Range<Token>(StorageService.getPartitioner().getMinimumToken(),StorageService.getPartitioner().getMinimumToken());
  return new TreeRequest(sessId,endpoint,range,cfpair);
}
