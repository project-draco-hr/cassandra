{
  Long curtime=System.currentTimeMillis();
  Long pretime=naturalRepairs.putIfAbsent(cf,curtime);
  if (pretime != null) {
    if (pretime < (curtime - NATURAL_REPAIR_FREQUENCY))     return naturalRepairs.replace(cf,pretime,curtime);
    logger.debug("Skipping natural repair: last occurred " + (curtime - pretime) + "ms ago.");
    return false;
  }
  return true;
}
