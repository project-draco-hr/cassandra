{
  StringBuilder query=new StringBuilder("SELECT ");
  if (state.isCql2())   query.append(state.settings.columns.maxColumnsPerKey).append(" ''..''");
 else   query.append("*");
  query.append(" FROM Standard1");
  if (state.isCql2())   query.append(" USING CONSISTENCY ").append(state.settings.command.consistencyLevel);
  final String columnName=getColumnName(1);
  query.append(" WHERE ").append(columnName).append("=?").append(" AND KEY > ? LIMIT ").append(((SettingsCommandMulti)state.settings.command).keysAtOnce);
  return query.toString();
}
