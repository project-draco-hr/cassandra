{
  final String indexColumn=(state.settings.columns.namestrs.get(1));
  StringBuilder query=new StringBuilder("SELECT * FROM ");
  query.append(wrapInQuotes(state.type.table));
  query.append(" WHERE ").append(indexColumn).append("=?").append(" AND KEY > ? LIMIT ").append(state.settings.command.keysAtOnce);
  return query.toString();
}
