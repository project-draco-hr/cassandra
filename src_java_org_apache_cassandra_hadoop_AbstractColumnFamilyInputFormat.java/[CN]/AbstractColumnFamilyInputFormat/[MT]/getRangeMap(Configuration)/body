{
  Cassandra.Client client=ConfigHelper.getClientFromInputAddressList(conf);
  List<TokenRange> map;
  try {
    map=client.describe_local_ring(ConfigHelper.getInputKeyspace(conf));
  }
 catch (  InvalidRequestException e) {
    throw new RuntimeException(e);
  }
catch (  TException e) {
    throw new RuntimeException(e);
  }
  return map;
}
