{
  int size=dis.readInt();
  SortedSet<ByteBuffer> columns=new TreeSet<ByteBuffer>(comparator);
  for (int i=0; i < size; ++i)   columns.add(ByteBufferUtil.readWithShortLength(dis));
  boolean countCQL3Rows=version >= MessagingService.VERSION_12 ? dis.readBoolean() : false;
  return new NamesQueryFilter(columns,countCQL3Rows);
}
