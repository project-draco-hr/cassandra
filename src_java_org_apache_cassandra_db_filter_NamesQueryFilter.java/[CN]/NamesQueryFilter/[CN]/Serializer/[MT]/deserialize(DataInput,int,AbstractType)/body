{
  int size=in.readInt();
  SortedSet<ByteBuffer> columns=new TreeSet<ByteBuffer>(comparator);
  for (int i=0; i < size; ++i)   columns.add(ByteBufferUtil.readWithShortLength(in));
  boolean countCQL3Rows=version >= MessagingService.VERSION_12 ? in.readBoolean() : false;
  return new NamesQueryFilter(columns,countCQL3Rows);
}
