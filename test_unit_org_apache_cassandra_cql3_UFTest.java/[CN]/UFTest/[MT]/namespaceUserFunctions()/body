{
  createTable("CREATE TABLE %s (key int primary key, val double)");
  execute("create or replace function math::sin ( input double ) returns double 'org.apache.cassandra.cql3.UFTest'");
  execute("INSERT INTO %s (key, val) VALUES (?, ?)",1,1d);
  execute("INSERT INTO %s (key, val) VALUES (?, ?)",2,2d);
  execute("INSERT INTO %s (key, val) VALUES (?, ?)",3,3d);
  assertRows(execute("SELECT key, val, math::sin(val) FROM %s"),row(1,1d,Math.sin(1d)),row(2,2d,Math.sin(2d)),row(3,3d,Math.sin(3d)));
}
