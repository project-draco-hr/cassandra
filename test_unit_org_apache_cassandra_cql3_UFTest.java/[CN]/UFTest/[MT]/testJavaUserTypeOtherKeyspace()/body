{
  String type=KEYSPACE + '.' + createType("CREATE TYPE %s (txt text, i int)");
  String fName=createFunction(KEYSPACE_PER_TEST,"frozen<" + type + '>',"CREATE FUNCTION %s( udt frozen<" + type + "> ) "+ "RETURNS frozen<"+ type+ "> "+ "LANGUAGE java "+ "AS $$return "+ "     udt;$$;");
  execute("DROP FUNCTION " + fName);
}
