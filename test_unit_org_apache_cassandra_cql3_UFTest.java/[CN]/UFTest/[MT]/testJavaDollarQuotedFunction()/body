{
  String functionBody="\n" + "  // parameter val is of type java.lang.Double\n" + "  /* return type is of type java.lang.Double */\n"+ "  if (input == null) {\n"+ "    return null;\n"+ "  }\n"+ "  double v = Math.sin( input.doubleValue() );\n"+ "  return \"'\"+Double.valueOf(v)+'\\\'';\n";
  execute("create function foo::pgfun1 ( input double ) returns text language java\n" + "AS $$" + functionBody + "$$;");
  execute("CREATE FUNCTION foo::pgsin ( input double ) RETURNS double LANGUAGE java AS $$return Double.valueOf(Math.sin(input.doubleValue()));$$");
  assertRows(execute("SELECT language, body FROM system.schema_functions WHERE namespace='foo' AND name='pgfun1'"),row("java",functionBody));
}
