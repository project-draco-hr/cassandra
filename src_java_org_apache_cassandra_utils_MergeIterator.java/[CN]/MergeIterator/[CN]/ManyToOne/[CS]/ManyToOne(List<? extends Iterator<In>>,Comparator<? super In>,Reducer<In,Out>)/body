{
  super(iters,reducer);
  @SuppressWarnings("unchecked") Candidate<In>[] heap=new Candidate[iters.size()];
  this.heap=heap;
  size=0;
  for (int i=0; i < iters.size(); i++) {
    Candidate<In> candidate=new Candidate<>(i,iters.get(i),comp);
    heap[size++]=candidate;
  }
  needingAdvance=size;
}
