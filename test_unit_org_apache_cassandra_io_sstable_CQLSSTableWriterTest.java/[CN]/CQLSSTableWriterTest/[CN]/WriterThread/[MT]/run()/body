{
  String insert="INSERT INTO cql_keyspace2.table2 (k, v) VALUES (?, ?)";
  CQLSSTableWriter writer=CQLSSTableWriter.builder().withCfs(cfs).using(insert).build();
  try {
    for (int i=0; i < NUMBER_WRITES_IN_RUNNABLE; i++) {
      writer.addRow(id,i);
    }
    writer.close();
  }
 catch (  Exception e) {
    exception=e;
  }
}
