{
  executor.execute(new Runnable(){
    public void run(){
switch (getDaemonGatewayService()) {
case Thrift:
default :
        daemon=new org.apache.cassandra.thrift.CassandraDaemon();
    }
    daemon.activate();
  }
}
);
try {
  TimeUnit.SECONDS.sleep(3);
}
 catch (InterruptedException e) {
  throw new AssertionError(e);
}
}
