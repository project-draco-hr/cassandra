{
  if (current instanceof RangeTombstone) {
    tombstone=(RangeTombstone)current;
  }
 else {
    Column column=(Column)current;
    container.addColumn(column);
    if (indexer != SecondaryIndexManager.nullUpdater && !column.isMarkedForDelete() && !container.getColumn(column.name()).equals(column)) {
      indexer.remove(column);
    }
  }
}
