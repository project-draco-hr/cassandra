{
  Filter bf=FilterFactory.getFilter(1000000,0.0001);
  for (int i=0; i < 100; i++)   bf.add(StorageService.getPartitioner().getTokenFactory().toByteArray(StorageService.getPartitioner().getRandomToken()));
  DataOutputStream out=getOutput("utils.BloomFilter.bin");
  FilterFactory.serialize(bf,out,murmur);
  out.close();
}
