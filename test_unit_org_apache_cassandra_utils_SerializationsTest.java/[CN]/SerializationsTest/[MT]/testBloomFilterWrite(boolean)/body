{
  IFilter bf=FilterFactory.getFilter(1000000,0.0001,offheap);
  for (int i=0; i < 100; i++)   bf.add(StorageService.getPartitioner().getTokenFactory().toByteArray(StorageService.getPartitioner().getRandomToken()));
  DataOutputStreamAndChannel out=getOutput("utils.BloomFilter.bin");
  FilterFactory.serialize(bf,out);
  out.close();
  bf.close();
}
