{
  super(name);
  this.modifyKeysForRead_=new HashSet<SelectionKey>();
  this.modifyKeysForWrite_=new HashSet<SelectionKey>();
  this.cancelledKeys_=new HashSet<SelectionKey>();
  try {
    selector_=Selector.open();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  setDaemon(false);
  start();
}
