{
switch (fieldID) {
case TABLE:
    if (value == null) {
      unsetTable();
    }
 else {
      setTable((String)value);
    }
  break;
case KEY:
if (value == null) {
  unsetKey();
}
 else {
  setKey((String)value);
}
break;
case CFMAP:
if (value == null) {
unsetCfmap();
}
 else {
setCfmap((Map<String,List<superColumn_t>>)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
