{
  String req="SELECT token_bytes FROM system.%s WHERE key='%s'";
  UntypedResultSet result=processInternal(String.format(req,LOCAL_CF,LOCAL_KEY));
  return result.isEmpty() || !result.one().has("token_bytes") ? null : StorageService.getPartitioner().getTokenFactory().fromByteArray(result.one().getBytes("token_bytes"));
}
