{
  ColumnFamilyStore cfs=Table.open(Table.SYSTEM_KS).getColumnFamilyStore(INDEX_CF);
  QueryFilter filter=QueryFilter.getNamesFilter(decorate(ByteBufferUtil.bytes(table)),INDEX_CF,ByteBufferUtil.bytes(indexName),System.currentTimeMillis());
  return ColumnFamilyStore.removeDeleted(cfs.getColumnFamily(filter),Integer.MAX_VALUE) != null;
}
