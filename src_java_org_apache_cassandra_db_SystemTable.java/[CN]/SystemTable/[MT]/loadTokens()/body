{
  IPartitioner p=StorageService.getPartitioner();
  HashMap<Token,InetAddress> tokenMap=new HashMap<Token,InetAddress>();
  for (  UntypedResultSet.Row row : processInternal("SELECT * FROM system." + PEERS_CF))   tokenMap.put(p.getTokenFactory().fromByteArray(row.getBytes("token_bytes")),row.getInetAddress("peer"));
  return tokenMap;
}
