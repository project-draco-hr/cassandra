{
  processInternal(String.format("UPDATE %s USING TIMESTAMP %d AND TTL %d SET proposal = 0x%s WHERE row_key = 0x%s AND cf_id = %s",PAXOS_CF,UUIDGen.microsTimestamp(commit.ballot),commit.update.metadata().getGcGraceSeconds(),ByteBufferUtil.bytesToHex(commit.update.toBytes()),ByteBufferUtil.bytesToHex(commit.key),commit.update.id()));
}
