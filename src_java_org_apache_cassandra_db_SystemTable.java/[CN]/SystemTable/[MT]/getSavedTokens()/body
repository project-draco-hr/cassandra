{
  String req="SELECT tokens FROM system.%s WHERE key='%s'";
  UntypedResultSet result=processInternal(String.format(req,LOCAL_CF,LOCAL_KEY));
  return result.isEmpty() || !result.one().has("tokens") ? Collections.<Token>emptyList() : deserializeTokens(result.one().<ByteBuffer>getSet("tokens",BytesType.instance));
}
