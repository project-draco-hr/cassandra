{
  UntypedResultSet results=QueryProcessor.resultify("SELECT * FROM system." + SystemKeyspace.SCHEMA_AGGREGATES_TABLE,row);
  Map<Composite,UDAggregate> udfs=new HashMap<>(results.size());
  for (  UntypedResultSet.Row result : results)   udfs.put(SystemKeyspace.SchemaAggregatesTable.comparator.make(result.getString("aggregate_name"),result.getBlob("signature")),fromSchema(result));
  return udfs;
}
