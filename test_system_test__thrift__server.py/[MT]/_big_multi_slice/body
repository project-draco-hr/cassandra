def _big_multi_slice(key='abc'):
    c1 = ColumnSlice()
    c1.start = 'a'
    c1.finish = 'c'
    c2 = ColumnSlice()
    c2.start = 'e'
    c2.finish = 'f'
    m = MultiSliceRequest()
    m.key = key
    m.column_parent = ColumnParent('Standard1')
    m.column_slices = [c1, c2]
    m.reversed = False
    m.count = 10
    m.consistency_level = ConsistencyLevel.ONE
    return client.get_multi_slice(m)
