def _insert_multi_batch(keys, block):
    cfmap = {'Standard1': [ColumnOrSuperColumn(c) for c in _SIMPLE_COLUMNS], 'Standard2': [ColumnOrSuperColumn(c) for c in _SIMPLE_COLUMNS], }
    if block:
        consistencyLevel = ConsistencyLevel.ONE
    else:
        consistencyLevel = ConsistencyLevel.ZERO
    for key in keys:
        client.batch_insert(key, cfmap, consistencyLevel)
