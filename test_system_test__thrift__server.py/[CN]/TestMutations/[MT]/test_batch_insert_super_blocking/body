def test_batch_insert_super_blocking(self):
    _set_keyspace('Keyspace1')
    cfmap = {'Super1': [ColumnOrSuperColumn(super_column=c) for c in _SUPER_COLUMNS], 'Super2': [ColumnOrSuperColumn(super_column=c) for c in _SUPER_COLUMNS], }
    client.batch_insert('key1', cfmap, ConsistencyLevel.ONE)
    _verify_super('Super1')
    _verify_super('Super2')
