def test_missing_super(self):
    _set_keyspace('Keyspace1')
    _expect_missing((lambda : client.get('key1', ColumnPath('Super1', 'sc1', _i64(1)), ConsistencyLevel.ONE)))
    _insert_super()
    _expect_missing((lambda : client.get('key1', ColumnPath('Super1', 'sc1', _i64(1)), ConsistencyLevel.ONE)))
