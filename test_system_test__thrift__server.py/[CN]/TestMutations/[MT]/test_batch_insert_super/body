def test_batch_insert_super(self):
    _empty_login('Keyspace1')
    cfmap = {'Super1': [ColumnOrSuperColumn(super_column=c) for c in _SUPER_COLUMNS], 'Super2': [ColumnOrSuperColumn(super_column=c) for c in _SUPER_COLUMNS], }
    client.batch_insert('key1', cfmap, ConsistencyLevel.ZERO)
    waitfor(ZERO_WAIT, _verify_super, 'Super1')
    waitfor(ZERO_WAIT, _verify_super, 'Super2')
