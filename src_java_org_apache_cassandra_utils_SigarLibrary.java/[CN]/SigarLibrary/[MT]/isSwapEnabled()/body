{
  try {
    Swap swap=sigar.getSwap();
    long swapSize=swap.getTotal();
    if (swapSize > 0) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  SigarException sigarException) {
    logger.warn("Could not determine if swap configuration is acceptable. Error message: {}",sigarException);
    return false;
  }
}
