{
  BigInteger h=BigInteger.ZERO;
  char val[]=key.toCharArray();
  for (int i=0; i < OrderPreservingHashPartitioner.maxKeyHashLength_; i++) {
    if (i < val.length)     h=OrderPreservingHashPartitioner.prime_.multiply(h).add(BigInteger.valueOf(val[i]));
 else     h=OrderPreservingHashPartitioner.prime_.multiply(h).add(OrderPreservingHashPartitioner.prime_);
  }
  return h;
}
