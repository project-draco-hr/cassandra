{
  if (type instanceof LongType || type instanceof DateType)   return DataType.LONG;
 else   if (type instanceof IntegerType || type instanceof Int32Type)   return DataType.INTEGER;
 else   if (type instanceof AsciiType || type instanceof UTF8Type || type instanceof DecimalType|| type instanceof InetAddressType|| type instanceof LexicalUUIDType|| type instanceof UUIDType)   return DataType.CHARARRAY;
 else   if (type instanceof FloatType)   return DataType.FLOAT;
 else   if (type instanceof DoubleType)   return DataType.DOUBLE;
 else   if (type instanceof AbstractCompositeType || type instanceof CollectionType)   return DataType.TUPLE;
  return DataType.BYTEARRAY;
}
