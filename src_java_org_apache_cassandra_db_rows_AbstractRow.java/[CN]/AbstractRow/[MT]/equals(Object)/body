{
  if (!(other instanceof Row))   return false;
  Row that=(Row)other;
  if (!this.clustering().equals(that.clustering()) || !this.columns().equals(that.columns()) || !this.primaryKeyLivenessInfo().equals(that.primaryKeyLivenessInfo())|| !this.deletion().equals(that.deletion()))   return false;
  Iterator<Cell> thisCells=this.iterator();
  Iterator<Cell> thatCells=that.iterator();
  while (thisCells.hasNext()) {
    if (!thatCells.hasNext() || !thisCells.next().equals(thatCells.next()))     return false;
  }
  return !thatCells.hasNext();
}
