{
  if (options.containsKey(TRUSTSTORE))   truststore=options.get(TRUSTSTORE);
  if (options.containsKey(TRUSTSTORE_PASSWORD))   truststorePassword=options.get(TRUSTSTORE_PASSWORD);
  if (options.containsKey(KEYSTORE))   keystore=options.get(KEYSTORE);
  if (options.containsKey(KEYSTORE_PASSWORD))   keystorePassword=options.get(KEYSTORE_PASSWORD);
  if (options.containsKey(PROTOCOL))   protocol=options.get(PROTOCOL);
  if (options.containsKey(CIPHER_SUITES))   cipherSuites=options.get(CIPHER_SUITES).split(",");
}
