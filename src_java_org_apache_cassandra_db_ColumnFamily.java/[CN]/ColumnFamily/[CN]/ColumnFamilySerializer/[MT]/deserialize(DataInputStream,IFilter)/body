{
  ColumnFamily cf=defreezeColumnFamily(dis);
  int size=dis.readInt();
  IColumn column=null;
  for (int i=0; i < size; ++i) {
    column=cf.getColumnSerializer().deserialize(dis,filter);
    if (column != null) {
      cf.addColumn(column);
      column=null;
      if (filter.isDone()) {
        break;
      }
    }
  }
  return cf;
}
