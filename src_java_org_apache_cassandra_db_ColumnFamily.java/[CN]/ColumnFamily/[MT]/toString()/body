{
  StringBuilder sb=new StringBuilder();
  sb.append("ColumnFamily(");
  sb.append(name_);
  if (isMarkedForDelete()) {
    sb.append(" -delete at " + getMarkedForDeleteAt() + "-");
  }
  sb.append(" [");
  sb.append(StringUtils.join(getAllColumns(),", "));
  sb.append("])");
  return sb.toString();
}
