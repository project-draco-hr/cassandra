{
  if (this == o)   return true;
  if (o == null || this.getClass() != o.getClass())   return false;
  ColumnFamily comparison=(ColumnFamily)o;
  return cfm.equals(comparison.cfm) && deletionInfo().equals(comparison.deletionInfo()) && ByteBufferUtil.compareUnsigned(digest(this),digest(comparison)) == 0;
}
