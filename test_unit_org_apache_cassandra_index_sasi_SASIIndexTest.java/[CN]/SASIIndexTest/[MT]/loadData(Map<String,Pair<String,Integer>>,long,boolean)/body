{
  for (  Map.Entry<String,Pair<String,Integer>> e : data.entrySet())   newMutation(e.getKey(),e.getValue().left,null,e.getValue().right,timestamp).apply();
  ColumnFamilyStore store=Keyspace.open(KS_NAME).getColumnFamilyStore(CF_NAME);
  if (forceFlush)   store.forceBlockingFlush();
  return store;
}
