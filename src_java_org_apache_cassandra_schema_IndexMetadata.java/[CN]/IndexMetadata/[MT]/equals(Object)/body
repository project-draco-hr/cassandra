{
  if (obj == this)   return true;
  if (!(obj instanceof IndexMetadata))   return false;
  IndexMetadata other=(IndexMetadata)obj;
  return Objects.equal(name,other.name) && Objects.equal(indexType,other.indexType) && Objects.equal(targetType,other.targetType)&& Objects.equal(options,other.options)&& Objects.equal(columns,other.columns);
}
