{
  String targetFile=dis.readUTF();
  long expectedBytes=dis.readLong();
  FileStatus streamStatus=new FileStatus(targetFile,expectedBytes);
  int ordinal=dis.readInt();
  if (ordinal == Action.DELETE.ordinal())   streamStatus.setAction(Action.DELETE);
 else   if (ordinal == Action.STREAM.ordinal())   streamStatus.setAction(Action.STREAM);
 else   throw new IOException("Bad FileStatus.Action: " + ordinal);
  return streamStatus;
}
