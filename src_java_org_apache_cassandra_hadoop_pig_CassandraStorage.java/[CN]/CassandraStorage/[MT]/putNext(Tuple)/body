{
  if (t.size() < 1) {
    logger.warn("Empty output skipped, filter empty tuples to suppress this warning");
    return;
  }
  ByteBuffer key=objToBB(t.get(0));
  if (t.getType(1) == DataType.TUPLE)   writeColumnsFromTuple(key,t,1);
 else   if (t.getType(1) == DataType.BAG) {
    if (t.size() > 2)     throw new IOException("No arguments allowed after bag");
    writeColumnsFromBag(key,(DataBag)t.get(1));
  }
 else   throw new IOException("Second argument in output must be a tuple or bag");
}
