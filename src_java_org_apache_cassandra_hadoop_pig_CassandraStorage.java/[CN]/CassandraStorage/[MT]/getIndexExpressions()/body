{
  UDFContext context=UDFContext.getUDFContext();
  Properties property=context.getUDFProperties(CassandraStorage.class);
  if (property.getProperty(StorageHelper.PARTITION_FILTER_SIGNATURE) != null)   return indexExpressionsFromString(property.getProperty(StorageHelper.PARTITION_FILTER_SIGNATURE));
 else   return null;
}
