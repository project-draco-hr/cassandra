{
  List<AbstractCompositeType.CompositeComponent> result=comparator.deconstruct(name);
  Tuple t=TupleFactory.getInstance().newTuple(result.size());
  for (int i=0; i < result.size(); i++)   StorageHelper.setTupleValue(t,i,StorageHelper.cassandraToObj(result.get(i).comparator,result.get(i).value,nativeProtocolVersion));
  return t;
}
