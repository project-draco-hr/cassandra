{
  if (!oldCell.name().equals(newCell.name()))   throw new IllegalArgumentException();
  Cell previous=map.put(oldCell.name(),newCell);
  if (previous == null) {
    map.remove(oldCell.name());
    return false;
  }
  if (!previous.equals(oldCell)) {
    map.put(oldCell.name(),previous);
    return false;
  }
  return true;
}
