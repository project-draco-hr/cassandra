{
  ColumnFamily cf;
  try {
    file.seek(dataStart);
    IndexHelper.skipBloomFilter(file);
    IndexHelper.skipIndex(file);
    cf=sstable.makeColumnFamily();
    ColumnFamily.serializer().deserializeFromSSTableNoColumns(cf,file);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  return cf;
}
