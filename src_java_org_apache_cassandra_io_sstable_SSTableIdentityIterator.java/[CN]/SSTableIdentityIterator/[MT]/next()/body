{
  try {
    return doCompute();
  }
 catch (  IndexOutOfBoundsException e) {
    sstable.markSuspect();
    throw new CorruptSSTableException(e,filename);
  }
catch (  IOError e) {
    if (e.getCause() instanceof IOException) {
      sstable.markSuspect();
      throw new CorruptSSTableException((Exception)e.getCause(),filename);
    }
 else {
      throw e;
    }
  }
}
