{
  try {
    OnDiskAtom atom=atomSerializer.deserializeFromSSTable(inputWithTracker,flag,expireBefore,dataVersion);
    if (validateColumns)     atom.validateFields(columnFamily.metadata());
    return atom;
  }
 catch (  IOException e) {
    throw new IOError(e);
  }
catch (  MarshalException e) {
    throw new IOError(new IOException("Error validating row " + key,e));
  }
}
