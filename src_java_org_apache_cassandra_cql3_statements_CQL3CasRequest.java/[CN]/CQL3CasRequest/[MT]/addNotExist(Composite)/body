{
  RowCondition previous=conditions.put(prefix,new NotExistCondition(prefix,now));
  if (previous != null && !(previous instanceof NotExistCondition)) {
    if (previous instanceof ExistCondition)     throw new InvalidRequestException("Cannot mix IF EXISTS and IF NOT EXISTS conditions for the same row");
 else     throw new InvalidRequestException("Cannot mix IF conditions and IF NOT EXISTS for the same row");
  }
}
