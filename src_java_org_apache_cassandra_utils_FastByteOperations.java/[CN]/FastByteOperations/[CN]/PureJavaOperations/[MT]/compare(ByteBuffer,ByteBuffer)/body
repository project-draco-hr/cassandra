{
  int end1=buffer1.limit();
  int end2=buffer2.limit();
  for (int i=buffer1.position(), j=buffer2.position(); i < end1 && j < end2; i++, j++) {
    int a=(buffer1.get(i) & 0xff);
    int b=(buffer2.get(j) & 0xff);
    if (a != b) {
      return a - b;
    }
  }
  return buffer1.remaining() - buffer2.remaining();
}
