{
  final Object obj1;
  final long offset1;
  final int length1;
  if (buffer1.hasArray()) {
    obj1=buffer1.array();
    offset1=BYTE_ARRAY_BASE_OFFSET + buffer1.arrayOffset() + buffer1.position();
