{
  Object src;
  long srcOffset;
  if (srcBuf.hasArray()) {
    src=srcBuf.array();
    srcOffset=BYTE_ARRAY_BASE_OFFSET + srcBuf.arrayOffset();
  }
 else {
    src=null;
    srcOffset=theUnsafe.getLong(srcBuf,DIRECT_BUFFER_ADDRESS_OFFSET);
  }
  copy(src,srcOffset + srcPosition,trgBuf,trgPosition,length);
}
