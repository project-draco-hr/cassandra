{
  writeBody_=writeBody;
  sb_.append("<html>\n");
  if (writeCSS || writeJSCallback) {
    sb_.append("<head>\n");
    if (writeJSCallback)     addJSCallback(jsCallbackFunction);
    if (writeCSS)     addCSS();
    sb_.append("</head>\n");
  }
  if (writeBody) {
    sb_.append("<body bgcolor=black>\n");
  }
}
