{
  byte[] n1, n2;
  n1=new byte[]{0};
  n2=new byte[]{0,0,0,0};
  assertEquals(0,comparator.compare(n1,n2));
  n1=new byte[]{1,0,0,1};
  n2=new byte[]{0,0,0,1,0,0,1};
  assertEquals(0,comparator.compare(n1,n2));
  n1=new byte[]{-1,0,0,-1};
  n2=new byte[]{-1,-1,-1,-1,0,0,-1};
  assertEquals(0,comparator.compare(n1,n2));
  n1=new byte[]{-1,0};
  n2=new byte[]{0,-1,0};
  assertSignum("",-1,comparator.compare(n1,n2));
  n1=new byte[]{1,0};
  n2=new byte[]{0,-1,0};
  assertSignum("",-1,comparator.compare(n1,n2));
}
