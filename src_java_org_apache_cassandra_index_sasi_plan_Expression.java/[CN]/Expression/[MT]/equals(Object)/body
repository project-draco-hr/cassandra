{
  if (!(other instanceof Expression))   return false;
  if (this == other)   return true;
  Expression o=(Expression)other;
  return Objects.equals(index.getColumnName(),o.index.getColumnName()) && validator.equals(o.validator) && operation == o.operation && Objects.equals(lower,o.lower) && Objects.equals(upper,o.upper) && exclusions.equals(o.exclusions);
}
