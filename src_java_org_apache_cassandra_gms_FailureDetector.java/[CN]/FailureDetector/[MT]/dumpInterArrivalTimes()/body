{
  OutputStream os=null;
  try {
    File file=File.createTempFile("failuredetector-",".dat");
    os=new BufferedOutputStream(new FileOutputStream(file,true));
    os.write(toString().getBytes());
  }
 catch (  IOException e) {
    throw new IOError(e);
  }
 finally {
    FileUtils.closeQuietly(os);
  }
}
