{
  if (tLast > 0L) {
    double interArrivalTime=(value - tLast);
    if (interArrivalTime <= MAX_INTERVAL_IN_MS)     arrivalIntervals.add(interArrivalTime);
 else     logger.debug("Ignoring interval time of {}",interArrivalTime);
  }
 else {
    arrivalIntervals.add(Gossiper.intervalInMillis * 30);
  }
  tLast=value;
}
