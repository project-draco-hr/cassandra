{
  int nowInSec=FBUtilities.nowInSeconds();
  Mutation mutation=new Mutation(NAME,Keyspaces.decorateKey(getSchemaKSKey(keyspace.name)));
  for (  CFMetaData schemaTable : ALL_TABLE_METADATA)   mutation.add(PartitionUpdate.fullPartitionDelete(schemaTable,mutation.key(),timestamp,nowInSec));
  return mutation;
}
