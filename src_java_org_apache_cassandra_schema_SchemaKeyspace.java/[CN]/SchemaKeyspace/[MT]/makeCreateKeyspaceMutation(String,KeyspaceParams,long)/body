{
  Mutation.SimpleBuilder builder=Mutation.simpleBuilder(Keyspaces.ksName,decorate(Keyspaces,name)).timestamp(timestamp);
  builder.update(Keyspaces).row().add(KeyspaceParams.Option.DURABLE_WRITES.toString(),params.durableWrites).add(KeyspaceParams.Option.REPLICATION.toString(),params.replication.asMap());
  return builder;
}
