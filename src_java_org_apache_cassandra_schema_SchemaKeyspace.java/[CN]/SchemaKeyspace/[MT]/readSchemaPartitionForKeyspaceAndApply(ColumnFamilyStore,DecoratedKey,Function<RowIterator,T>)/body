{
  int nowInSec=FBUtilities.nowInSeconds();
  try (OpOrder.Group op=store.readOrdering.start();RowIterator partition=UnfilteredRowIterators.filter(SinglePartitionReadCommand.fullPartitionRead(store.metadata,nowInSec,keyspaceKey).queryMemtableAndDisk(store,op),nowInSec)){
    return fct.apply(partition);
  }
 }
