{
  long seed=identityDistribution.next();
  sizeDistribution.setSeed(seed);
  rnd.setSeed(~seed);
  int size=(int)sizeDistribution.next();
  for (int i=0; i < size; )   for (long v=rnd.nextLong(), n=Math.min(size - i,Long.SIZE / Byte.SIZE); n-- > 0; v>>=Byte.SIZE)   chars[i++]=(char)(((v & 127) + 32) & 127);
  return new String(chars,0,size);
}
