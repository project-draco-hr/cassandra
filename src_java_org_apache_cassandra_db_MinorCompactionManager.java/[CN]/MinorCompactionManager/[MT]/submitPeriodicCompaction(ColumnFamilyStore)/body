{
  Runnable runnable=new Runnable(){
    public void run(){
      try {
        columnFamilyStore.doCompaction(COMPACTION_THRESHOLD);
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
  }
;
  compactor_.scheduleWithFixedDelay(runnable,MinorCompactionManager.intervalInMins_,MinorCompactionManager.intervalInMins_,TimeUnit.MINUTES);
}
