{
  StringBuilder sb=new StringBuilder();
  sb.append(key.name);
  for (  Name name : columns.values())   sb.append(", ").append(name.name);
  sb.append(" => ");
  if (value != null)   sb.append(value.name);
  if (!metadata.isEmpty()) {
    sb.append("{");
    for (    Name name : metadata.values())     sb.append(" ").append(name.name);
    sb.append(" }");
  }
  sb.append("]");
  return sb.toString();
}
