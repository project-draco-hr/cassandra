{
  List<ColumnDefinition> definedNames=cfm.partitionKeyColumns();
  return definedNames == null || i >= definedNames.size() || definedNames.get(i) == null ? new ColumnIdentifier(i == 0 ? DEFAULT_KEY_ALIAS : DEFAULT_KEY_ALIAS + (i + 1),false) : new ColumnIdentifier(definedNames.get(i).name,definitionType);
}
