{
  if (listFields.size() == 0)   return null;
  if (listFields.size() == 1)   return listFields.get(0);
  String mergedKey=null;
  for (  String field : listFields) {
    if (mergedKey == null) {
      mergedKey=field;
    }
 else {
      mergedKey=mergedKey + combiner + field;
    }
  }
  return mergedKey;
}
