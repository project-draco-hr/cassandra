{
  inBuf.reset(outBuf.getData(),outBuf.getLength());
  ColumnFamily columnFamily=ColumnFamily.serializer().deserialize(inBuf);
  if (curCF == null)   curCF=columnFamily.cloneMeShallow();
  curColumns.clear();
  for (  IColumn column : columnFamily.getAllColumns())   if (isColumnNeeded(column))   curColumns.add(column);
  if (isAscending)   curColumnIndex=0;
 else   curColumnIndex=curColumns.size() - 1;
}
