import exceptions
import datetime
import time
import connection
import marshal
apilevel = 1.0
threadsafety = 1
paramstyle = 'named'
Date = datetime.date
Time = datetime.time
Timestamp = datetime.datetime
Binary = buffer
STRING = DBAPITypeObject(marshal.BYTES_TYPE, marshal.ASCII_TYPE, marshal.UTF8_TYPE)
BINARY = DBAPITypeObject(marshal.BYTES_TYPE, marshal.UUID_TYPE, marshal.LEXICAL_UUID_TYPE)
NUMBER = DBAPITypeObject(marshal.LONG_TYPE, marshal.INTEGER_TYPE)
DATETIME = DBAPITypeObject(marshal.TIME_UUID_TYPE)
ROWID = DBAPITypeObject(marshal.BYTES_TYPE, marshal.ASCII_TYPE, marshal.UTF8_TYPE, marshal.INTEGER_TYPE, marshal.LONG_TYPE, marshal.UUID_TYPE, marshal.LEXICAL_UUID_TYPE, marshal.TIME_UUID_TYPE)
