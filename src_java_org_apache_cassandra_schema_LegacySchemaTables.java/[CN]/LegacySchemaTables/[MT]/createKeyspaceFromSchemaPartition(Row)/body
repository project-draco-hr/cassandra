{
  String query=String.format("SELECT * FROM %s.%s",SystemKeyspace.NAME,KEYSPACES);
  UntypedResultSet.Row row=QueryProcessor.resultify(query,partition).one();
  return new KSMetaData(row.getString("keyspace_name"),AbstractReplicationStrategy.getClass(row.getString("strategy_class")),fromJsonMap(row.getString("strategy_options")),row.getBoolean("durable_writes"));
}
