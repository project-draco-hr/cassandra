{
  if (clusterings != null) {
    return new ClusteringIndexNamesFilter(clusterings,reversed);
  }
 else {
    Slice slice=Slice.make(lowerClusteringBound == null ? Slice.Bound.BOTTOM : lowerClusteringBound,upperClusteringBound == null ? Slice.Bound.TOP : upperClusteringBound);
    return new ClusteringIndexSliceFilter(Slices.with(cfs.metadata.comparator,slice),reversed);
  }
}
