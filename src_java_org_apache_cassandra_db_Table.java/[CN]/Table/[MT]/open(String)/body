{
  Table tableInstance=instances.get(table);
  if (tableInstance == null) {
    Table.createLock.lock();
    try {
      if (tableInstance == null) {
        tableInstance=new Table(table);
        instances.put(table,tableInstance);
      }
    }
  finally {
      createLock.unlock();
    }
  }
  return tableInstance;
}
