{
  Table tableInstance=instances_.get(table);
  if (tableInstance == null) {
    Table.createLock_.lock();
    try {
      if (tableInstance == null) {
        tableInstance=new Table(table);
        instances_.put(table,tableInstance);
      }
    }
  finally {
      createLock_.unlock();
    }
  }
  return tableInstance;
}
