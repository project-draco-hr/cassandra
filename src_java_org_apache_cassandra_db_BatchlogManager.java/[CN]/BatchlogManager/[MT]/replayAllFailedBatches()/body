{
  if (!isReplaying.compareAndSet(false,true))   return;
  try {
    logger.debug("Started replayAllFailedBatches");
    for (    Row row : getRangeSlice(new NamesQueryFilter(WRITTEN_AT))) {
      if (row.cf == null || row.cf.isMarkedForDelete())       continue;
      Column writtenAt=row.cf.getColumn(WRITTEN_AT);
      if (writtenAt == null || System.currentTimeMillis() > LongType.instance.compose(writtenAt.value()) + TIMEOUT)       replayBatch(row.key);
    }
    cleanup();
  }
  finally {
    isReplaying.set(false);
  }
  logger.debug("Finished replayAllFailedBatches");
}
