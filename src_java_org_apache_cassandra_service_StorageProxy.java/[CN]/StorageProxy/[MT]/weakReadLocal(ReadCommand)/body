{
  logger.debug("weakreadlocal reading " + command);
  List<EndPoint> endpoints=StorageService.instance().getNLiveStorageEndPoint(command.key);
  endpoints.remove(StorageService.getLocalStorageEndPoint());
  Table table=Table.open(command.table);
  Row row=command.getRow(table);
  if (endpoints.size() > 0 && DatabaseDescriptor.getConsistencyCheck())   StorageService.instance().doConsistencyCheck(row,endpoints,command);
  return row;
}
