{
  if (!hasLiveColumns(expected))   return !hasLiveColumns(current);
 else   if (!hasLiveColumns(current))   return false;
  for (  Column e : expected) {
    Column c=current.getColumn(e.name());
    if (e.isLive()) {
      if (!(c != null && c.isLive() && c.value().equals(e.value())))       return false;
    }
 else {
      if (c != null && c.isLive())       return false;
    }
  }
  return true;
}
