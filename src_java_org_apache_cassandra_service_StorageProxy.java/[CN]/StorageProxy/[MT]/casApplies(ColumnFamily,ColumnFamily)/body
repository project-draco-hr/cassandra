{
  long now=System.currentTimeMillis();
  if (!hasLiveColumns(expected,now))   return !hasLiveColumns(current,now);
 else   if (!hasLiveColumns(current,now))   return false;
  for (  Cell e : expected) {
    Cell c=current.getColumn(e.name());
    if (e.isLive(now)) {
      if (!(c != null && c.isLive(now) && c.value().equals(e.value())))       return false;
    }
 else {
      if (c != null && c.isLive(now))       return false;
    }
  }
  return true;
}
