{
  if (logger.isDebugEnabled())   logger.debug("insert writing local key " + FBUtilities.bytesToHex(rm.key()) + " (keyspace: "+ rm.getTable()+ ", CFs:"+ rm.columnFamilyNames()+ ")");
  Runnable runnable=new WrappedRunnable(){
    public void runMayThrow() throws IOException {
      rm.apply();
      responseHandler.localResponse();
    }
  }
;
  StageManager.getStage(StageManager.MUTATION_STAGE).execute(runnable);
}
