{
  if (logger.isDebugEnabled())   logger.debug("insert writing local key " + rm.key());
  Runnable runnable=new Runnable(){
    public void run(){
      try {
        rm.apply();
        responseHandler.localResponse();
      }
 catch (      IOException e) {
