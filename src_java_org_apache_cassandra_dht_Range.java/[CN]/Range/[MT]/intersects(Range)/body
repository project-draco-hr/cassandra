{
  boolean thiswraps=isWrapAround(left,right);
  boolean thatwraps=isWrapAround(that.left,that.right);
  if (thiswraps && thatwraps)   return true;
 else   if (!thiswraps && !thatwraps)   return left.compareTo(that.right) < 0 && that.left.compareTo(right) < 0;
 else   return left.compareTo(that.right) < 0 || that.left.compareTo(right) < 0;
}
