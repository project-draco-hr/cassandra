{
  boolean thiswraps=isWrapAround(this.left(),this.right());
  boolean thatwraps=isWrapAround(that.left(),that.right());
  if (thiswraps == thatwraps)   return this.left().compareTo(that.left()) <= 0 && that.right().compareTo(this.right()) <= 0;
 else   if (thiswraps)   return this.left().compareTo(that.left()) <= 0 || that.right().compareTo(this.right()) <= 0;
 else   return false;
}
