{
  boolean thiswraps=isWrapAround(left,right);
  boolean thatwraps=isWrapAround(that.left,that.right);
  if (thiswraps == thatwraps)   return left.compareTo(that.left) <= 0 && that.right.compareTo(right) <= 0;
 else   if (thiswraps)   return left.compareTo(that.left) <= 0 || that.right.compareTo(right) <= 0;
 else   return false;
}
