{
  AbstractType<?> receiverType=receiver.type;
  if (getType().isFrozenCollection())   receiverType=receiverType.freeze();
  if (receiverType.equals(getType()))   return AssignmentTestable.TestResult.EXACT_MATCH;
  if (receiverType.isValueCompatibleWith(getType()))   return AssignmentTestable.TestResult.WEAKLY_ASSIGNABLE;
  return AssignmentTestable.TestResult.NOT_ASSIGNABLE;
}
