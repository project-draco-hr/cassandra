{
switch (consistencyLevel) {
case ONE:
    return 1;
case ANY:
  return 1;
case TWO:
return 2;
case THREE:
return 3;
case QUORUM:
return (writeEndpoints.size() / 2) + 1;
case ALL:
return writeEndpoints.size();
default :
throw new UnsupportedOperationException("invalid consistency level: " + consistencyLevel.toString());
}
}
