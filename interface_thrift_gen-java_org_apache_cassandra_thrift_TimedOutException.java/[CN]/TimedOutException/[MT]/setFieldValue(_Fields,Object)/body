{
switch (field) {
case ACKNOWLEDGED_BY:
    if (value == null) {
      unsetAcknowledged_by();
    }
 else {
      setAcknowledged_by((Integer)value);
    }
  break;
case ACKNOWLEDGED_BY_BATCHLOG:
if (value == null) {
  unsetAcknowledged_by_batchlog();
}
 else {
  setAcknowledged_by_batchlog((Boolean)value);
}
break;
}
}
