{
  while (nextRowPositionFromIndex < dataFile.length()) {
    try {
      dataFile.seek(nextRowPositionFromIndex);
      return;
    }
 catch (    Throwable th) {
      throwIfFatal(th);
      outputHandler.warn(String.format("Failed to seek to next row position %d",nextRowPositionFromIndex),th);
      badRows++;
    }
    updateIndexKey();
  }
}
