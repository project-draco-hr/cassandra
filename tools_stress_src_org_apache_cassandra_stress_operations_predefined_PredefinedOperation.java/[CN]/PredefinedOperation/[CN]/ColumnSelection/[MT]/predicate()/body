{
  final SlicePredicate predicate=new SlicePredicate();
  if (indices == null) {
    predicate.setSlice_range(new SliceRange().setStart(settings.columns.names.get(lb)).setFinish(new byte[]{}).setReversed(false).setCount(count()));
  }
 else   predicate.setColumn_names(select(settings.columns.names));
  return predicate;
}
