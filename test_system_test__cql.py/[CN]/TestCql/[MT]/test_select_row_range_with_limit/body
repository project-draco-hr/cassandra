def test_select_row_range_with_limit(self):
    'retrieve a limited range of rows with columns'
    cursor = init()
    cursor.execute("\n            SELECT 1,5,9 FROM StandardLongA WHERE KEY > 'aa'\n                    AND KEY < 'ag' LIMIT 3\n        ")
    assert (cursor.rowcount == 3)
    cursor.execute("\n            SELECT 20,40 FROM StandardIntegerA WHERE KEY > 'k1'\n                    AND KEY < 'k7' LIMIT 5\n        ")
    assert (cursor.rowcount == 5)
    for i in range(5):
        r = cursor.fetchone()
        assert (r[0] == ('k%d' % (i + 1)))
