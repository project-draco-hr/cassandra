def test_select_row_range_with_limit(self):
    'retrieve a limited range of rows with columns'
    conn = init()
    r = conn.execute("\n            SELECT 1,5,9 FROM StandardLongA WHERE KEY > 'aa'\n                    AND KEY < 'ag' LIMIT 3\n        ")
    assert (len(r) == 3)
    r = conn.execute("\n            SELECT 20,40 FROM StandardIntegerA WHERE KEY > 'k1'\n                    AND KEY < 'k7' LIMIT 5\n        ")
    assert (len(r) == 5)
    (r[0].key == 'k1')
    (r[4].key == 'k5')
