def test_delete_columns_multi_rows(self):
    'delete columns from multiple rows'
    cursor = init()
    cursor.execute("SELECT 'col' FROM StandardString1 WHERE KEY = 'kc'")
    row = cursor.fetchone()
    assert (['val'] == row), row
    cursor.execute("SELECT 'col' FROM StandardString1 WHERE KEY = 'kd'")
    row = cursor.fetchone()
    assert (['val'] == row), row
    cursor.execute("\n            DELETE 'col' FROM StandardString1 WHERE KEY IN ('kc', 'kd')\n        ")
    cursor.execute("SELECT 'col' FROM StandardString1 WHERE KEY = 'kc'")
    row = cursor.fetchone()
    assert ([None] == row), row
    cursor.execute("SELECT 'col' FROM StandardString1 WHERE KEY = 'kd'")
    r = cursor.fetchone()
    assert ([None] == r), r
