def test_newline_strings(self):
    'reading and writing strings w/ newlines'
    cursor = init()
    cursor.execute('\n                       UPDATE StandardString1 SET :name = :val WHERE KEY = :key;\n                       ', {'key': '\nkey', 'name': '\nname', 'val': '\nval', })
    cursor.execute('\n                       SELECT :name FROM StandardString1 WHERE KEY = :key\n                       ', {'key': '\nkey', 'name': '\nname', })
    assert (cursor.rowcount == 1)
    r = cursor.fetchone()
    assert (len(r) == 1), 'wrong number of results'
    d = cursor.description
    assert (d[0][0] == '\nname')
    assert (r[0] == '\nval')
