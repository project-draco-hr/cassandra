def test_no_where_clause(self):
    'empty where clause (range query w/o start key)'
    cursor = init()
    cursor.execute("SELECT 'col' FROM StandardString1 LIMIT 3")
    assert (cursor.rowcount == 3)
    rows = cursor.fetchmany(3)
    assert (rows[0][0] == 'ka')
    assert (rows[1][0] == 'kb')
    assert (rows[2][0] == 'kc')
