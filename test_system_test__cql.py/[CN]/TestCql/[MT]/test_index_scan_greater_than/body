def test_index_scan_greater_than(self):
    'indexed scan where a column is greater than a value'
    cursor = init()
    cursor.execute("\n            SELECT 'birthdate' FROM IndexedA WHERE 'birthdate' = 100\n                    AND 'unindexed' > 200\n        ")
    assert (cursor.rowcount == 1)
    r = cursor.fetchone()
    assert (r[0] == 'asmith')
