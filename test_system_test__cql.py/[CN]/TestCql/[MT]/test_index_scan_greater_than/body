def test_index_scan_greater_than(self):
    'indexed scan where a column is greater than a value'
    cursor = init()
    cursor.execute("\n            SELECT KEY, 'birthdate' FROM IndexedA \n            WHERE 'birthdate' = 100 AND 'unindexed' > 200\n        ")
    assert (cursor.rowcount == 1)
    row = cursor.fetchone()
    assert (row[0] == 'asmith'), row
