def test_index_scan_greater_than(self):
    'indexed scan where a column is greater than a value'
    conn = init()
    r = conn.execute('\n            SELECT "birthdate" FROM Indexed1 WHERE "birthdate" = 100L AND "unindexed" > 200L\n        ')
    assert (len(r) == 1)
    assert (r[0].key == 'asmith')
