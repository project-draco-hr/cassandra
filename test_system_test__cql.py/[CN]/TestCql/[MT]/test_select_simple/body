def test_select_simple(self):
    'retrieve a column'
    cursor = init()
    cursor.execute("SELECT 'ca1' FROM StandardString1 WHERE KEY='ka'")
    r = cursor.fetchone()
    d = cursor.description
    assert (d[0][0] == cql.ROW_KEY)
    assert (r[0] == 'ka')
    assert (d[1][0] == 'ca1')
    assert (r[1] == 'va1')
