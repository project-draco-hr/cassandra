def test_select_row_range(self):
    'retrieve a range of rows with columns'
    cursor = init()
    cursor.execute("\n            SELECT 4 FROM StandardLongA WHERE KEY > 'ad' AND KEY < 'ag';\n        ")
    keys = ['ad', 'ae', 'af']
    assert (cursor.rowcount == 4)
    for i in range(3):
        r = cursor.fetchone()
        assert (len(r) == 2)
        assert (r[0] == keys[i])
