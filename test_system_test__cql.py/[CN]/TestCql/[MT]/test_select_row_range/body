def test_select_row_range(self):
    'retrieve a range of rows with columns'
    conn = init()
    r = conn.execute("\n            SELECT 4 FROM StandardLongA WHERE KEY > 'ad' AND KEY < 'ag';\n        ")
    assert (len(r) == 3)
    assert (r[0].key == 'ad')
    assert (r[1].key == 'ae')
    assert (r[2].key == 'af')
    assert (len(r[0].columns) == 1)
    assert (len(r[1].columns) == 1)
    assert (len(r[2].columns) == 1)
