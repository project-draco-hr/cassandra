def test_select_rows(self):
    'fetch multiple rows, all columns'
    conn = init()
    r = conn.execute('\n            SELECT FROM\n                Standard1\n            WHERE\n                KEY = "ka" AND KEY = "kd" AND KEY = "kb"\n        ')
    for result in r:
        assert (result['key'] in ('ka', 'kd', 'kb'))
        assert (len(result['columns']) == 2)
