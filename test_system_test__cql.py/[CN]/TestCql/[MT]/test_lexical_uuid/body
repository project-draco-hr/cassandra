def test_lexical_uuid(self):
    'store and retrieve lexical uuids'
    conn = init()
    uid = uuid.uuid4()
    conn.execute("UPDATE StandardUUID SET ? = 10 WHERE KEY = 'uuidtest'", uid)
    r = conn.execute("SELECT ? FROM StandardUUID WHERE KEY = 'uuidtest'", uid)
    assert (r[0].columns[0].name == uid), r[0].columns[0].name
