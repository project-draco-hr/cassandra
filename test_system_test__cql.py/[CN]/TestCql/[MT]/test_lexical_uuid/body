def test_lexical_uuid(self):
    'store and retrieve lexical uuids'
    conn = init()
    uid = uuid.uuid4()
    conn.execute(('\n            UPDATE Standard2 SET uuid("%s") = 10 WHERE KEY = "uuidtest"\n        ' % str(uid)))
    r = conn.execute(('\n            SELECT uuid("%s") FROM Standard2 WHERE KEY = "uuidtest"\n        ' % str(uid)))
    assert (r[0].columns[0].name == uid.bytes)
