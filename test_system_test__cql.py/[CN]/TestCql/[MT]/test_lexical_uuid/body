def test_lexical_uuid(self):
    'store and retrieve lexical uuids'
    cursor = init()
    uid = uuid.uuid4()
    cursor.execute("UPDATE StandardUUID SET :name = 10 WHERE KEY = 'uuidtest'", dict(name=uid))
    cursor.execute("SELECT :name FROM StandardUUID WHERE KEY = 'uuidtest'", dict(name=uid))
    d = cursor.description
    assert (d[0][0] == uid), ('expected %s, got %s (%s)' % (uid.bytes.encode('hex'), str(d[1][0]).encode('hex'), d[1][1]))
