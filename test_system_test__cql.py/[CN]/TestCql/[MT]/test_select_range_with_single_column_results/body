def test_select_range_with_single_column_results(self):
    'range should not fail when keys were not set'
    cursor = init()
    cursor.execute("\n          BEGIN BATCH\n            UPDATE StandardString2 SET name='1',password='pass1' WHERE KEY = 'user1'\n            UPDATE StandardString2 SET name='2',password='pass2' WHERE KEY = 'user2'\n            UPDATE StandardString2 SET password='pass3' WHERE KEY = 'user3'\n          APPLY BATCH\n        ")
    cursor.execute('\n          SELECT KEY, name FROM StandardString2\n        ')
    assert (cursor.rowcount == 3), ('expected 3 results, got %d' % cursor.rowcount)
    for i in range(1, 3):
        r = cursor.fetchone()
        assert (len(r) == 2)
        assert (r[0] == ('user%d' % i))
        assert (r[1] == ('%s' % i))
    r = cursor.fetchone()
    assert (len(r) == 2)
    assert (r[0] == 'user3')
    assert (r[1] == None)
