def test_index_scan_with_start_key(self):
    'indexed scan with a starting key'
    cursor = init()
    cursor.execute("\n            SELECT 'birthdate' FROM IndexedA WHERE 'birthdate' = 100\n                    AND KEY >= 'asmithZ'\n        ")
    assert (cursor.rowcount == 1)
    r = cursor.fetchone()
    assert (r[0] == 'dozer')
