def test_select_columns_slice_with_limit(self):
    'range of columns (slice) by row with limit'
    cursor = init()
    cursor.execute("\n            SELECT FIRST 1 1..3 FROM StandardLongA WHERE KEY = 'aa';\n        ")
    assert (cursor.rowcount == 1)
    r = cursor.fetchone()
    assert (len(r) == 2)
    assert (r[0] == 'aa')
    assert (r[1] == '1')
