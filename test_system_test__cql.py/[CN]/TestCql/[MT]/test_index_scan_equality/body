def test_index_scan_equality(self):
    'indexed scan where column equals value'
    cursor = init()
    cursor.execute('\n            SELECT KEY, birthdate FROM IndexedA WHERE birthdate = 100\n        ')
    assert (cursor.rowcount == 2)
    r = cursor.fetchone()
    assert (r[0] == 'asmith')
    assert (len(r) == 2)
    r = cursor.fetchone()
    assert (r[0] == 'dozer')
    assert (len(r) == 2)
