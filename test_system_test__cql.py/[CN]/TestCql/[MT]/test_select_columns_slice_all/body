def test_select_columns_slice_all(self):
    'slice all columns in a row'
    conn = init()
    r = conn.execute("SELECT * FROM StandardString1 WHERE KEY = 'ka';")
    assert (len(r[0].columns) == 2)
    r = conn.execute("SELECT ''..'' FROM StandardString1 WHERE KEY = 'ka';")
    assert (len(r[0].columns) == 2)
