def load_sample(dbconn):
    dbconn.execute('\n        UPDATE\n            Standard1\n        WITH\n            ROW("ka", COL("ca1", "va1"), COL("col", "val")) AND\n            ROW("kb", COL("cb1", "vb1"), COL("col", "val")) AND\n            ROW("kc", COL("cc1", "vc1"), COL("col", "val")) AND\n            ROW("kd", COL("cd1", "vd1"), COL("col", "val"));\n    ')
