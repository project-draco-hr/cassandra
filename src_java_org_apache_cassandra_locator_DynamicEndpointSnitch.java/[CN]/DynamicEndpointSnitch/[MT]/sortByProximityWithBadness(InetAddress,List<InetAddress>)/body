{
  if (addresses.size() < 2)   return addresses;
  List<InetAddress> snitchordered=subsnitch.sortByProximity(address,addresses);
  Double first=scores.get(snitchordered.get(0));
  if (first == null)   return snitchordered;
  for (  InetAddress addr : addresses) {
    Double next=scores.get(addr);
    if (next == null)     return snitchordered;
    if ((first - next) / first > BADNESS_THRESHOLD)     return sortByProximityWithScore(address,addresses);
  }
  return snitchordered;
}
