{
  if (list == null)   return;
  Set<ByteBuffer> toDiscard=new HashSet<ByteBuffer>();
  for (  Term value : values)   toDiscard.add(value.getByteBuffer(validator.valueComparator(),params.variables));
  for (  Pair<ByteBuffer,IColumn> p : list) {
    IColumn c=p.right;
    if (toDiscard.contains(c.value()))     cf.addColumn(params.makeTombstone(c.name()));
  }
}
