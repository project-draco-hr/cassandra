{
  assertFiles(txnLogs.getDataFolder(),Sets.newHashSet(Iterables.concat(sstableNew.getAllFilePaths(),sstableOld.getAllFilePaths())));
  assertFiles(txnLogs.getLogsFolder(),Sets.newHashSet(txnLogs.getData().oldLog().file.getPath(),txnLogs.getData().newLog().file.getPath()));
  assertEquals(2,TransactionLogs.getLogFiles(cfs.metadata).size());
}
