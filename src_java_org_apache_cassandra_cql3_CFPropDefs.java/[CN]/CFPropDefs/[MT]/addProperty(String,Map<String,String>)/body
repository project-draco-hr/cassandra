{
  if (name.equalsIgnoreCase(COMPACTION_PARAMETERS)) {
    for (    Map.Entry<String,String> entry : value.entrySet())     compactionStrategyOptions.put(entry.getKey().toLowerCase(),entry.getValue());
  }
 else   if (name.equalsIgnoreCase(COMPRESSION_PARAMETERS)) {
    for (    Map.Entry<String,String> entry : value.entrySet())     compressionParameters.put(entry.getKey().toLowerCase(),entry.getValue());
  }
}
