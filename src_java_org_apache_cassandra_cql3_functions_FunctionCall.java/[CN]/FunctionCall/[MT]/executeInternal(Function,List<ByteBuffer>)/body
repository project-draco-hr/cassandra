{
  ByteBuffer result=fun.execute(params);
  try {
    if (result != null)     fun.returnType().validate(result);
    return result;
  }
 catch (  MarshalException e) {
    throw new RuntimeException(String.format("Return of function %s (%s) is not a valid value for its declared return type %s",fun,ByteBufferUtil.bytesToHex(result),fun.returnType().asCQL3Type()));
  }
}
