{
  List<ByteBuffer> buffers=new ArrayList<ByteBuffer>(terms.size());
  for (  Term t : terms) {
    ByteBuffer val=t.bindAndGet(options);
    if (val == null)     throw new InvalidRequestException(String.format("Invalid null value for argument to %s",fun));
    buffers.add(val);
  }
  return fun.execute(buffers);
}
