{
  int count=0;
  Map<String,IColumn> columns=columns_.getColumns();
  if (columns != null) {
    if (!DatabaseDescriptor.getColumnType(name_).equals("Super")) {
      count=columns.size();
    }
 else {
      Collection<IColumn> values=columns.values();
      for (      IColumn column : values) {
        count+=column.getObjectCount();
      }
    }
  }
  return count;
}
