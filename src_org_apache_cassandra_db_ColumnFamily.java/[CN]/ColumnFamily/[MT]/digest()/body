{
  Set<IColumn> columns=columns_.getSortedColumns();
  byte[] xorHash=new byte[0];
  byte[] tmpHash=new byte[0];
  for (  IColumn column : columns) {
    if (xorHash.length == 0) {
      xorHash=column.digest();
    }
 else {
      tmpHash=column.digest();
      xorHash=FBUtilities.xor(xorHash,tmpHash);
    }
  }
  return xorHash;
}
