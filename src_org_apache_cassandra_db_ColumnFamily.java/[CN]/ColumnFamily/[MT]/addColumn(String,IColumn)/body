{
  int newSize=0;
  IColumn oldColumn=columns_.get(name);
  if (oldColumn != null) {
    int oldSize=oldColumn.size();
    if (oldColumn.putColumn(column)) {
      columns_.put(name,column);
      newSize=column.size();
    }
 else {
      newSize=oldColumn.size();
    }
    size_.addAndGet(newSize - oldSize);
  }
 else {
    newSize=column.size();
    size_.addAndGet(newSize);
    columns_.put(name,column);
  }
}
