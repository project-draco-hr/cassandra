{
  StringBuilder sb=new StringBuilder();
  sb.append(name_);
  sb.append(":");
  sb.append(isMarkedForDelete());
  sb.append(":");
  Collection<IColumn> columns=getAllColumns();
  sb.append(columns.size());
  sb.append(":");
  for (  IColumn column : columns) {
    sb.append(column.toString());
  }
  sb.append(":");
  return sb.toString();
}
