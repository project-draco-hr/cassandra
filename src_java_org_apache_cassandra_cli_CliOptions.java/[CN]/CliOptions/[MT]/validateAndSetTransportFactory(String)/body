{
  try {
    Class<?> factory=Class.forName(transportFactory);
    if (!TTransportFactory.class.isAssignableFrom(factory))     throw new IllegalArgumentException(String.format("transport factory '%s' " + "not derived from TTransportFactory",transportFactory));
    return (TTransportFactory)factory.newInstance();
  }
 catch (  Exception e) {
    throw new IllegalArgumentException(String.format("Cannot create a transport factory '%s'.",transportFactory),e);
  }
}
