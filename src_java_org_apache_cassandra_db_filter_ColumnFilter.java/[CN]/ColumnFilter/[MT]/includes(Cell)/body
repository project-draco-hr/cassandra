{
  if (isFetchAll || subSelections == null || !cell.column().isComplex())   return true;
  SortedSet<ColumnSubselection> s=subSelections.get(cell.column().name);
  if (s.isEmpty())   return true;
  for (  ColumnSubselection subSel : s)   if (subSel.compareInclusionOf(cell.path()) == 0)   return true;
  return false;
}
