{
  if (subSelections == null) {
    sb.append(column.name);
    return;
  }
  SortedSet<ColumnSubselection> s=subSelections.get(column.name);
  if (s.isEmpty()) {
    sb.append(column.name);
    return;
  }
  int i=0;
  for (  ColumnSubselection subSel : s)   sb.append(i++ == 0 ? "" : ", ").append(column.name).append(subSel);
}
