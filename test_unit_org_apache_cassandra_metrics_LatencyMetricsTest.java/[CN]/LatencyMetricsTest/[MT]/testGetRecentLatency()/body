{
  final LatencyMetrics l=new LatencyMetrics("test","test");
  Runnable r=new Runnable(){
    public void run(){
      for (int i=0; i < 10000; i++) {
        l.addMicro(1);
      }
    }
  }
;
  new Thread(r).start();
  for (int i=0; i < 10000; i++) {
    Double recent=l.getRecentLatency();
    assertFalse(recent.equals(Double.POSITIVE_INFINITY));
  }
}
