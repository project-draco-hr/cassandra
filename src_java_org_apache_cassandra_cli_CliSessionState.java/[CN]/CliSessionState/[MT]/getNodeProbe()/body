{
  try {
    return jmxUsername != null && jmxPassword != null ? new NodeProbe(hostName,jmxPort,jmxUsername,jmxPassword) : new NodeProbe(hostName,jmxPort);
  }
 catch (  Exception e) {
    JVMStabilityInspector.inspectThrowable(e);
    err.printf("WARNING: Could not connect to the JMX on %s:%d - some information won't be shown.%n%n",hostName,jmxPort);
  }
  return null;
}
