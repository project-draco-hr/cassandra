{
  try {
    ClientState state=new ClientState();
    ResultMessage result=processStatement(getStatement(query,state).statement,state,Collections.<ByteBuffer>emptyList());
    if (result instanceof ResultMessage.Rows)     return new UntypedResultSet(((ResultMessage.Rows)result).result);
 else     return null;
  }
 catch (  UnavailableException e) {
    throw new RuntimeException(e);
  }
catch (  InvalidRequestException e) {
    throw new AssertionError(e);
  }
catch (  TimedOutException e) {
    throw new RuntimeException(e);
  }
catch (  SchemaDisagreementException e) {
    throw new RuntimeException(e);
  }
}
