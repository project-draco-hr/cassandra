{
  super.endOfRow();
  if (staticRow == null) {
    staticRow=build();
  }
 else {
    StaticRow.Builder builder=StaticRow.builder(columns.statics,true,metadata().isCounter());
    Rows.merge(staticRow,build(),columns.statics,builder,FBUtilities.nowInSeconds());
    staticRow=builder.build();
  }
}
