{
  if (column.isComplex()) {
    if (updatedComplex.contains(column) && (!column.equals(lastUpdatedComplex) || (column.cellPathComparator().compare(path,lastUpdatedComplexPath)) <= 0)) {
      endOfRow();
      int clusteringSize=metadata.clusteringColumns().size();
      int base=(row - 1) * clusteringSize;
      for (int i=0; i < clusteringSize; i++)       writer.writeClusteringValue(clusterings[base + i]);
      updatedComplex.clear();
    }
    lastUpdatedComplex=column;
    lastUpdatedComplexPath=path;
    updatedComplex.add(column);
  }
  super.writeCell(column,isCounter,value,info,path);
}
