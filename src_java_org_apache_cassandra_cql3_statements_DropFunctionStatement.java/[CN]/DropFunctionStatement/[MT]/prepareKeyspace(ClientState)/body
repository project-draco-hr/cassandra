{
  if (!functionName.hasKeyspace() && state.getRawKeyspace() != null)   functionName=new FunctionName(state.getKeyspace(),functionName.name);
  if (!functionName.hasKeyspace())   throw new InvalidRequestException("You need to be logged in a keyspace or use a fully qualified function name");
  ThriftValidation.validateKeyspaceNotSystem(functionName.keyspace);
}
