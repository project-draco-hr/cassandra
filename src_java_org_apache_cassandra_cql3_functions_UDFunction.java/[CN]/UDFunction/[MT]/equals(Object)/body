{
  if (!(o instanceof UDFunction))   return false;
  UDFunction that=(UDFunction)o;
  return Objects.equal(name,that.name) && Objects.equal(argNames,that.argNames) && Functions.typesMatch(argTypes,that.argTypes)&& Functions.typesMatch(returnType,that.returnType)&& Objects.equal(language,that.language)&& Objects.equal(body,that.body);
}
