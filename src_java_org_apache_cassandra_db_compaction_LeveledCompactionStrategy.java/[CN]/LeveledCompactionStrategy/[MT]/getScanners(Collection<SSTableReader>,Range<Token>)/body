{
  Multimap<Integer,SSTableReader> byLevel=ArrayListMultimap.create();
  for (  SSTableReader sstable : sstables)   byLevel.get(manifest.levelOf(sstable)).add(sstable);
  List<ICompactionScanner> scanners=new ArrayList<ICompactionScanner>(sstables.size());
  for (  Integer level : byLevel.keySet()) {
    if (level <= 0) {
      for (      SSTableReader sstable : byLevel.get(level))       scanners.add(sstable.getDirectScanner(range));
    }
 else {
      scanners.add(new LeveledScanner(byLevel.get(level),range));
    }
  }
  return scanners;
}
