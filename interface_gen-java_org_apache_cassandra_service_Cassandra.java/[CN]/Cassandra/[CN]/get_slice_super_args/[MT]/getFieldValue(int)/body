{
switch (fieldID) {
case TABLENAME:
    return getTablename();
case KEY:
  return getKey();
case COLUMNFAMILY:
return getColumnFamily();
case START:
return getStart();
case FINISH:
return getFinish();
case ISASCENDING:
return new Boolean(isIsAscending());
case OFFSET:
return new Integer(getOffset());
case COUNT:
return new Integer(getCount());
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
