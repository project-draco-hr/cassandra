{
switch (fieldID) {
case BATCHMUTATION:
    if (value == null) {
      unsetBatchMutation();
    }
 else {
      setBatchMutation((batch_mutation_t)value);
    }
  break;
case BLOCK:
if (value == null) {
  unsetBlock();
}
 else {
  setBlock((Boolean)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
