{
switch (fieldID) {
case TABLENAME:
    return getTablename();
case KEY:
  return getKey();
case COLUMNPARENT:
return getColumnParent();
case START:
return getStart();
case END:
return getEnd();
case COUNT:
return new Integer(getCount());
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
