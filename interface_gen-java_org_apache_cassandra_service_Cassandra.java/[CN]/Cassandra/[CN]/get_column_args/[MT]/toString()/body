{
  StringBuilder sb=new StringBuilder("get_column_args(");
  boolean first=true;
  sb.append("table:");
  if (this.table == null) {
    sb.append("null");
  }
 else {
    sb.append(this.table);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("key:");
  if (this.key == null) {
    sb.append("null");
  }
 else {
    sb.append(this.key);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("column_path:");
  if (this.column_path == null) {
    sb.append("null");
  }
 else {
    sb.append(this.column_path);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
