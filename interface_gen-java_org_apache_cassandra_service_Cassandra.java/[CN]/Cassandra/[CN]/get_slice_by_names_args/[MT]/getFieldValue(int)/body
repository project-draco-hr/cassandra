{
switch (fieldID) {
case TABLE:
    return getTable();
case KEY:
  return getKey();
case COLUMN_PARENT:
return getColumn_parent();
case COLUMN_NAMES:
return getColumn_names();
case CONSISTENCY_LEVEL:
return getConsistency_level();
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
