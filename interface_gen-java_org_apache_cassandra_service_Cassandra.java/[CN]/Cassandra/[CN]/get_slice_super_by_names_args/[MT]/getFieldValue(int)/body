{
switch (fieldID) {
case TABLE:
    return getTable();
case KEY:
  return getKey();
case COLUMN_FAMILY:
return getColumn_family();
case SUPER_COLUMN_NAMES:
return getSuper_column_names();
case CONSISTENCY_LEVEL:
return getConsistency_level();
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
