{
switch (fieldID) {
case TABLE:
    return isSetTable();
case KEY:
  return isSetKey();
case COLUMN_FAMILY:
return isSetColumn_family();
case SUPER_COLUMN_NAMES:
return isSetSuper_column_names();
case CONSISTENCY_LEVEL:
return isSetConsistency_level();
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
