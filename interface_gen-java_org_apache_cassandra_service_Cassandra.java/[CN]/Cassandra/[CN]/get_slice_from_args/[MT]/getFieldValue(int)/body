{
switch (fieldID) {
case TABLENAME:
    return getTablename();
case KEY:
  return getKey();
case COLUMNFAMILY_COLUMN:
return getColumnFamily_column();
case ISASCENDING:
return new Boolean(isIsAscending());
case COUNT:
return new Integer(getCount());
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
