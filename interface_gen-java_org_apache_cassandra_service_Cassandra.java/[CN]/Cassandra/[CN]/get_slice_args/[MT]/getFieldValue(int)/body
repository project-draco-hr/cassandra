{
switch (fieldID) {
case TABLE:
    return getTable();
case KEY:
  return getKey();
case COLUMN_PARENT:
return getColumn_parent();
case START:
return getStart();
case FINISH:
return getFinish();
case IS_ASCENDING:
return new Boolean(isIs_ascending());
case OFFSET:
return new Integer(getOffset());
case COUNT:
return new Integer(getCount());
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
