{
switch (fieldID) {
case TABLE:
    return getTable();
case KEY:
  return getKey();
case COLUMN_PATH_OR_PARENT:
return getColumn_path_or_parent();
case TIMESTAMP:
return new Long(getTimestamp());
case CONSISTENCY_LEVEL:
return getConsistency_level();
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
