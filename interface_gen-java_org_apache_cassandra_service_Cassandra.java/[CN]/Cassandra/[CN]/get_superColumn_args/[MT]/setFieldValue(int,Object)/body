{
switch (fieldID) {
case TABLENAME:
    if (value == null) {
      unsetTablename();
    }
 else {
      setTablename((String)value);
    }
  break;
case KEY:
if (value == null) {
  unsetKey();
}
 else {
  setKey((String)value);
}
break;
case SUPERCOLUMNPATH:
if (value == null) {
unsetSuperColumnPath();
}
 else {
setSuperColumnPath((String)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
