{
switch (fieldID) {
case TABLE:
    if (value == null) {
      unsetTable();
    }
 else {
      setTable((String)value);
    }
  break;
case KEY:
if (value == null) {
  unsetKey();
}
 else {
  setKey((String)value);
}
break;
case COLUMN_PATH:
if (value == null) {
unsetColumn_path();
}
 else {
setColumn_path((ColumnPath)value);
}
break;
case VALUE:
if (value == null) {
unsetValue();
}
 else {
setValue((byte[])value);
}
break;
case TIMESTAMP:
if (value == null) {
unsetTimestamp();
}
 else {
setTimestamp((Long)value);
}
break;
case CONSISTENCY_LEVEL:
if (value == null) {
unsetConsistency_level();
}
 else {
setConsistency_level((Integer)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
