{
switch (fieldID) {
case TABLE:
    return getTable();
case KEY:
  return getKey();
case COLUMN_PATH:
return getColumn_path();
case VALUE:
return getValue();
case TIMESTAMP:
return new Long(getTimestamp());
case BLOCK_FOR:
return new Integer(getBlock_for());
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
