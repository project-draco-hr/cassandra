{
switch (fieldID) {
case TABLE:
    return isSetTable();
case KEY:
  return isSetKey();
case COLUMN_PATH:
return isSetColumn_path();
case VALUE:
return isSetValue();
case TIMESTAMP:
return isSetTimestamp();
case CONSISTENCY_LEVEL:
return isSetConsistency_level();
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
