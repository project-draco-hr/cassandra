{
switch (fieldID) {
case TABLE:
    return getTable();
case COLUMN_FAMILY:
  return getColumn_family();
case STARTWITH:
return getStartWith();
case STOPAT:
return getStopAt();
case MAXRESULTS:
return new Integer(getMaxResults());
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
