{
switch (fieldID) {
case TABLENAME:
    return getTablename();
case COLUMNFAMILY:
  return getColumnFamily();
case STARTWITH:
return getStartWith();
case STOPAT:
return getStopAt();
case MAXRESULTS:
return new Integer(getMaxResults());
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
