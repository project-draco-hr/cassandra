{
  RowMutation rm=new RowMutation("Table1",TEST_KEY);
  ColumnFamily cf=ColumnFamily.create("Table1","Standard1");
  cf.addColumn(new Column("col1","val1".getBytes(),1L));
  cf.addColumn(new Column("col2","val2".getBytes(),1L));
