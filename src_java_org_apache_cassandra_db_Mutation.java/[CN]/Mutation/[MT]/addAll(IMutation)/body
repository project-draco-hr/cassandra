{
  if (!(m instanceof Mutation))   throw new IllegalArgumentException();
  Mutation mutation=(Mutation)m;
  if (!keyspaceName.equals(mutation.keyspaceName) || !key.equals(mutation.key))   throw new IllegalArgumentException();
  for (  Map.Entry<UUID,ColumnFamily> entry : mutation.modifications.entrySet()) {
    ColumnFamily cf=modifications.put(entry.getKey(),entry.getValue());
    if (cf != null)     entry.getValue().resolve(cf);
  }
}
