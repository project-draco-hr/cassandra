{
  queue=new ConcurrentLinkedQueue<CheaterFutureTask>();
  Runnable runnable=new Runnable(){
    public void run(){
      if (DatabaseDescriptor.isCommitLogSyncEnabled()) {
        while (true) {
          processWithSyncDelay();
        }
      }
 else {
        while (true) {
          process();
        }
      }
    }
  }
;
  new Thread(runnable).start();
}
