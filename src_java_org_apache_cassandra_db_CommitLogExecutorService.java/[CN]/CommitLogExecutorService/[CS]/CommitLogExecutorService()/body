{
  queue=new ArrayBlockingQueue<CheaterFutureTask>(10000);
  Runnable runnable=new Runnable(){
    public void run(){
      try {
        if (DatabaseDescriptor.isCommitLogSyncEnabled()) {
          while (true) {
            processWithSyncDelay();
          }
        }
 else {
          while (true) {
            process();
          }
        }
      }
 catch (      Exception e) {
        throw new RuntimeException(e);
      }
    }
  }
;
  new Thread(runnable).start();
}
