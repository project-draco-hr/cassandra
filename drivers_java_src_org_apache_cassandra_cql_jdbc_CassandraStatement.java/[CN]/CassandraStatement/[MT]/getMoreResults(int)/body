{
  checkNotClosed();
switch (current) {
case CLOSE_CURRENT_RESULT:
    resetResults();
  break;
case CLOSE_ALL_RESULTS:
case KEEP_CURRENT_RESULT:
throw new SQLFeatureNotSupportedException(NO_MULTIPLE);
default :
throw new SQLSyntaxErrorException(String.format(BAD_KEEP_RSET,current));
}
return false;
}
