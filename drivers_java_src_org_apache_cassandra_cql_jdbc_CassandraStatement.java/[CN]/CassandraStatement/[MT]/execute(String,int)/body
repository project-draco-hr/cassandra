{
  checkNotClosed();
  if (!(autoGeneratedKeys == RETURN_GENERATED_KEYS || autoGeneratedKeys == NO_GENERATED_KEYS))   throw new SQLSyntaxErrorException(BAD_AUTO_GEN);
  if (autoGeneratedKeys == RETURN_GENERATED_KEYS)   throw new SQLFeatureNotSupportedException(NO_GEN_KEYS);
  return execute(sql);
}
