{
  if (columns.size() <= MAX_ELEMENTS)   return columns;
  logger.error("Detected collection with {} elements, more than the {} limit. Only the first {} elements will be returned to the client. " + "Please see http://cassandra.apache.org/doc/cql3/CQL.html#collections for more details.",columns.size(),MAX_ELEMENTS,MAX_ELEMENTS);
  return columns.subList(0,MAX_ELEMENTS);
}
