{
  fetchKeys();
  String selectColumnList=makeColumnList(getSelectColumns());
  String partitionKeyList=makeColumnList(partitionKeys);
  return String.format("SELECT %s FROM %s.%s WHERE token(%s)>? AND token(%s)<=?" + getAdditionalWhereClauses(),selectColumnList,keyspace,cfName,partitionKeyList,partitionKeyList);
}
