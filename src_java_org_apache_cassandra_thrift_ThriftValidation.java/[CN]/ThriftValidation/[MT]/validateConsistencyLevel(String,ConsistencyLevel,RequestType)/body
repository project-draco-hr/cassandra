{
switch (cl) {
case ANY:
    if (requestType == RequestType.READ)     throw new InvalidRequestException("ANY ConsistencyLevel is only supported for writes");
  break;
case LOCAL_QUORUM:
requireNetworkTopologyStrategy(table,cl);
break;
case EACH_QUORUM:
requireNetworkTopologyStrategy(table,cl);
if (requestType == RequestType.READ) throw new InvalidRequestException("EACH_QUORUM ConsistencyLevel is only supported for writes");
break;
}
}
