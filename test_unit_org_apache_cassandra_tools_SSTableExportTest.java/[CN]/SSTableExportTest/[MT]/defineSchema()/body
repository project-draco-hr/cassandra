{
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE1,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD),CFMetaData.denseCFMetaData(KEYSPACE1,CF_COUNTER,BytesType.instance).defaultValidator(CounterColumnType.instance),SchemaLoader.standardCFMD(KEYSPACE1,CF_UUID).keyValidator(UUIDType.instance),CFMetaData.denseCFMetaData(KEYSPACE1,CF_VALSWITHQUOTES,BytesType.instance).defaultValidator(UTF8Type.instance));
}
