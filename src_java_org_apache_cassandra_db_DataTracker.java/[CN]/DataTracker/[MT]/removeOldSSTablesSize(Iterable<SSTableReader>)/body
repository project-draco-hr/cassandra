{
  for (  SSTableReader sstable : oldSSTables) {
    if (logger.isDebugEnabled())     logger.debug(String.format("removing %s from list of files tracked for %s.%s",sstable.descriptor,cfstore.keyspace.getName(),cfstore.name));
    long size=sstable.bytesOnDisk();
    StorageMetrics.load.dec(size);
    cfstore.metric.liveDiskSpaceUsed.dec(size);
  }
}
