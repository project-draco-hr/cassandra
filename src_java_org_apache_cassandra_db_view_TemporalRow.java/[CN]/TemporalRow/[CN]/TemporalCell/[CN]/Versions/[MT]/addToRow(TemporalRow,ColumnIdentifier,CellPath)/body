{
  if (existingCell != null)   row.addColumnValue(column,path,existingCell.timestamp,existingCell.ttl,existingCell.localDeletionTime,existingCell.value,existingCell.isNew);
  if (newCell != null)   row.addColumnValue(column,path,newCell.timestamp,newCell.ttl,newCell.localDeletionTime,newCell.value,newCell.isNew);
}
