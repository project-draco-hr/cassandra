{
  CFMetaData metadata=baseCfs.metadata;
  CBuilder builder=CBuilder.create(metadata.comparator);
  ByteBuffer[] buffers=new ByteBuffer[clusteringColumns.size()];
  for (  Map.Entry<ColumnIdentifier,ByteBuffer> buffer : clusteringColumns.entrySet())   buffers[metadata.getColumnDefinition(buffer.getKey()).position()]=buffer.getValue();
  for (  ByteBuffer byteBuffer : buffers)   builder=builder.add(byteBuffer);
  return builder;
}
