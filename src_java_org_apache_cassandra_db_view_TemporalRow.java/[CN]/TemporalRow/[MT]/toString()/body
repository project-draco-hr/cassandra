{
  return MoreObjects.toStringHelper(this).add("table",baseCfs.keyspace.getName() + "." + baseCfs.getTableName()).add("basePartitionKey",ByteBufferUtil.bytesToHex(basePartitionKey)).add("startRow",startRow.toString(baseCfs.metadata)).add("startIsNew",startIsNew).add("nowInSec",nowInSec).add("viewClusteringTtl",viewClusteringTtl).add("viewClusteringTimestamp",viewClusteringTimestamp).add("viewClusteringLocalDeletionTime",viewClusteringLocalDeletionTime).add("columnValues",columnValues).toString();
}
