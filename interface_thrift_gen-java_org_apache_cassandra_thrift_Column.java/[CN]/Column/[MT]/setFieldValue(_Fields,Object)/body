{
switch (field) {
case NAME:
    if (value == null) {
      unsetName();
    }
 else {
      setName((byte[])value);
    }
  break;
case VALUE:
if (value == null) {
  unsetValue();
}
 else {
  setValue((byte[])value);
}
break;
case CLOCK:
if (value == null) {
unsetClock();
}
 else {
setClock((Clock)value);
}
break;
case TTL:
if (value == null) {
unsetTtl();
}
 else {
setTtl((Integer)value);
}
break;
}
}
