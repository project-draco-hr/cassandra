{
switch (field) {
case NAME:
    if (value == null) {
      unsetName();
    }
 else {
      setName((ByteBuffer)value);
    }
  break;
case VALUE:
if (value == null) {
  unsetValue();
}
 else {
  setValue((ByteBuffer)value);
}
break;
case TIMESTAMP:
if (value == null) {
unsetTimestamp();
}
 else {
setTimestamp((Long)value);
}
break;
case TTL:
if (value == null) {
unsetTtl();
}
 else {
setTtl((Integer)value);
}
break;
}
}
