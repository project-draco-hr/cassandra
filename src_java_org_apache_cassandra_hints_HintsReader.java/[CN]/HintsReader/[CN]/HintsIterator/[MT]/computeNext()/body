{
  Hint hint;
  do {
    long position=reader.getFilePointer();
    if (reader.length() == position)     return endOfData();
    if (position - offset >= PAGE_SIZE)     return endOfData();
    try {
      hint=computeNextInternal();
    }
 catch (    IOException e) {
      throw new FSReadError(e,file);
    }
  }
 while (hint == null);
  return hint;
}
