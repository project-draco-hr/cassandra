{
  ByteBuffer buffer;
  do {
    long position=input.getFilePointer();
    if (input.length() == position)     return endOfData();
    if (position - offset >= PAGE_SIZE)     return endOfData();
    try {
      buffer=computeNextInternal();
    }
 catch (    IOException e) {
      throw new FSReadError(e,file);
    }
  }
 while (buffer == null);
  return buffer;
}
