{
  for (  SSTableReader sstable : sstables)   sstable.readMeter=new RestorableMeter(100.0,100.0);
  sstables=redistributeSummaries(Collections.EMPTY_LIST,sstables,originalOffHeapSize * sstables.size());
  for (  SSTableReader sstable : sstables)   assertEquals(BASE_SAMPLING_LEVEL,sstable.getIndexSummarySamplingLevel());
  return sstables;
}
