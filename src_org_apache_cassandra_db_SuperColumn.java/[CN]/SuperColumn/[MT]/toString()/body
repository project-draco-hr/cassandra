{
  StringBuilder sb=new StringBuilder();
  sb.append(name_);
  sb.append(":");
  sb.append(isMarkedForDelete());
  sb.append(":");
  Collection<IColumn> columns=getSubColumns();
  sb.append(columns.size());
  sb.append(":");
  sb.append(size());
  sb.append(":");
  for (  IColumn subColumn : columns) {
    sb.append(subColumn.toString());
  }
  sb.append(":");
  return sb.toString();
}
