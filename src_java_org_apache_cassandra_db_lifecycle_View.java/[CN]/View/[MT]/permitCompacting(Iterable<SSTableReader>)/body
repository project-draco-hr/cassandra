{
  return new Predicate<View>(){
    public boolean apply(    View view){
      for (      SSTableReader reader : readers)       if (view.compacting.contains(reader) || view.sstablesMap.get(reader) != reader || reader.isMarkedCompacted() || view.premature.contains(reader))       return false;
      return true;
    }
  }
;
}
