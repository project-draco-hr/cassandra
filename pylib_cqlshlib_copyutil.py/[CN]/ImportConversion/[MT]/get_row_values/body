def get_row_values(self, row):
    '\n        Parse the row into a list of row values to be returned\n        '
    ret = ([None] * len(row))
    for (i, val) in enumerate(row):
        if (val != self.nullval):
            ret[i] = self.converters[self.columns[i]](val)
        else:
            if (i in self.primary_key_indexes):
                message = ("Cannot insert null value for primary key column '%s'." % (self.columns[i],))
                if (self.nullval == ''):
                    message += ' If you want to insert empty strings, consider using the WITH NULL=<marker> option for COPY.'
                raise Exception(message=message)
            ret[i] = None
    return ret
