def run(self):
    try:
        try:
            pr = (profile_on() if PROFILE_ON else None)
            self.on_fork()
            self.inner_run(*self.make_params())
            if pr:
                profile_off(pr, file_name=('worker_profile_%d.txt' % (os.getpid(),)))
        except Exception as exc:
            if self.debug:
                traceback.print_exc(exc)
    finally:
        self.close()
