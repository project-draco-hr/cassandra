def make_params(self):
    '\n        Return a dictionary of parameters to be used by the worker processes.\n        On Windows this dictionary must be pickle-able.\n        '
    shell = self.shell
    return dict(ks=self.ks, table=self.table, local_dc=self.local_dc, columns=self.columns, options=self.options, connect_timeout=shell.conn.connect_timeout, hostname=shell.hostname, port=shell.port, ssl=shell.ssl, auth_provider=shell.auth_provider, cql_version=shell.conn.cql_version, config_file=self.config_file, protocol_version=self.protocol_version, debug=shell.debug)
