{
  for (  IMutation m : statement.getMutations(variables,local,cl,getTimestamp(now),true)) {
    Pair<String,ByteBuffer> key=Pair.create(m.getTable(),m.key());
    IMutation existing=mutations.get(key);
    if (existing == null) {
      mutations.put(key,m);
    }
 else {
      existing.addAll(m);
    }
  }
}
