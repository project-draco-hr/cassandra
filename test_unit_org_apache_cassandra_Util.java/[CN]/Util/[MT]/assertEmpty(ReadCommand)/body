{
  try (ReadExecutionController executionController=command.executionController();PartitionIterator iterator=command.executeInternal(executionController)){
    if (iterator.hasNext()) {
      try (RowIterator partition=iterator.next()){
        throw new AssertionError("Expected no results for query " + command.toCQLString() + " but got key "+ command.metadata().getKeyValidator().getString(partition.partitionKey().getKey()));
      }
     }
  }
 }
