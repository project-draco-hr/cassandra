{
  IDiskAtomFilter filter=superColumn == null ? new IdentityQueryFilter() : new SliceQueryFilter(SuperColumns.startOf(superColumn),SuperColumns.endOf(superColumn),false,Integer.MAX_VALUE);
  Token min=StorageService.getPartitioner().getMinimumToken();
  return cfs.getRangeSlice(Bounds.makeRowBounds(min,min),null,filter,10000);
}
