{
  if (currentSlice == null) {
    if (idx >= slices.length)     return endOfData();
    ColumnSlice slice=slices[idx++];
    if (slice.start.isEmpty()) {
      if (slice.finish.isEmpty())       currentSlice=set.iterator();
 else       currentSlice=set.headSet(fakeCell(slice.finish),true).iterator();
    }
 else     if (slice.finish.isEmpty()) {
      currentSlice=set.tailSet(fakeCell(slice.start),true).iterator();
    }
 else {
      currentSlice=set.subSet(fakeCell(slice.start),true,fakeCell(slice.finish),true).iterator();
    }
  }
  if (currentSlice.hasNext())   return currentSlice.next();
  currentSlice=null;
  return computeNext();
}
