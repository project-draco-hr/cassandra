{
  Comparator<ByteBuffer> orderedComparator=reversed ? comparator.reverseComparator : comparator;
  if (slice.start.remaining() > 0 && slice.finish.remaining() > 0 && orderedComparator.compare(slice.start,slice.finish) > 0)   throw new IllegalArgumentException("Slice finish must come after start in traversal order");
}
