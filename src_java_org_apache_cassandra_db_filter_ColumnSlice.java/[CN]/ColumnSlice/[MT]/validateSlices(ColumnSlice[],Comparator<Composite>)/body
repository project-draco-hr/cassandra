{
  for (int i=0; i < slices.length; i++) {
    if (i > 0 && comparator.compare(slices[i - 1].finish,slices[i].start) >= 0)     return false;
    if (slices[i].finish.isEmpty())     return i == slices.length - 1;
    if (comparator.compare(slices[i].start,slices[i].finish) > 0)     return false;
  }
  return true;
}
