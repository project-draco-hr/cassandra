{
  try {
    ssProxy.truncate(tableName,cfName);
  }
 catch (  UnavailableException e) {
    throw new RuntimeException("Error while executing truncate",e);
  }
catch (  TimeoutException e) {
    throw new RuntimeException("Error while executing truncate",e);
  }
catch (  IOException e) {
    throw new RuntimeException("Error while executing truncate",e);
  }
}
