{
  for (  String upNode : getLiveNodes().split("\\s+")) {
    if (upNode.length() > 0) {
      outs.println(String.format("%-21s up",upNode));
    }
  }
  for (  String downNode : getUnreachableNodes().split("\\s+")) {
    if (downNode.length() > 0) {
      outs.println(String.format("%-21s down",downNode));
    }
  }
}
