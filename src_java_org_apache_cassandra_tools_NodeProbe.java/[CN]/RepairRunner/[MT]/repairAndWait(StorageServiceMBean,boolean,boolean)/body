{
  cmd=ssProxy.forceRepairAsync(keyspace,isSequential,primaryRangeOnly,columnFamilies);
  if (cmd > 0) {
    condition.await();
  }
 else {
    String message=String.format("[%s] Nothing to repair for keyspace '%s'",format.format(System.currentTimeMillis()),keyspace);
    out.println(message);
  }
}
