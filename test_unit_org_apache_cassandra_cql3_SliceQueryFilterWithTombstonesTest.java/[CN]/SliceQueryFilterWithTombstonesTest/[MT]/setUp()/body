{
  SchemaLoader.loadSchema();
  process(String.format("CREATE KEYSPACE IF NOT EXISTS %s WITH replication = {'class': 'SimpleStrategy', 'replication_factor': '1'}",KEYSPACE),ConsistencyLevel.ONE);
  process(String.format("CREATE TABLE IF NOT EXISTS %s.%s (a text, b text, c text, PRIMARY KEY (a, b));",KEYSPACE,TABLE),ConsistencyLevel.ONE);
  DatabaseDescriptor.setTombstoneFailureThreshold(THRESHOLD);
}
