{
  for (int i=0; i < THRESHOLD; i++)   execute("INSERT INTO %s.%s (a, b, c) VALUES ('key1', 'column" + i + "', null);");
  try {
    execute("SELECT * FROM %s.%s WHERE a = 'key1';");
  }
 catch (  Throwable e) {
    fail("SELECT with tombstones below the threshold should not have failed, but has: " + e);
  }
}
