{
  List<SSTableStreamingSections> sstableDetails=new ArrayList<>(sstables.size());
  for (  SSTableReader sstable : sstables)   sstableDetails.add(new SSTableStreamingSections(sstable,sstable.getPositionsForRanges(ranges),sstable.estimatedKeysForRanges(ranges)));
  addTransferFiles(sstableDetails);
}
