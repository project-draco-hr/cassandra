{
  boolean completed=receivers.isEmpty() && transfers.isEmpty();
  if (completed) {
    if (state == State.WAIT_COMPLETE) {
      if (!completeSent) {
        handler.sendMessage(new CompleteMessage());
        completeSent=true;
      }
      closeSession(State.COMPLETE);
    }
 else {
      handler.sendMessage(new CompleteMessage());
      completeSent=true;
      state(State.WAIT_COMPLETE);
    }
  }
  return completed;
}
