{
  super(sw);
  this.file=new File(file);
  this.descriptor=Descriptor.fromFilename(file);
  this.writer=sw;
  ArrayBackedSortedColumns cf=ArrayBackedSortedColumns.factory.create(cfs.metadata);
  for (int i=0; i < 10; i++)   cf.addColumn(Util.cellname(i),SSTableRewriterTest.random(0,1000),1);
  for (int i=0; i < 100; i++)   writer.append(StorageService.getPartitioner().decorateKey(ByteBufferUtil.bytes(i)),cf);
}
