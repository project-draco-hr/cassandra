{
  StringBuilder sb=new StringBuilder("CqlRow(");
  boolean first=true;
  sb.append("key:");
  if (this.key == null) {
    sb.append("null");
  }
 else {
    TBaseHelper.toString(this.key,sb);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("columns:");
  if (this.columns == null) {
    sb.append("null");
  }
 else {
    sb.append(this.columns);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
