{
  ClusteringComparator comparator=metadata.comparator;
  NavigableSet<Clustering> clusterings=new TreeSet<>(comparator);
  int size=in.readInt();
  for (int i=0; i < size; i++)   clusterings.add(Clustering.serializer.deserialize(in,version,comparator.subtypes()).takeAlias());
  return new ClusteringIndexNamesFilter(clusterings,reversed);
}
