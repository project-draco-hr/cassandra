{
  if (!row.primaryKeyLivenessInfo().hasTimestamp() || row.primaryKeyLivenessInfo().timestamp() <= sstableTimestamp)   return false;
  for (  ColumnDefinition column : requestedColumns) {
    if (column.type.isCollection())     return false;
    Cell cell=row.getCell(column);
    if (cell == null || cell.livenessInfo().timestamp() <= sstableTimestamp)     return false;
  }
  return true;
}
