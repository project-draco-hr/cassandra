{
  if (writer == null) {
    writer=newWriter;
    return;
  }
  SSTableReader reader=writer.closeAndOpenReader(maxAge);
  finished.add(reader);
  replaceReader(currentlyOpenedEarly,reader);
  moveStarts(reader,Functions.constant(reader.last),false);
  currentlyOpenedEarly=null;
  currentlyOpenedEarlyAt=0;
  writer=newWriter;
}
