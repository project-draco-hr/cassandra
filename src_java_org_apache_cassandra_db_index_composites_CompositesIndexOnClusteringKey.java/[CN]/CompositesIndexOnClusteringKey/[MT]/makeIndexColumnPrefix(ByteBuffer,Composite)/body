{
  int count=Math.min(baseCfs.metadata.clusteringColumns().size(),columnName.size());
  CBuilder builder=getIndexComparator().prefixBuilder();
  builder.add(rowKey);
  for (int i=0; i < Math.min(columnDef.position(),count); i++)   builder.add(columnName.get(i));
  for (int i=columnDef.position() + 1; i < count; i++)   builder.add(columnName.get(i));
  return builder.build();
}
