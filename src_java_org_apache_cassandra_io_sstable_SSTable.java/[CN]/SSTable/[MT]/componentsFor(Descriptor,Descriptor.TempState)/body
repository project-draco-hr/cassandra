{
  final Set<Component> components=new HashSet<Component>();
  desc.directory.listFiles(new FileFilter(){
    public boolean accept(    File file){
      if (file.isDirectory() || !file.getName().startsWith(desc.cfname))       return false;
      Pair<Descriptor,Component> component=tryComponentFromFilename(file.getParentFile(),file.getName());
      if (component != null && component.left.equals(desc) && (matchState.isMatch(component.left)))       components.add(component.right);
      return false;
    }
  }
);
  return components;
}
