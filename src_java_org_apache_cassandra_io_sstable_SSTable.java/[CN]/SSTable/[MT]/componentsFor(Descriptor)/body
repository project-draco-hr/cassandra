{
  try {
    try {
      return readTOC(desc);
    }
 catch (    FileNotFoundException e) {
      Set<Component> components=discoverComponentsFor(desc);
      if (!components.contains(Component.TOC))       components.add(Component.TOC);
      appendTOC(desc,components);
      return components;
    }
  }
 catch (  IOException e) {
    throw new IOError(e);
  }
}
