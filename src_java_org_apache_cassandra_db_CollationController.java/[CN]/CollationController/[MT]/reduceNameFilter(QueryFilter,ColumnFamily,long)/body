{
  AbstractColumnContainer container=filter.path.superColumnName == null ? returnCF : (SuperColumn)returnCF.getColumn(filter.path.superColumnName);
  if (container == null)   return;
  for (Iterator<ByteBuffer> iterator=((NamesQueryFilter)filter.filter).columns.iterator(); iterator.hasNext(); ) {
    ByteBuffer filterColumn=iterator.next();
    IColumn column=container.getColumn(filterColumn);
    if (column != null && column.timestamp() > sstableTimestamp)     iterator.remove();
  }
}
