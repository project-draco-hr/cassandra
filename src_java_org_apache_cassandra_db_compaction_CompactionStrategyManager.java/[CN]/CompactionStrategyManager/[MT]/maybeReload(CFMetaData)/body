{
  if (repaired != null && repaired.getClass().equals(metadata.params.compaction.klass()) && unrepaired != null && unrepaired.getClass().equals(metadata.params.compaction.klass()) && repaired.options.equals(metadata.params.compaction.options()) && unrepaired.options.equals(metadata.params.compaction.options()))   return;
  reload(metadata);
}
