{
  if (!hasNext())   return null;
  int state=this.state;
  int index=seekTo(target,forwards,(state & (ON_ITEM | BEFORE_FIRST)) != 0);
  boolean found=index >= 0;
  if (!found)   index=-1 - index;
  V next=null;
  if (state == BEFORE_FIRST && compareToFirst(index) < 0)   return null;
  int compareToLast=compareToLast(index);
  if ((compareToLast <= 0)) {
    state=compareToLast < 0 ? MIDDLE : LAST;
    if (found) {
      state|=ON_ITEM;
      next=(V)currentValue();
    }
  }
 else   state=END;
  this.state=(byte)state;
  this.index=index;
  return next;
}
