{
  columnCounter=getColumnCounter(container);
  while (reducedColumns.hasNext()) {
    IColumn column=reducedColumns.next();
    if (logger.isTraceEnabled())     logger.trace(String.format("collecting %s of %s: %s",columnCounter.live(),count,column.getString(container.getComparator())));
    columnCounter.count(column,container);
    if (columnCounter.live() > count)     break;
    if (QueryFilter.isRelevant(column,container,gcBefore))     container.addColumn(column);
  }
  logger.debug("Read {} live cells and {} tombstoned",columnCounter.live(),columnCounter.ignored());
}
