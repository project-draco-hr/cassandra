{
  if (compositesToGroup < 0)   return new ColumnCounter(now);
 else   if (compositesToGroup == 0)   return new ColumnCounter.GroupByPrefix(now,null,0);
 else   if (reversed)   return new ColumnCounter.GroupByPrefixReversed(now,comparator,compositesToGroup);
 else   return new ColumnCounter.GroupByPrefix(now,comparator,compositesToGroup);
}
