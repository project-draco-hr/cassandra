{
  int liveColumns=0;
  for (  IColumn column : reducedColumns) {
    if (liveColumns >= count)     break;
    if (!finish.isEmpty() && ((isAscending && column.name().compareTo(finish) > 0)) || (!isAscending && column.name().compareTo(finish) < 0))     break;
    if (!column.isMarkedForDelete())     liveColumns++;
    returnCF.addColumn(column);
  }
}
