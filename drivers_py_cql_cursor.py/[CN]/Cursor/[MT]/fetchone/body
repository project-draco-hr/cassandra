def fetchone(self):
    self.__checksock()
    if (self.rs_idx == len(self.result)):
        return None
    row = self.result[self.rs_idx]
    self.rs_idx += 1
    if (self.description == _COUNT_DESCRIPTION):
        return row
    else:
        self.description = self.decoder.decode_description(row)
        return self.decoder.decode_row(row)
