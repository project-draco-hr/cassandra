def fetchmany(self, size=None):
    self.__checksock()
    if (size is None):
        size = self.arraysize
    L = []
    while ((len(L) < size) and (self.rs_idx < len(self.result))):
        row = self.result[self.rs_idx]
        self.rs_idx += 1
        L.append(self.decoder.decode_row(self._query_ks, self._query_cf, row))
    return L
