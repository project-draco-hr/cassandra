{
  if (!(variables.isEmpty() && (statement.boundTerms == 0))) {
    if (variables.size() != statement.boundTerms)     throw new InvalidRequestException(String.format("there were %d markers(?) in CQL but %d bound variables",statement.boundTerms,variables.size()));
    if (logger.isTraceEnabled())     for (int i=0; i < variables.size(); i++)     logger.trace("[{}] '{}'",i + 1,variables.get(i));
  }
  return processStatement(statement,new ExecutionContext(clientState,null,variables));
}
