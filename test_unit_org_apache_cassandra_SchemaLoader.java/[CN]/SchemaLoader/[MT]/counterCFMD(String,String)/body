{
  return CFMetaData.Builder.create(ksName,cfName,false,true,true).addPartitionKey("key",AsciiType.instance).addClusteringColumn("name",AsciiType.instance).addRegularColumn("val",CounterColumnType.instance).addRegularColumn("val2",CounterColumnType.instance).build().compressionParameters(getCompressionParameters());
}
