{
  CFMetaData.Builder builder=CFMetaData.Builder.create(ksName,cfName).addPartitionKey("key",keyType).addClusteringColumn("name",clusteringType).addRegularColumn("val",valType);
  for (int i=0; i < columnCount; i++)   builder.addRegularColumn("val" + i,AsciiType.instance);
  return builder.build().compressionParameters(getCompressionParameters());
}
