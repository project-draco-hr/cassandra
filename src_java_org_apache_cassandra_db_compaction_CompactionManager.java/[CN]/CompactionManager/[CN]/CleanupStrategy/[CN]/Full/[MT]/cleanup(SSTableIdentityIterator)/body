{
  if (Range.isInRanges(row.getKey().token,ranges))   return row;
  cfs.invalidateCachedRow(row.getKey());
  if (indexedColumnsInRow != null)   indexedColumnsInRow.clear();
  while (row.hasNext()) {
    OnDiskAtom column=row.next();
    if (column instanceof Cell && cfs.indexManager.indexes((Cell)column)) {
      if (indexedColumnsInRow == null)       indexedColumnsInRow=new ArrayList<>();
      indexedColumnsInRow.add((Cell)column);
    }
  }
  if (indexedColumnsInRow != null && !indexedColumnsInRow.isEmpty()) {
    try (OpOrder.Group opGroup=cfs.keyspace.writeOrder.start()){
      cfs.indexManager.deleteFromIndexes(row.getKey(),indexedColumnsInRow,opGroup);
    }
   }
  return null;
}
