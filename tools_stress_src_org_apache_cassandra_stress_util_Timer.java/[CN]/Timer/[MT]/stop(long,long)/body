{
  maybeReport();
  long now=System.nanoTime();
  long time=now - sampleStartNanos;
  if (rnd.nextInt(p(opCount)) == 0)   sample[index(opCount)]=time;
  if (time > max) {
    maxStart=sampleStartNanos;
    max=time;
  }
  total+=time;
  opCount+=1;
  this.partitionCount+=partitionCount;
  this.rowCount+=rowCount;
  upToDateAsOf=now;
}
