{
  final List<SampleOfLongs> sampleLatencies=Arrays.asList(new SampleOfLongs(Arrays.copyOf(sample,index(opCount)),p(opCount)),new SampleOfLongs(Arrays.copyOfRange(sample,index(opCount),Math.min(opCount,sample.length)),p(opCount) - 1));
  final TimingInterval report=new TimingInterval(lastSnap,upToDateAsOf,max,maxStart,max,partitionCount,rowCount,total,opCount,SampleOfLongs.merge(rnd,sampleLatencies,Integer.MAX_VALUE));
  opCount=0;
  partitionCount=0;
  rowCount=0;
  total=0;
  max=0;
  lastSnap=upToDateAsOf;
  return report;
}
