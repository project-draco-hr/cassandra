{
  Cassandra.Client client=new Cassandra.Client(new TBinaryProtocol(new TFramedTransportFactory().openTransport(InetAddress.getLocalHost().getHostName(),9170)));
  client.set_keyspace(ksName);
  client.cas(ByteBufferUtil.bytes(6),cfName,Collections.EMPTY_LIST,Collections.singletonList(getColumnForInsert("v1",6)),org.apache.cassandra.thrift.ConsistencyLevel.LOCAL_SERIAL,org.apache.cassandra.thrift.ConsistencyLevel.ONE);
  assertUpdateIsAugmented(6);
}
