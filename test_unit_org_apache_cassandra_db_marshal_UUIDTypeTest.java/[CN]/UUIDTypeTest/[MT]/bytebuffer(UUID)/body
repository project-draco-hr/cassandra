{
  long msb=uuid.getMostSignificantBits();
  long lsb=uuid.getLeastSignificantBits();
  byte[] bytes=new byte[16];
  for (int i=0; i < 8; i++) {
    bytes[i]=(byte)(msb >>> 8 * (7 - i));
  }
  for (int i=8; i < 16; i++) {
    bytes[i]=(byte)(lsb >>> 8 * (7 - i));
  }
  return ByteBuffer.wrap(bytes);
}
