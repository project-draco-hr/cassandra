{
  if (nextFull != null)   return;
  nextIdx=0;
  nextSize=0;
  int length=in.readShort() & 0xFFFF;
  if (length == 0) {
    nextFull=EMPTY;
    return;
  }
  nextFull=new byte[length];
  in.readFully(nextFull);
  nextIsStatic=false;
  if (peekShort() == CompositeType.STATIC_MARKER) {
    nextIsStatic=true;
    readShort();
  }
}
