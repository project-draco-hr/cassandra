{
  if (this.curr_ >= this.hi_) {
    if (this.hitEOF_ && this.hi_ < this.maxHi_) {
      this.hi_++;
    }
 else {
      this.seek(this.curr_);
      if (this.curr_ == this.hi_) {
        this.hi_++;
      }
    }
  }
  this.buffer_[(int)(this.curr_ - this.lo_)]=(byte)b;
  this.curr_++;
  this.dirty_=true;
}
