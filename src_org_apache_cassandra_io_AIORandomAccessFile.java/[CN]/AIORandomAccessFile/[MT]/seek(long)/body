{
  if (pos >= this.hi_ || pos < this.lo_) {
    this.flushBuffer();
    this.lo_=pos & BuffMask_;
    this.maxHi_=this.lo_ + (long)this.buffer_.length;
    if (this.diskPos_ != this.lo_) {
      this.diskPos_=this.lo_;
    }
    int n=0;
    if (bContinuations_) {
      n=fillBuffer2();
    }
 else {
      n=fillBuffer();
    }
    this.hi_=this.lo_ + (long)n;
  }
 else {
    if (pos < this.curr_) {
      this.flushBuffer();
    }
  }
  this.curr_=pos;
}
