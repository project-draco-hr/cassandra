{
  StringBuilder sb=new StringBuilder("superColumn_t(");
  boolean first=true;
  if (!first)   sb.append(", ");
  sb.append("name:");
  sb.append(this.name);
  first=false;
  if (!first)   sb.append(", ");
  sb.append("columns:");
  sb.append(this.columns);
  first=false;
  sb.append(")");
  return sb.toString();
}
