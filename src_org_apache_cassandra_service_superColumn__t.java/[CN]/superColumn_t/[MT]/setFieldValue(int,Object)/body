{
switch (fieldID) {
case NAME:
    if (value == null) {
      unsetName();
    }
 else {
      setName((String)value);
    }
  break;
case COLUMNS:
if (value == null) {
  unsetColumns();
}
 else {
  setColumns((List<column_t>)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
