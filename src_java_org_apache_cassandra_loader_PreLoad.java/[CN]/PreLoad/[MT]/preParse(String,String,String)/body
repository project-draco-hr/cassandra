{
  BufferedReader bufReader=new BufferedReader(new InputStreamReader(new FileInputStream(rootDirectory)),16 * 1024 * 1024);
  String line=null;
  while ((line=bufReader.readLine()) != null) {
    String userId=line;
    RowMutation rm=new RowMutation(table,userId);
    rm.add(cfName,userId.getBytes(),0);
    rm.apply();
  }
}
