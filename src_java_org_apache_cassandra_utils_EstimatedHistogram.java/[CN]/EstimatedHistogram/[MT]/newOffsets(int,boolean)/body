{
  long[] result=new long[size + (considerZeroes ? 1 : 0)];
  int i=0;
  if (considerZeroes)   result[i++]=0;
  long last=1;
  result[i++]=last;
  for (; i < result.length; i++) {
    long next=Math.round(last * 1.2);
    if (next == last)     next++;
    result[i]=next;
    last=next;
  }
  return result;
}
