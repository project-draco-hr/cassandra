{
  nameDeserializer.skipNext();
  nextFlags=nextFlags == Integer.MIN_VALUE ? in.readUnsignedByte() : nextFlags;
  if ((nextFlags & ColumnSerializer.RANGE_TOMBSTONE_MASK) != 0)   type.rangeTombstoneSerializer().skipBody(in,version);
 else   type.columnSerializer().skipColumnBody(in,nextFlags);
  nextFlags=Integer.MIN_VALUE;
}
