{
  Matcher m=FULL.matcher(spec);
  if (!m.matches())   throw new IllegalArgumentException("Illegal data generator specification: " + spec);
  String name=m.group(1);
  Impl impl=LOOKUP.get(name.toLowerCase());
  if (impl == null)   throw new IllegalArgumentException("Illegal data generator type: " + name);
  List<String> params=new ArrayList<>();
  m=ARGS.matcher(m.group(2));
  while (m.find())   params.add(m.group());
  return impl.getFactory(params);
}
