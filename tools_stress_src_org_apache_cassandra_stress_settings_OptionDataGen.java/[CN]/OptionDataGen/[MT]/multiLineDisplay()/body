{
  return Arrays.asList(GroupedOptions.formatMultiLine("RANDOM()","Completely random byte generation"),GroupedOptions.formatMultiLine("REPEAT(<freq>)","An MD5 hash of (opIndex % freq) combined with the column index"),GroupedOptions.formatMultiLine("DICT(<file>)","Random words from a dictionary; the file should be in the format \"<freq> <word>\""));
}
