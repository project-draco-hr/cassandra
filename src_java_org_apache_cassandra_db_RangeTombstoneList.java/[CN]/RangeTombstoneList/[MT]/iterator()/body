{
  return new AbstractIterator<RangeTombstone>(){
    private int idx;
    protected RangeTombstone computeNext(){
      if (idx >= size)       return endOfData();
      RangeTombstone t=new RangeTombstone(starts[idx],ends[idx],markedAts[idx],delTimes[idx]);
      idx++;
      return t;
    }
  }
;
}
