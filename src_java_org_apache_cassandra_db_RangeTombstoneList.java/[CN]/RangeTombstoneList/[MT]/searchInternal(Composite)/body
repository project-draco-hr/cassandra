{
  if (isEmpty())   return -1;
  int pos=Arrays.binarySearch(starts,0,size,name,comparator);
  if (pos >= 0) {
    if (pos > 0 && comparator.compare(name,ends[pos - 1]) == 0 && markedAts[pos - 1] > markedAts[pos])     return pos - 1;
 else     return pos;
  }
 else {
    int idx=-pos - 2;
    if (idx < 0)     return -1;
    return comparator.compare(name,ends[idx]) <= 0 ? idx : -1;
  }
}
