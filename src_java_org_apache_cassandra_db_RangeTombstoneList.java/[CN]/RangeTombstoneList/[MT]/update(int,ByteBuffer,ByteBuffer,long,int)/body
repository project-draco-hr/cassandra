{
  if (markedAts[i] < markedAt) {
    if (comparator.compare(ends[i],end) != 0)     addInternal(i + 1,end,ends[i],markedAts[i],delTimes[i]);
    if (comparator.compare(starts[i],start) == 0) {
      markedAts[i]=markedAt;
      delTimes[i]=delTime;
      ends[i]=end;
    }
 else {
      addInternal(i + 1,start,end,markedAt,delTime);
      ends[i]=start;
    }
  }
}
