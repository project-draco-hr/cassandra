{
  RangeTombstoneList copy=new RangeTombstoneList(comparator,new Slice.Bound[size],new Slice.Bound[size],Arrays.copyOf(markedAts,size),Arrays.copyOf(delTimes,size),boundaryHeapSize,size);
  for (int i=0; i < size; i++) {
    copy.starts[i]=clone(starts[i],allocator);
    copy.ends[i]=clone(ends[i],allocator);
  }
  return copy;
}
