{
  if (!(o instanceof RangeTombstoneList))   return false;
  RangeTombstoneList that=(RangeTombstoneList)o;
  if (size != that.size)   return false;
  for (int i=0; i < size; i++) {
    if (!starts[i].equals(that.starts[i]))     return false;
    if (!ends[i].equals(that.ends[i]))     return false;
    if (markedAts[i] != that.markedAts[i])     return false;
    if (delTimes[i] != that.delTimes[i])     return false;
  }
  return true;
}
