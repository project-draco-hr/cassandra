{
  while (idx < size) {
    int cmp=comparator.compare(name,starts[idx]);
    if (cmp == 0) {
      if (idx > 0 && comparator.compare(name,ends[idx - 1]) == 0 && markedAts[idx - 1] > markedAts[idx])       return markedAts[idx - 1] >= timestamp;
 else       return markedAts[idx] >= timestamp;
    }
 else     if (cmp < 0) {
      return false;
    }
 else {
      if (comparator.compare(name,ends[idx]) <= 0)       return markedAts[idx] >= timestamp;
 else       idx++;
    }
  }
  return false;
}
