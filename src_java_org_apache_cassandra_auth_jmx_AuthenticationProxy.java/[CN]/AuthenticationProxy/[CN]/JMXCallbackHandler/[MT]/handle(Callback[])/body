{
  for (int i=0; i < callbacks.length; i++) {
    if (callbacks[i] instanceof NameCallback)     ((NameCallback)callbacks[i]).setName(username == null ? null : new String(username));
 else     if (callbacks[i] instanceof PasswordCallback)     ((PasswordCallback)callbacks[i]).setPassword(password == null ? null : password);
 else     throw new UnsupportedCallbackException(callbacks[i],"Unrecognized Callback: " + callbacks[i].getClass().getName());
  }
}
