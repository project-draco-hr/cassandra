{
  if (keysToSave >= getKeySet().size())   keys=getKeySet();
 else   keys=hotKeySet(keysToSave);
  estimatedTotalBytes=estimateSizeToSave(keys);
  OperationType type;
  if (cacheType == CacheService.CacheType.KEY_CACHE)   type=OperationType.KEY_CACHE_SAVE;
 else   if (cacheType == CacheService.CacheType.ROW_CACHE)   type=OperationType.ROW_CACHE_SAVE;
 else   type=OperationType.UNKNOWN;
  info=new CompactionInfo(this.hashCode(),"Global",cacheType.toString(),type,0,estimatedTotalBytes);
}
