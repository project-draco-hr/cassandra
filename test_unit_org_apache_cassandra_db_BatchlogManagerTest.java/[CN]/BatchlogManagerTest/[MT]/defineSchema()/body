{
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE1,KeyspaceParams.simple(1),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD1,1,BytesType.instance),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD2),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD3));
  System.out.println(Keyspace.open(KEYSPACE1).getColumnFamilyStore(CF_STANDARD1).metadata.partitionKeyColumns());
