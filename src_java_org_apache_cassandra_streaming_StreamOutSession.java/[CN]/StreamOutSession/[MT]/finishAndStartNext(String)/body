{
  PendingFile pf=fileMap.remove(pfname);
  files.remove(pf);
  if (files.size() > 0)   streamFile(files.get(0));
 else {
    if (logger.isDebugEnabled())     logger.debug("Signalling that streaming is done for {} session {}",context.host,context.sessionId);
    remove(context);
    condition.signalAll();
  }
}
