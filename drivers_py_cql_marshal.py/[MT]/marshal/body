def marshal(term):
    if isinstance(term, long):
        return ('%dL' % term)
    elif isinstance(term, int):
        return ('%d' % term)
    elif isinstance(term, unicode):
        return ('u"%s"' % term)
    elif isinstance(term, str):
        return ('"%s"' % term)
    elif isinstance(term, UUID):
        if (term.version == 1):
            return ('timeuuid("%s")' % str(term))
        else:
            return ('uuid("%s")' % str(term))
    else:
        return str(term)
