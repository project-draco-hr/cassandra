{
  Set<SSTableReader> container;
  if (staged.contains(reader))   container=staged.update.contains(reader) ? staged.update : staged.obsolete;
 else   if (logged.contains(reader))   container=logged.update.contains(reader) ? logged.update : logged.obsolete;
 else   if (originals.contains(reader))   container=originals;
 else   throw new AssertionError();
  return select(reader,container);
}
