{
  BloomFilter bf=new BloomFilter(64 * 1024 * 1024,15);
  for (int i=0; i < 64 * 1024 * 1024; ++i) {
    bf.fill(Integer.toString(i));
  }
  System.out.println("Done filling ...");
  for (int i=0; i < 64 * 1024 * 1024; ++i) {
    if (!bf.isPresent(Integer.toString(i)))     System.out.println("Oops");
  }
}
