{
  Map<String,String> lowerCased=new HashMap<String,String>(value.size());
  for (  Map.Entry<String,String> entry : value.entrySet())   lowerCased.put(entry.getKey().toLowerCase(),entry.getValue());
  if (properties.put(name,lowerCased) != null)   throw new SyntaxException(String.format("Multiple definition for property '%s'",name));
}
