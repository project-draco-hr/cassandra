{
  boolean logTpstats=false;
  for (  Map.Entry<StorageService.Verb,AtomicInteger> entry : droppedMessages.entrySet()) {
    AtomicInteger dropped=entry.getValue();
    if (dropped.get() > 0) {
      logTpstats=true;
      logger_.info("{} {} messages dropped in server lifetime",dropped,entry.getKey());
    }
  }
  if (logTpstats)   StatusLogger.log();
}
