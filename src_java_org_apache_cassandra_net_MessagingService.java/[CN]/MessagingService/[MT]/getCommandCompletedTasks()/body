{
  Map<String,Long> completedTasks=new HashMap<String,Long>(connectionManagers.size());
  for (  Map.Entry<InetAddress,OutboundTcpConnectionPool> entry : connectionManagers.entrySet())   completedTasks.put(entry.getKey().getHostAddress(),entry.getValue().cmdCon.getCompletedMesssages());
  return completedTasks;
}
