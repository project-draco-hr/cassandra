{
  OutboundTcpConnectionPool cp=connectionManagers.get(to);
  if (cp == null) {
    cp=new OutboundTcpConnectionPool(to);
    OutboundTcpConnectionPool existingPool=connectionManagers.putIfAbsent(to,cp);
    if (existingPool != null) {
      cp.close();
      cp=existingPool;
    }
  }
  return cp;
}
