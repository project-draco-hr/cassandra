{
  int header=0;
  header|=serializerType_.ordinal();
  if (compress)   header|=4;
  header|=(version_ << 8);
  ByteBuffer buffer=ByteBuffer.allocate(4 + 4 + 4+ bytes.length);
  buffer.putInt(PROTOCOL_MAGIC);
  buffer.putInt(header);
  buffer.putInt(bytes.length);
  buffer.put(bytes);
  buffer.flip();
  return buffer;
}
