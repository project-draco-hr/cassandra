{
  if ((relation != null) && relation.isKey()) {
    if (relation.operator().equals(RelationType.EQ))     keys.add(relation.getValue());
 else     if ((relation.operator().equals(RelationType.GT) || relation.operator().equals(RelationType.GTE)))     startKey=relation.getValue();
 else     if ((relation.operator().equals(RelationType.LT) || relation.operator().equals(RelationType.LTE)))     finishKey=relation.getValue();
  }
 else   columns.add(relation);
}
