{
  for (  String keyspaceName : Schema.instance.getKeyspaces()) {
    for (    CFMetaData cfm : Schema.instance.getKeyspaceMetaData(keyspaceName).values()) {
      if (LegacyLeveledManifest.manifestNeedsMigration(keyspaceName,cfm.cfName))       LegacyLeveledManifest.migrateManifests(keyspaceName,cfm.cfName);
    }
  }
}
