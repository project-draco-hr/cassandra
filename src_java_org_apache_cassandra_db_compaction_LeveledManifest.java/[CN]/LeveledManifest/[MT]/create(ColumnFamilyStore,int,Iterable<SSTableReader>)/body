{
  LeveledManifest manifest=new LeveledManifest(cfs,maxSSTableSize);
  load(cfs,manifest,sstables);
  for (  SSTableReader ssTableReader : sstables) {
    if (manifest.levelOf(ssTableReader) < 0)     manifest.add(ssTableReader);
  }
  for (int i=1; i < manifest.getAllLevelSize().length; i++)   manifest.repairOverlappingSSTables(i);
  return manifest;
}
