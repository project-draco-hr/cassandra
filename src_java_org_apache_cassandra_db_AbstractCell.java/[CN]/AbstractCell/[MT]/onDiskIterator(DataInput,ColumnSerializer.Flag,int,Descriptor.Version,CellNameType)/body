{
  return new AbstractIterator<OnDiskAtom>(){
    protected OnDiskAtom computeNext(){
      OnDiskAtom atom;
      try {
        atom=type.onDiskAtomSerializer().deserializeFromSSTable(in,flag,expireBefore,version);
      }
 catch (      IOException e) {
        throw new IOError(e);
      }
      if (atom == null)       return endOfData();
      return atom;
    }
  }
;
}
