{
  try {
    reader=new BufferedReader(new InputStreamReader(new FileInputStream("/usr/share/dict/words")));
  }
 catch (  FileNotFoundException e) {
    throw new RuntimeException(e);
  }
  for (int i=0; i < skip; i++) {
    try {
      reader.readLine();
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
  next();
}
