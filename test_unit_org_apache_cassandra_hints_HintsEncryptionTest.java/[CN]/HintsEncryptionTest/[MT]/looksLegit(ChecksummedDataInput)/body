{
  if (!(checksummedDataInput instanceof EncryptedChecksummedDataInput))   return false;
  EncryptedChecksummedDataInput encryptedDataInput=(EncryptedChecksummedDataInput)checksummedDataInput;
  return Arrays.equals(cipher.getIV(),encryptedDataInput.getCipher().getIV()) && encryptedDataInput.getCompressor().getClass().isAssignableFrom(encryptionContext.getCompressor().getClass());
}
