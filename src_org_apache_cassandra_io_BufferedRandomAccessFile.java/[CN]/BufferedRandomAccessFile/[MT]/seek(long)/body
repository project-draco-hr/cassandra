{
  if (pos >= this.hi_ || pos < this.lo_) {
    this.flushBuffer();
    this.lo_=pos & BuffMask_;
    this.maxHi_=this.lo_ + (long)this.buff_.length;
    if (this.diskPos_ != this.lo_) {
      super.seek(this.lo_);
      this.diskPos_=this.lo_;
    }
    int n=this.fillBuffer();
    this.hi_=this.lo_ + (long)n;
  }
 else {
    if (pos < this.curr_) {
      this.flushBuffer();
    }
  }
  this.curr_=pos;
}
