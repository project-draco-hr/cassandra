{
switch (level) {
case ROOT:
    return "<all functions>";
case KEYSPACE:
  return String.format("<all functions in %s>",keyspace);
case FUNCTION:
return String.format("<function %s.%s(%s)>",keyspace,name,Joiner.on(", ").join(AbstractType.asCQLTypeStringList(argTypes)));
}
throw new AssertionError();
}
