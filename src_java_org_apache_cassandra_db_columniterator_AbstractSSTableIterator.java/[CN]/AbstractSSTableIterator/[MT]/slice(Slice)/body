{
  try {
    if (reader != null)     reader.setForSlice(slice);
  }
 catch (  IOException e) {
    try {
      closeInternal();
    }
 catch (    IOException suppressed) {
      e.addSuppressed(suppressed);
    }
    sstable.markSuspect();
    throw new CorruptSSTableException(e,reader.file.getPath());
  }
}
