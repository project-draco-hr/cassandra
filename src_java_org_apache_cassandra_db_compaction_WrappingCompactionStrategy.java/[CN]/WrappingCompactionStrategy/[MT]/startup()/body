{
  super.startup();
  for (  SSTableReader sstable : cfs.getSSTables()) {
    if (sstable.openReason != SSTableReader.OpenReason.EARLY) {
      if (sstable.isRepaired())       repaired.addSSTable(sstable);
 else       unrepaired.addSSTable(sstable);
    }
  }
  repaired.startup();
  unrepaired.startup();
}
