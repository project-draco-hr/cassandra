{
  RandomAccessFile file=null;
  try {
    file=new RandomAccessFile(path,"r");
    return getfd(file.getFD());
  }
 catch (  Throwable t) {
    JVMStabilityInspector.inspectThrowable(t);
    return -1;
  }
 finally {
    try {
      if (file != null)       file.close();
    }
 catch (    Throwable t) {
    }
  }
}
