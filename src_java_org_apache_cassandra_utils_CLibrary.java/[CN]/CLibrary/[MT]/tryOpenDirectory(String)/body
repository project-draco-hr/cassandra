{
  int fd=-1;
  try {
    return open(path,O_RDONLY);
  }
 catch (  UnsatisfiedLinkError e) {
  }
catch (  RuntimeException e) {
    if (!(e instanceof LastErrorException))     throw e;
    logger.warn(String.format("open(%s, O_RDONLY) failed, errno (%d).",path,CLibrary.errno(e)));
  }
  return fd;
}
