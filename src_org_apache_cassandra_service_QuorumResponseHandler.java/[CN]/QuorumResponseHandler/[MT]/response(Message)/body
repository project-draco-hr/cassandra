{
  lock_.lock();
  try {
    int majority=(responseCount_ >> 1) + 1;
    if (!done_.get()) {
      responses_.add(message);
      if (responses_.size() >= majority && responseResolver_.isDataPresent(responses_)) {
        done_.set(true);
        condition_.signal();
      }
    }
  }
  finally {
    lock_.unlock();
  }
}
