{
  if (buffer == null)   return;
  syncInternal();
  buffer=null;
  if (skipIOCache && bytesSinceCacheFlush > 0)   CLibrary.trySkipCache(fd,0,0);
  try {
    out.close();
  }
 catch (  IOException e) {
    throw new FSWriteError(e,getPath());
  }
  FileUtils.closeQuietly(metadata);
  CLibrary.tryCloseFD(directoryFD);
}
