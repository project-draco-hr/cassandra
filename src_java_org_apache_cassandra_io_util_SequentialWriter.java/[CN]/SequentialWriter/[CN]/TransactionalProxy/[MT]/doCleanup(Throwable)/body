{
  if (directoryFD >= 0) {
    try {
      CLibrary.tryCloseFD(directoryFD);
    }
 catch (    Throwable t) {
      accumulate=merge(accumulate,t);
    }
    directoryFD=-1;
  }
  try {
    channel.close();
  }
 catch (  Throwable t) {
    accumulate=merge(accumulate,t);
  }
  if (buffer != null) {
    try {
      FileUtils.clean(buffer);
    }
 catch (    Throwable t) {
      accumulate=merge(accumulate,t);
    }
    buffer=null;
  }
  return accumulate;
}
