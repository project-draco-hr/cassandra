{
  if (startSessionIfRequested()) {
    Map<String,String> traceParameters=ImmutableMap.of("key",ByteBufferUtil.bytesToHex(key),"column_path",column_path.toString(),"consistency_level",consistency_level.name());
    Tracing.instance().begin("get",traceParameters);
  }
 else {
    logger.debug("get");
  }
  try {
    return internal_get(key,column_path,consistency_level);
  }
 catch (  RequestValidationException e) {
    throw ThriftConversion.toThrift(e);
  }
 finally {
    Tracing.instance().stopSession();
  }
}
