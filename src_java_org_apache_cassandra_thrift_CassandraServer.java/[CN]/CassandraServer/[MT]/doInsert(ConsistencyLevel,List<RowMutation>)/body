{
  try {
    schedule();
    if (consistency_level == ConsistencyLevel.ZERO) {
      StorageProxy.mutate(mutations);
    }
 else {
      try {
        StorageProxy.mutateBlocking(mutations,consistency_level);
      }
 catch (      TimeoutException e) {
        throw new TimedOutException();
      }
    }
  }
  finally {
    release();
  }
}
