{
  try {
    String queryString=uncompress(query,compression);
    if (startSessionIfRequested()) {
      Tracing.instance.begin("execute_cql3_query",ImmutableMap.of("query",queryString));
    }
 else {
      logger.debug("execute_cql3_query");
    }
    ThriftClientState cState=state();
    return ClientState.getCQLQueryHandler().process(queryString,cState.getQueryState(),QueryOptions.fromProtocolV2(ThriftConversion.fromThrift(cLevel),Collections.<ByteBuffer>emptyList()),null).toThriftResult();
  }
 catch (  RequestExecutionException e) {
    throw ThriftConversion.rethrow(e);
  }
catch (  RequestValidationException e) {
    throw ThriftConversion.toThrift(e);
  }
 finally {
    Tracing.instance.stopSession();
  }
}
