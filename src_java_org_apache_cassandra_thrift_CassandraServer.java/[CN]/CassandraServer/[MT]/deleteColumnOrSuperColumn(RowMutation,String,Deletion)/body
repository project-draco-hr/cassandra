{
  if (del.predicate != null && del.predicate.column_names != null) {
    for (    ByteBuffer c : del.predicate.column_names) {
      if (del.super_column == null && Schema.instance.getColumnFamilyType(rm.getTable(),cfName) == ColumnFamilyType.Super)       rm.deleteRange(cfName,SuperColumns.startOf(c),SuperColumns.endOf(c),del.timestamp);
 else       if (del.super_column != null)       rm.delete(cfName,CompositeType.build(del.super_column,c),del.timestamp);
 else       rm.delete(cfName,c,del.timestamp);
    }
  }
 else {
    if (del.super_column != null)     rm.deleteRange(cfName,SuperColumns.startOf(del.super_column),SuperColumns.endOf(del.super_column),del.timestamp);
 else     rm.delete(cfName,del.timestamp);
  }
}
