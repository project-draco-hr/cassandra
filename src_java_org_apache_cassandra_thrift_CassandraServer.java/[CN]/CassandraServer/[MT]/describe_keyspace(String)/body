{
  state().hasKeyspaceListAccess(Permission.READ);
  KSMetaData ksm=DatabaseDescriptor.getTableDefinition(table);
  if (ksm == null)   throw new NotFoundException();
  List<CfDef> cfDefs=new ArrayList<CfDef>();
  for (  CFMetaData cfm : ksm.cfMetaData().values())   cfDefs.add(CFMetaData.convertToThrift(cfm));
  return new KsDef(ksm.name,ksm.strategyClass.getName(),ksm.replicationFactor,cfDefs);
}
