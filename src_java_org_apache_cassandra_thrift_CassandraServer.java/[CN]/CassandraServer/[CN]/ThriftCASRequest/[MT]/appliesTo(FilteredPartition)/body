{
  if (expected == null || expected.isEmpty())   return current.isEmpty();
 else   if (current.isEmpty())   return false;
  for (  Row e : expected) {
    Row c=current.getRow(e.clustering());
    if (c == null)     return false;
    for (    Cell ce : e) {
      Cell cc=c.getCell(ce.column());
      if (cc == null || !cc.value().equals(ce.value()))       return false;
    }
  }
  return true;
}
