{
  if (group == null || type == null) {
    throw new IllegalArgumentException("Both group and type need to be specified");
  }
  if (name == null) {
    throw new IllegalArgumentException("Name needs to be specified");
  }
  this.group=group;
  this.type=type;
  this.name=name;
  this.scope=scope;
  this.mBeanName=mBeanName;
}
