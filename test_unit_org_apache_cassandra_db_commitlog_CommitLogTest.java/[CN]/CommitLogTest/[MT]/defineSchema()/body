{
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE1,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE1,CF1),SchemaLoader.standardCFMD(KEYSPACE1,CF2));
  SchemaLoader.createKeyspace(KEYSPACE2,false,true,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE1,CF1),SchemaLoader.standardCFMD(KEYSPACE1,CF2));
  CompactionManager.instance.disableAutoCompaction();
}
