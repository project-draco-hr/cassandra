{
  Bound b;
  boolean inclusive;
switch (type) {
case GT:
    b=Bound.START;
  inclusive=false;
break;
case GTE:
b=Bound.START;
inclusive=true;
break;
case LT:
b=Bound.END;
inclusive=false;
break;
case LTE:
b=Bound.END;
inclusive=true;
break;
default :
throw new AssertionError();
}
setBound(b,inclusive,t);
}
