{
  if (!isInit) {
    seekToPosition(indexEntry.position);
    ByteBufferUtil.skipShortLength(file);
    DeletionTime.serializer.skip(file);
    if (sstable.header.hasStatic())     UnfilteredSerializer.serializer.skipStaticRow(file,sstable.header,helper);
    isInit=true;
  }
  return deserializer.hasNext();
}
