{
  super.setForSlice(slice);
  isInit=true;
  if (indexState.isDone()) {
    sliceDone=true;
    return;
  }
  int startIdx=indexState.findBlockIndex(slice.start());
  if (startIdx >= indexState.blocksCount()) {
    sliceDone=true;
    return;
  }
  if (startIdx != indexState.currentBlockIdx())   indexState.setToBlock(startIdx);
  lastBlockIdx=indexState.findBlockIndex(slice.end());
  if (indexState.currentBlockIdx() == lastBlockIdx && metadata().comparator.compare(slice.end(),indexState.currentIndex().firstName) < 0 && openMarker == null && sstable.descriptor.version.storeRows()) {
    sliceDone=true;
  }
}
