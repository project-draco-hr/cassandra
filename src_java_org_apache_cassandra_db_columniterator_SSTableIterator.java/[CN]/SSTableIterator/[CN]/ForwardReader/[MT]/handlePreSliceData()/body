{
  while (deserializer.hasNext() && deserializer.compareNextTo(start) <= 0) {
    if (deserializer.nextIsRow())     deserializer.skipNext();
 else     updateOpenMarker((RangeTombstoneMarker)deserializer.readNext());
  }
  Slice.Bound sliceStart=start;
  start=null;
  if (openMarker != null)   return new RangeTombstoneBoundMarker(sliceStart,openMarker);
  return null;
}
