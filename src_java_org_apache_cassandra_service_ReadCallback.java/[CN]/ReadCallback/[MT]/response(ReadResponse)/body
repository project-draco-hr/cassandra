{
  ((RowDigestResolver)resolver).injectPreProcessed(result);
  if (logger.isDebugEnabled())   logger.debug("Preprocessed {} response",result.isDigestQuery() ? "digest" : "data");
  int n=waitingFor(result,FBUtilities.getLocalAddress()) ? received.incrementAndGet() : received.get();
  if (n >= blockfor && resolver.isDataPresent()) {
    condition.signal();
    maybeResolveForRepair();
  }
}
