{
  if (thriftDefs == null)   return new HashMap<ByteBuffer,ColumnDefinition>();
  Map<ByteBuffer,ColumnDefinition> cds=new TreeMap<ByteBuffer,ColumnDefinition>();
  for (  ColumnDef thriftColumnDef : thriftDefs)   cds.put(ByteBufferUtil.clone(thriftColumnDef.name),fromThrift(thriftColumnDef));
  return cds;
}
