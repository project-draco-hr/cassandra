{
  return new ColumnDefinition(ByteBufferUtil.clone(thriftColumnDef.name),TypeParser.parse(thriftColumnDef.validation_class),thriftColumnDef.index_type == null ? null : IndexType.valueOf(thriftColumnDef.index_type.name()),thriftColumnDef.index_options,thriftColumnDef.index_name,isSuper ? 1 : null,Type.REGULAR);
}
