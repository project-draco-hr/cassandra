{
  if (obj == this) {
    return true;
  }
 else   if (obj == null || obj.getClass() != getClass()) {
    return false;
  }
  ColumnDefinition rhs=(ColumnDefinition)obj;
  return new EqualsBuilder().append(name,rhs.name).append(validation_class,rhs.validation_class).append(index_name,rhs.index_name).append(index_type,rhs.index_type).isEquals();
}
