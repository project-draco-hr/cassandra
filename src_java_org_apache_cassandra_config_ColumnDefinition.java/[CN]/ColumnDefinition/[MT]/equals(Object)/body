{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  ColumnDefinition that=(ColumnDefinition)o;
  if (index_name != null ? !index_name.equals(that.index_name) : that.index_name != null)   return false;
  if (index_type != that.index_type)   return false;
  if (index_options != null ? !index_options.equals(that.index_options) : that.index_options != null)   return false;
  if (!name.equals(that.name))   return false;
  return !(validator != null ? !validator.equals(that.validator) : that.validator != null);
}
