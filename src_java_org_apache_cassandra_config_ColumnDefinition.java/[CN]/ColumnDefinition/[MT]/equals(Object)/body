{
  if (this == o)   return true;
  if (!(o instanceof ColumnDefinition))   return false;
  ColumnDefinition cd=(ColumnDefinition)o;
  return Objects.equal(name,cd.name) && Objects.equal(validator,cd.validator) && Objects.equal(componentIndex,cd.componentIndex)&& Objects.equal(indexName,cd.indexName)&& Objects.equal(indexType,cd.indexType)&& Objects.equal(indexOptions,cd.indexOptions);
}
