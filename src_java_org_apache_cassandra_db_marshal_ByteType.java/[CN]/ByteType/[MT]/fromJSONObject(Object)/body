{
  try {
    if (parsed instanceof String)     return new Constants.Value(fromString((String)parsed));
    Number parsedNumber=(Number)parsed;
    if (!(parsedNumber instanceof Byte))     throw new MarshalException(String.format("Expected a byte value, but got a %s: %s",parsed.getClass().getSimpleName(),parsed));
    return new Constants.Value(getSerializer().serialize(parsedNumber.byteValue()));
  }
 catch (  ClassCastException exc) {
    throw new MarshalException(String.format("Expected a byte value, but got a %s: %s",parsed.getClass().getSimpleName(),parsed));
  }
}
