def _check_child_pipes(self, current_record, pipes):
    for pipe in pipes:
        if pipe.poll():
            try:
                (record_num, error) = pipe.recv()
                self.printerr(('\n' + str(error)))
                self.printerr(('Aborting import at record #%d. Previously inserted records are still present, and some records after that may be present as well.' % (record_num,)))
                return False
            except EOFError:
                self.printerr(('\nChild process died without notification, aborting import at record #%d. Previously inserted records are probably still present, and some records after that may be present as well.' % (current_record,)))
                return False
    return True
