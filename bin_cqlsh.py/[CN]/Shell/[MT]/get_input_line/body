def get_input_line(self, prompt=''):
    if self.tty:
        self.lastcmd = raw_input(prompt)
        line = (self.lastcmd + '\n')
    else:
        self.lastcmd = self.stdin.readline()
        line = self.lastcmd
        if (not len(line)):
            raise EOFError
    self.lineno += 1
    return line
