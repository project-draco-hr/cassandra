{
  boolean supported=false;
  if (hasBound(Bound.START))   supported|=isInclusive(Bound.START) ? index.supportsOperator(Operator.GTE) : index.supportsOperator(Operator.GT);
  if (hasBound(Bound.END))   supported|=isInclusive(Bound.END) ? index.supportsOperator(Operator.LTE) : index.supportsOperator(Operator.LT);
  return supported;
}
