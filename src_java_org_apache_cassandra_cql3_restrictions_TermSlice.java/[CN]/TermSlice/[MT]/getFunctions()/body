{
  if (hasBound(Bound.START) && hasBound(Bound.END))   return Iterables.concat(bound(Bound.START).getFunctions(),bound(Bound.END).getFunctions());
 else   if (hasBound(Bound.START))   return bound(Bound.START).getFunctions();
 else   if (hasBound(Bound.END))   return bound(Bound.END).getFunctions();
 else   return Collections.emptySet();
}
