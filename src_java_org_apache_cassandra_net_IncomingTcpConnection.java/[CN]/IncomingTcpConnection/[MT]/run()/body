{
  try {
    if (version < MessagingService.VERSION_12)     throw new UnsupportedOperationException("Unable to read obsolete message version " + version + "; the earliest version supported is 1.2.0");
    receiveMessages();
  }
 catch (  EOFException e) {
    logger.trace("eof reading from socket; closing",e);
  }
catch (  UnknownColumnFamilyException e) {
    logger.warn("UnknownColumnFamilyException reading from socket; closing",e);
  }
catch (  IOException e) {
    logger.debug("IOException reading from socket; closing",e);
  }
 finally {
    close();
  }
}
