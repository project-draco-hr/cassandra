{
  try {
    if (logger.isTraceEnabled())     logger.trace("Closing socket {} - isclosed: {}",socket,socket.isClosed());
    if (!socket.isClosed()) {
      socket.close();
    }
  }
 catch (  IOException e) {
    logger.debug("Error closing socket",e);
  }
 finally {
    group.remove(this);
  }
}
