{
  if (version < MessagingService.VERSION_12)   input.readInt();
  String id=input.readUTF();
  long timestamp=version >= MessagingService.VERSION_12 ? (System.currentTimeMillis() & 0xFFFFFFFF00000000L) | (((long)input.readInt() << 2) >> 2) : System.currentTimeMillis();
  MessageIn message=MessageIn.read(input,version,id);
  if (message == null) {
    return null;
  }
  if (version <= MessagingService.current_version) {
    MessagingService.instance().receive(message,id,timestamp);
  }
 else {
    logger.debug("Received connection from newer protocol version {}. Ignoring message",version);
  }
  return message.from;
}
