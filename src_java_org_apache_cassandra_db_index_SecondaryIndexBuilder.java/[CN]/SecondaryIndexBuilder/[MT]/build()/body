{
  try {
    while (iter.hasNext()) {
      if (isStopRequested())       throw new CompactionInterruptedException(getCompactionInfo());
      DecoratedKey key=iter.next();
      Keyspace.indexPartition(key,cfs,idxNames);
    }
  }
  finally {
    try {
      iter.close();
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
}
