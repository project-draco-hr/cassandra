{
  while (iter.hasNext()) {
    if (isStopRequested())     throw new CompactionInterruptedException(getCompactionInfo());
    DecoratedKey key=iter.next();
    Keyspace.indexRow(key,cfs,idxNames);
  }
  try {
    iter.close();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
