{
  checkArgument(args.size() == 2,"Timeout type followed by value in ms (0 disables socket streaming timeout)." + " Type should be one of (" + GetTimeout.TIMEOUT_TYPES + ")");
  try {
    String type=args.get(0);
    long timeout=Long.parseLong(args.get(1));
    probe.setTimeout(type,timeout);
  }
 catch (  Exception e) {
    throw new IllegalArgumentException(e.getMessage());
  }
}
