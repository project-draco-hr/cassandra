{
  System.setProperty("storage-config","test/conf");
  for (  KSMetaData table : DatabaseDescriptor.readTablesFromXml())   DatabaseDescriptor.setTableDefinition(table,DatabaseDescriptor.getDefsVersion());
  cassandra=new EmbeddedCassandraService();
  cassandra.init();
  Thread t=new Thread(cassandra);
  t.setDaemon(true);
  t.start();
}
