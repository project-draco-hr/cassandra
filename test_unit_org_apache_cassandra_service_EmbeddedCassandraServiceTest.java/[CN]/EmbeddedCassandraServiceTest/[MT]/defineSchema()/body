{
  SchemaLoader.prepareServer();
  setup();
  SchemaLoader.createKeyspace(KEYSPACE1,SimpleStrategy.class,KSMetaData.optsWithRF(1),CFMetaData.Builder.create(KEYSPACE1,CF_STANDARD,true,false,false).addPartitionKey("pk",AsciiType.instance).addClusteringColumn("ck",AsciiType.instance).addRegularColumn("val",AsciiType.instance).build());
}
