{
  if (type instanceof LongType)   return DataType.LONG;
 else   if (type instanceof IntegerType)   return DataType.INTEGER;
 else   if (type instanceof AsciiType)   return DataType.CHARARRAY;
 else   if (type instanceof UTF8Type)   return DataType.CHARARRAY;
 else   if (type instanceof FloatType)   return DataType.FLOAT;
 else   if (type instanceof DoubleType)   return DataType.DOUBLE;
  return DataType.BYTEARRAY;
}
