{
  String names[];
  try {
    if (!location.startsWith("cassandra://"))     throw new Exception("Bad scheme.");
    String[] parts=location.split("/+");
    names=new String[]{parts[1],parts[2]};
  }
 catch (  Exception e) {
    throw new IOException("Expected 'cassandra://<keyspace>/<columnfamily>': " + e.getMessage());
  }
  return names;
}
