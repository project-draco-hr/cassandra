{
  String[] stmts=new String[]{"ALTER KEYSPACE WITH WITH DURABLE_WRITES = true","ALTER KEYSPACE ks WITH WITH DURABLE_WRITES = true","CREATE KEYSPACE WITH WITH DURABLE_WRITES = true","CREATE KEYSPACE ks WITH WITH DURABLE_WRITES = true"};
  for (  String stmt : stmts) {
    assertInvalidSyntax(stmt,"no viable alternative at input 'WITH'");
  }
}
