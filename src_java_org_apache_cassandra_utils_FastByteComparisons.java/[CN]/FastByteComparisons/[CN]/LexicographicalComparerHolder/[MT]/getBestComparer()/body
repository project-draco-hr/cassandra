{
  String arch=System.getProperty("os.arch");
  boolean unaligned=arch.equals("i386") || arch.equals("x86") || arch.equals("amd64")|| arch.equals("x86_64");
  if (!unaligned)   return lexicographicalComparerJavaImpl();
  try {
    Class<?> theClass=Class.forName(UNSAFE_COMPARER_NAME);
    @SuppressWarnings("unchecked") Comparer<byte[]> comparer=(Comparer<byte[]>)theClass.getEnumConstants()[0];
    return comparer;
  }
 catch (  Throwable t) {
    return lexicographicalComparerJavaImpl();
  }
}
