{
  int cellCount=0;
  for (  SSTableReader sstable : cfs.getSSTables()) {
    Iterator<OnDiskAtomIterator> it=sstable.getScanner();
    while (it.hasNext()) {
      Iterator<OnDiskAtom> itr=it.next();
      while (itr.hasNext()) {
        ++cellCount;
        itr.next();
      }
    }
  }
  return cellCount;
}
