{
  ArrayList<StreamSession.SSTableStreamingSections> details=new ArrayList<>();
  for (  SSTableReader sstable : sstables) {
    details.add(new StreamSession.SSTableStreamingSections(sstable,sstable.getPositionsForRanges(ranges),sstable.estimatedKeysForRanges(ranges),sstable.getSSTableMetadata().repairedAt));
  }
  return details;
}
