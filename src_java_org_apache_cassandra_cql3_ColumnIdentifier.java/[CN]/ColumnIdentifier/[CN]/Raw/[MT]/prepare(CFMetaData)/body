{
  AbstractType<?> comparator=cfm.comparator.asAbstractType();
  if (cfm.getIsDense() || comparator instanceof CompositeType || comparator instanceof UTF8Type)   return new ColumnIdentifier(text,true);
  ByteBuffer bufferName=ByteBufferUtil.bytes(text);
  for (  ColumnDefinition def : cfm.partitionKeyColumns()) {
    if (def.name.bytes.equals(bufferName))     return new ColumnIdentifier(text,true);
  }
  return new ColumnIdentifier(comparator.fromString(rawText),text);
}
