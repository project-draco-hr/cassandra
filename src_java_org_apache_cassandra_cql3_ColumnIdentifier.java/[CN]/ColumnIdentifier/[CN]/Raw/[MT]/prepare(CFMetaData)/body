{
  if (cfm.getIsDense() || cfm.comparator instanceof CompositeType || cfm.comparator instanceof UTF8Type)   return new ColumnIdentifier(text,true);
  ByteBuffer bufferName=ByteBufferUtil.bytes(text);
  for (  ColumnDefinition def : cfm.partitionKeyColumns()) {
    if (def.name.equals(bufferName))     return new ColumnIdentifier(text,true);
  }
  return new ColumnIdentifier(cfm.comparator.fromString(rawText),text);
}
