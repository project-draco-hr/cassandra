{
  if (!cfm.isStaticCompactTable())   return getInterned(text,true);
  AbstractType<?> thriftColumnNameType=cfm.thriftColumnNameType();
  if (thriftColumnNameType instanceof UTF8Type)   return getInterned(text,true);
  ByteBuffer bufferName=ByteBufferUtil.bytes(text);
  for (  ColumnDefinition def : cfm.allColumns()) {
    if (def.name.bytes.equals(bufferName))     return def.name;
  }
  return getInterned(thriftColumnNameType.fromString(rawText),text);
}
