{
  try {
    if (Schema.instance.getKSMetaData(AuthKeyspace.NAME) == null)     maybeAddKeyspace(AuthKeyspace.metadata());
  }
 catch (  Exception e) {
    throw new AssertionError(e);
  }
  for (  CFMetaData table : AuthKeyspace.metadata().tables)   if (Schema.instance.getCF(table.cfId) == null)   maybeAddTable(table);
  DatabaseDescriptor.getRoleManager().setup();
  DatabaseDescriptor.getAuthenticator().setup();
  DatabaseDescriptor.getAuthorizer().setup();
  MigrationManager.instance.register(new AuthMigrationListener());
}
