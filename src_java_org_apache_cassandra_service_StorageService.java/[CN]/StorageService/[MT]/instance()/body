{
  if (instance_ == null) {
    StorageService.createLock_.lock();
    try {
      if (instance_ == null) {
        try {
          instance_=new StorageService();
        }
 catch (        Throwable th) {
          logger_.error(LogUtil.throwableToString(th));
          System.exit(1);
        }
      }
    }
  finally {
      createLock_.unlock();
    }
  }
  return instance_;
}
