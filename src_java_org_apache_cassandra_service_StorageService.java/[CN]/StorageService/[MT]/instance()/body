{
  if (instance_ == null) {
synchronized (StorageService.class) {
      if (instance_ == null) {
        try {
          instance_=new StorageService();
        }
 catch (        Throwable th) {
          logger_.error(LogUtil.throwableToString(th));
          System.exit(1);
        }
      }
    }
  }
  return instance_;
}
