{
  if (table == null)   bootstrapSet.removeAll(s);
 else   bootstrapSet.remove(s,table);
  if (logger_.isDebugEnabled())   logger_.debug("Removed " + s + " as a bootstrap source; remaining is ["+ StringUtils.join(bootstrapSet.keySet(),", ")+ "]");
  if (bootstrapSet.isEmpty()) {
    finishBootstrapping();
  }
}
