{
  if (table == null)   bootstrapSet.removeAll(s);
 else   bootstrapSet.remove(s,table);
  if (logger_.isDebugEnabled())   logger_.debug(String.format("Removed %s/%s as a bootstrap source; remaining is [%s]",s,table == null ? "<ALL>" : table,StringUtils.join(bootstrapSet.keySet(),", ")));
  if (bootstrapSet.isEmpty()) {
    finishBootstrapping();
  }
}
