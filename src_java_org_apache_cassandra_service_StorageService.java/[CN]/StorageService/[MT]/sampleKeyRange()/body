{
  List<DecoratedKey> keys=new ArrayList<DecoratedKey>();
  for (  Table keyspace : Table.nonSystem()) {
    for (    Range<Token> range : getPrimaryRangesForEndpoint(keyspace.name,FBUtilities.getBroadcastAddress()))     keys.addAll(keySamples(keyspace.getColumnFamilyStores(),range));
  }
  List<String> sampledKeys=new ArrayList<String>(keys.size());
  for (  DecoratedKey key : keys)   sampledKeys.add(key.getToken().toString());
  return sampledKeys;
}
