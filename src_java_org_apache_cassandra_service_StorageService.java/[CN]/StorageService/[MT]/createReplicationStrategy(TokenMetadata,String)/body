{
  AbstractReplicationStrategy replicationStrategy;
  KSMetaData ksm=DatabaseDescriptor.getKSMetaData(table);
  try {
    replicationStrategy=AbstractReplicationStrategy.createReplicationStrategy(table,ksm.strategyClass,tokenMetadata,DatabaseDescriptor.getEndpointSnitch(),ksm.strategyOptions);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return replicationStrategy;
}
