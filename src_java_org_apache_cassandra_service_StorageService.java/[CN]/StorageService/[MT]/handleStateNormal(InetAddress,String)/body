{
  Token token=getPartitioner().getTokenFactory().fromString(moveValue);
  if (logger_.isDebugEnabled())   logger_.debug("Node " + endpoint + " state normal, token "+ token);
  if (tokenMetadata_.isMember(endpoint))   logger_.info("Node " + endpoint + " state jump to normal");
  InetAddress currentNode=tokenMetadata_.getEndpoint(token);
  if (currentNode == null || (FBUtilities.getLocalAddress().equals(currentNode) && Gossiper.instance.compareEndpointStartup(endpoint,currentNode) > 0))   tokenMetadata_.updateNormalToken(token,endpoint);
 else   logger_.info("Will not change my token ownership to " + endpoint);
  calculatePendingRanges();
  if (!isClientMode)   SystemTable.updateToken(endpoint,token);
}
