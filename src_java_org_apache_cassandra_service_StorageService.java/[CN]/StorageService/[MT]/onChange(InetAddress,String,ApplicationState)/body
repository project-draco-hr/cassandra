{
  if (!MOVE_STATE.equals(apStateName))   return;
  String apStateValue=apState.getValue();
  String[] pieces=apStateValue.split(DelimiterStr,-1);
  assert(pieces.length > 0);
  String moveName=pieces[0];
  if (moveName.equals(STATE_BOOTSTRAPPING))   handleStateBootstrap(endpoint,pieces);
 else   if (moveName.equals(STATE_NORMAL))   handleStateNormal(endpoint,pieces);
 else   if (moveName.equals(STATE_LEAVING))   handleStateLeaving(endpoint,pieces);
 else   if (moveName.equals(STATE_LEFT))   handleStateLeft(endpoint,pieces);
}
