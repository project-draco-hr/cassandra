{
  try {
    LOG.info("Shutting down cluster...");
    if (service != null)     service.destroyCluster(clusterSpec);
    if (tarball != null)     BlobUtils.deleteBlob(config,clusterSpec,tarball);
    running=false;
  }
 catch (  Exception e) {
    LOG.error(String.format("Error shutting down cluster: %s",e));
  }
}
