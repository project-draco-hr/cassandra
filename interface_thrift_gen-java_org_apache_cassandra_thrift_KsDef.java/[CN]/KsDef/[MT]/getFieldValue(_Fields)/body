{
switch (field) {
case NAME:
    return getName();
case STRATEGY_CLASS:
  return getStrategy_class();
case REPLICATION_FACTOR:
return new Integer(getReplication_factor());
case CF_DEFS:
return getCf_defs();
case USERS_ACCESS:
return getUsers_access();
case GROUPS_ACCESS:
return getGroups_access();
}
throw new IllegalStateException();
}
