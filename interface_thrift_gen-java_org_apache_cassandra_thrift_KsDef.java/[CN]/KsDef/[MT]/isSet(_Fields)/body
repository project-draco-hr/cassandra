{
  if (field == null) {
    throw new IllegalArgumentException();
  }
switch (field) {
case NAME:
    return isSetName();
case STRATEGY_CLASS:
  return isSetStrategy_class();
case STRATEGY_OPTIONS:
return isSetStrategy_options();
case REPLICATION_FACTOR:
return isSetReplication_factor();
case CF_DEFS:
return isSetCf_defs();
}
throw new IllegalStateException();
}
