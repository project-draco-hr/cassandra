{
  if (components.length > size())   throw new IllegalArgumentException("Too many components, max is " + size());
  CBuilder builder=builder();
  for (int i=0; i < components.length; i++) {
    Object obj=components[i];
    if (obj instanceof ByteBuffer)     builder.add((ByteBuffer)obj);
 else     builder.add(obj);
  }
  return builder.build();
}
