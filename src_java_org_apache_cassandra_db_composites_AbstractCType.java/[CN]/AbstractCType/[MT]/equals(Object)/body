{
  if (this == o)   return true;
  if (o == null)   return false;
  if (!getClass().equals(o.getClass()))   return false;
  CType c=(CType)o;
  if (size() != c.size())   return false;
  for (int i=0; i < size(); i++) {
    if (!subtype(i).equals(c.subtype(i)))     return false;
  }
  return true;
}
