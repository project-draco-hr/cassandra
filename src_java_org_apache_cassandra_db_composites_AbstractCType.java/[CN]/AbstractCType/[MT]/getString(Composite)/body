{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < c.size(); i++) {
    if (i > 0)     sb.append(":");
    sb.append(AbstractCompositeType.escape(subtype(i).getString(c.get(i))));
  }
switch (c.eoc()) {
case START:
    sb.append(":_");
  break;
case END:
sb.append(":!");
break;
}
return sb.toString();
}
