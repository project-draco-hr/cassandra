{
  return new FilteringRow(){
    @Override protected boolean include(    LivenessInfo info){
      return !info.hasLocalDeletionTime() || !info.isPurgeable(getMaxPurgeableTimestamp(),gcBefore);
    }
    @Override protected boolean include(    DeletionTime dt){
      return includeDelTime(dt);
    }
    @Override protected boolean include(    ColumnDefinition c,    DeletionTime dt){
      return includeDelTime(dt);
    }
  }
;
}
