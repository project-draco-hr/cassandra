{
  AbstractType<?> comparator=metadata.comparator;
  List<IndexHelper.IndexInfo> ranges=new ArrayList<IndexHelper.IndexInfo>();
  int lastIndexIdx=-1;
  for (  ByteBuffer name : columns) {
    int index=IndexedSliceReader.indexFor(sstable,name,indexList,comparator,false,lastIndexIdx);
    if (index < 0 || index == indexList.size())     continue;
    IndexHelper.IndexInfo indexInfo=indexList.get(index);
    if (IndexedSliceReader.comparatorForIndex(sstable,comparator).compare(IndexedSliceReader.forIndexComparison(sstable,name),indexInfo.firstName) < 0 || index == lastIndexIdx)     continue;
    ranges.add(indexInfo);
    lastIndexIdx=index;
  }
  if (ranges.isEmpty())   return;
  for (  IndexHelper.IndexInfo indexInfo : ranges) {
    long positionToSeek=basePosition + indexInfo.offset;
    if (file == null)     file=createFileDataInput(positionToSeek);
    Iterator<OnDiskAtom> atomIterator=cf.metadata().getOnDiskIterator(file,Integer.MAX_VALUE,sstable.descriptor.version);
    file.seek(positionToSeek);
    FileMark mark=file.mark();
    while (file.bytesPastMark(mark) < indexInfo.width) {
      OnDiskAtom column=atomIterator.next();
      if (!(column instanceof Column) || columnNames.contains(column.name()))       result.add(column);
    }
  }
}
