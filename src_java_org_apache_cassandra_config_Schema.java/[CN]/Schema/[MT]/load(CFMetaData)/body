{
  Pair<String,String> key=new Pair<String,String>(cfm.ksName,cfm.cfName);
  if (cfIdMap.containsKey(key))   throw new RuntimeException(String.format("Attempting to load already loaded column family %s.%s",cfm.ksName,cfm.cfName));
  logger.debug("Adding {} to cfIdMap",cfm);
  cfIdMap.put(key,cfm.cfId);
  fixCFMaxId();
}
