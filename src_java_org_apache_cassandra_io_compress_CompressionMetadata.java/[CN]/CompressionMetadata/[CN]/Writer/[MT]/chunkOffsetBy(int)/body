{
  if (dataLengthOffset == -1)   throw new IllegalStateException("writeHeader wasn't called");
  try {
    long position=getFilePointer();
    seek(dataLengthOffset + 8 + 4+ (chunkIndex * 8L));
    try {
      return readLong();
    }
  finally {
      seek(position);
    }
  }
 catch (  IOException e) {
    throw new FSReadError(e,filePath);
  }
}
