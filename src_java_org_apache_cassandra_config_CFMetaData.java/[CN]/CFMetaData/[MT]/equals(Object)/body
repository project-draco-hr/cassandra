{
  if (!(obj instanceof CFMetaData))   return false;
  CFMetaData other=(CFMetaData)obj;
  return other.tableName.equals(tableName) && other.cfName.equals(cfName) && other.columnType.equals(columnType)&& other.comparator.equals(comparator)&& ObjectUtils.equals(other.subcolumnComparator,subcolumnComparator)&& ObjectUtils.equals(other.comment,comment)&& other.rowCacheSize == rowCacheSize && other.keyCacheSize == keyCacheSize && other.readRepairChance == readRepairChance && other.cfId == cfId;
}
