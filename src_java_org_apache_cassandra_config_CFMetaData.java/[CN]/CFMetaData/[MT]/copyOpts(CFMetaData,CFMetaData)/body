{
  return newCFMD.comment(oldCFMD.comment).readRepairChance(oldCFMD.readRepairChance).dcLocalReadRepairChance(oldCFMD.dcLocalReadRepairChance).gcGraceSeconds(oldCFMD.gcGraceSeconds).minCompactionThreshold(oldCFMD.minCompactionThreshold).maxCompactionThreshold(oldCFMD.maxCompactionThreshold).compactionStrategyClass(oldCFMD.compactionStrategyClass).compactionStrategyOptions(new HashMap<>(oldCFMD.compactionStrategyOptions)).compressionParameters(oldCFMD.compressionParameters.copy()).bloomFilterFpChance(oldCFMD.getBloomFilterFpChance()).caching(oldCFMD.caching).defaultTimeToLive(oldCFMD.defaultTimeToLive).minIndexInterval(oldCFMD.minIndexInterval).maxIndexInterval(oldCFMD.maxIndexInterval).speculativeRetry(oldCFMD.speculativeRetry).memtableFlushPeriod(oldCFMD.memtableFlushPeriod).droppedColumns(new HashMap<>(oldCFMD.droppedColumns)).triggers(new HashMap<>(oldCFMD.triggers));
}
