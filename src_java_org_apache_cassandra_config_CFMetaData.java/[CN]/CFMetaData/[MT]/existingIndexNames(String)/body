{
  Set<String> indexNames=new HashSet<String>();
  for (  ColumnFamilyStore cfs : ColumnFamilyStore.all()) {
    if (cfToExclude == null || !cfs.name.equals(cfToExclude))     for (    ColumnDefinition cd : cfs.metadata.allColumns())     indexNames.add(cd.getIndexName());
  }
  return indexNames;
}
