{
  Set<String> indexNames=new HashSet<String>();
  for (  ColumnFamilyStore cfs : ColumnFamilyStore.all()) {
    if (cfToExclude == null || !cfs.getColumnFamilyName().equals(cfToExclude))     for (    ColumnDefinition cd : cfs.metadata.getColumn_metadata().values())     indexNames.add(cd.getIndexName());
  }
  return indexNames;
}
