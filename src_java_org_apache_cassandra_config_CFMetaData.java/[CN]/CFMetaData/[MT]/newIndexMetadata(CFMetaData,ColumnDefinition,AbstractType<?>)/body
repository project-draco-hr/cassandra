{
  return new CFMetaData(parent.ksName,parent.indexColumnFamilyName(info),ColumnFamilyType.Standard,columnComparator,null).keyValidator(info.getValidator()).readRepairChance(0.0).dclocalReadRepairChance(0.0).gcGraceSeconds(parent.gcGraceSeconds).minCompactionThreshold(parent.minCompactionThreshold).maxCompactionThreshold(parent.maxCompactionThreshold);
}
