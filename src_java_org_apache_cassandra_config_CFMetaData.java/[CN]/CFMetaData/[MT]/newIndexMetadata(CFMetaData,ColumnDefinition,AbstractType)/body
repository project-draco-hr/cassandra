{
  return new CFMetaData(parent.tableName,indexName(parent.cfName,info),ColumnFamilyType.Standard,columnComparator,null,"",0,0,0,parent.gcGraceSeconds,BytesType.instance,parent.minCompactionThreshold,parent.maxCompactionThreshold,0,0,parent.memtableFlushAfterMins,parent.memtableThroughputInMb,parent.memtableOperationsInMillions,Collections.<ByteBuffer,ColumnDefinition>emptyMap());
