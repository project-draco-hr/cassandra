{
  return new CFMetaData(parent.ksName,parent.indexColumnFamilyName(info),ColumnFamilyType.Standard,columnComparator,null).keyValidator(info.getValidator()).keyCacheSize(0.0).readRepairChance(0.0).gcGraceSeconds(0).minCompactionThreshold(parent.minCompactionThreshold).maxCompactionThreshold(parent.maxCompactionThreshold);
}
