{
  return new CFMetaData(parent.ksName,indexName(parent.cfName,info),ColumnFamilyType.Standard,columnComparator,null).keyCacheSize(0.0).readRepairChance(0.0).gcGraceSeconds(parent.gcGraceSeconds).minCompactionThreshold(parent.minCompactionThreshold).maxCompactionThreshold(parent.maxCompactionThreshold).memTime(parent.memtableFlushAfterMins).memSize(parent.memtableThroughputInMb).memOps(parent.memtableOperationsInMillions);
}
