  if (cf_def.key_cache_save_period_in_seconds == null)   cf_def.key_cache_save_period_in_seconds=CFMetaData.DEFAULT_KEY_CACHE_SAVE_PERIOD_IN_SECONDS;
  if (cf_def.memtable_flush_after_mins == null)   cf_def.memtable_flush_after_mins=CFMetaData.DEFAULT_MEMTABLE_LIFETIME_IN_MINS;
