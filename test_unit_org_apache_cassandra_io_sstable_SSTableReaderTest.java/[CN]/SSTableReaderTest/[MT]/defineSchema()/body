{
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE1,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD2),SchemaLoader.indexCFMD(KEYSPACE1,CF_INDEXED,true),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARDLOWINDEXINTERVAL).minIndexInterval(8).maxIndexInterval(256).caching(CachingOptions.NONE));
}
