{
  List<Memtable> memtables=new ArrayList<>();
  List<SSTableReader> sstables=new ArrayList<>();
  for (int i=0; i < memtableCount; i++)   memtables.add(MockSchema.memtable());
  for (int i=0; i < sstableCount; i++)   sstables.add(MockSchema.sstable(i));
  return new View(ImmutableList.copyOf(memtables),Collections.<Memtable>emptyList(),Helpers.identityMap(sstables),Collections.<SSTableReader>emptySet(),SSTableIntervalTree.build(sstables));
}
