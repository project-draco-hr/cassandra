{
  try {
    List<ByteBuffer> valueList=new ArrayList<>();
    for (    String v : values) {
      valueList.add(ByteBufferUtil.bytes(v));
    }
    QueryProcessor.process(fmtQry,ConsistencyLevel.ONE,valueList);
  }
 catch (  Throwable t) {
    logger.error("Error executing query " + fmtQry,t);
  }
}
