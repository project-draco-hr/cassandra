{
  if (logger.isDebugEnabled())   logger.debug("batch_insert");
  byte[] keyBytes=key.getBytes();
  String keyspaceString=keyspace.toString();
  AvroValidation.validateKey(key.toString());
  for (  Utf8 cfName : cfmap.keySet()) {
    for (    ColumnOrSuperColumn cosc : cfmap.get(cfName))     AvroValidation.validateColumnOrSuperColumn(keyspaceString,cfName.toString(),cosc);
  }
  doInsert(consistency,getRowMutation(keyspaceString,keyBytes,cfmap));
  return null;
}
