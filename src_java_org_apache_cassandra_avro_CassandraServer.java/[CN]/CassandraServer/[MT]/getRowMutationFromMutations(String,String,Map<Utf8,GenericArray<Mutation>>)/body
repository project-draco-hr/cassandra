{
  RowMutation rm=new RowMutation(keyspace,key.trim().getBytes(UTF8));
  for (  Map.Entry<Utf8,GenericArray<Mutation>> entry : cfMap.entrySet()) {
    String cfName=entry.getKey().toString();
    for (    Mutation mutation : entry.getValue()) {
      if (mutation.deletion != null)       deleteColumnOrSuperColumnToRowMutation(rm,cfName,mutation.deletion);
 else       addColumnOrSuperColumnToRowMutation(rm,cfName,mutation.column_or_supercolumn);
    }
  }
  return rm;
}
