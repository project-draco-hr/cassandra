{
  if (cosc.column == null) {
    for (    Column column : cosc.super_column.columns)     rm.add(new QueryPath(cfName,cosc.super_column.name.array(),column.name.array()),column.value.array(),column.timestamp);
  }
 else {
    rm.add(new QueryPath(cfName,null,cosc.column.name.array()),cosc.column.value.array(),cosc.column.timestamp);
  }
}
