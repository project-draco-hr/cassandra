{
  for (int i=0; i < client.getNumKeys(); i++) {
    if (stop)     break;
    try {
      operations.put(createOperation(i % client.getNumDifferentKeys()));
    }
 catch (    InterruptedException e) {
      if (e.getMessage() != null)       System.err.println("Producer error - " + e.getMessage());
      return;
    }
  }
}
