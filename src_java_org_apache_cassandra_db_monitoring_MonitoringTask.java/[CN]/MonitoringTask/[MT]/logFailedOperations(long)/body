{
  FailedOperations failedOperations=aggregateFailedOperations();
  if (!failedOperations.isEmpty()) {
    long elapsed=now - lastLogTime;
    logger.warn("{} operations timed out in the last {} msecs, operation list available at debug log level",failedOperations.num(),elapsed);
    if (logger.isDebugEnabled())     logger.debug("{} operations timed out in the last {} msecs:{}{}",failedOperations.num(),elapsed,LINE_SEPARATOR,failedOperations.getLogMessage());
  }
  lastLogTime=now;
}
