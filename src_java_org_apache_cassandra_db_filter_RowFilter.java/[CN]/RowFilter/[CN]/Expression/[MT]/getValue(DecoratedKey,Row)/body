{
switch (column.kind) {
case PARTITION_KEY:
    return column.isOnAllComponents() ? partitionKey.getKey() : CompositeType.extractComponent(partitionKey.getKey(),column.position());
case CLUSTERING:
  return row.clustering().get(column.position());
default :
Cell cell=row.getCell(column);
return cell == null ? null : cell.value();
}
}
