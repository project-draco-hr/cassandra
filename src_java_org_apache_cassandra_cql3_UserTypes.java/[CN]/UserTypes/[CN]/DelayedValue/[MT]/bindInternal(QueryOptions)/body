{
  int version=options.getProtocolVersion();
  ByteBuffer[] buffers=new ByteBuffer[values.size()];
  for (int i=0; i < type.size(); i++) {
    buffers[i]=values.get(i).bindAndGet(options);
    if (version < 3 && type.fieldType(i).isCollection() && buffers[i] != null)     buffers[i]=((CollectionType)type.fieldType(i)).getSerializer().reserializeToV3(buffers[i]);
  }
  return buffers;
}
