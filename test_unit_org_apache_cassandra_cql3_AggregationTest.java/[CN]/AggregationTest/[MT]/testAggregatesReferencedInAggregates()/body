{
  String fState=createFunction(KEYSPACE,"int, int","CREATE FUNCTION %s(a int, b int) " + "RETURNS int " + "LANGUAGE javascript "+ "AS 'a + b;'");
  String a=createAggregate(KEYSPACE,"int, int","CREATE AGGREGATE %s(int) " + "SFUNC " + shortFunctionName(fState) + " "+ "STYPE int ");
  assertInvalid("CREATE AGGREGATE " + KEYSPACE + ".aggInv(int) "+ "SFUNC "+ shortFunctionName(a)+ " "+ "STYPE int ");
  assertInvalid("CREATE AGGREGATE " + KEYSPACE + ".aggInv(int) "+ "SFUNC "+ shortFunctionName(fState)+ " "+ "STYPE int "+ "FINALFUNC "+ shortFunctionName(a));
}
