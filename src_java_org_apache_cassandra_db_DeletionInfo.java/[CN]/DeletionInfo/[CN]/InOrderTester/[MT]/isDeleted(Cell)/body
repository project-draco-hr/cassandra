{
  if (cell.timestamp() <= topLevel.markedForDeleteAt)   return true;
  if (!topLevel.isLive() && cell instanceof CounterCell)   return true;
  if (reversed)   return DeletionInfo.this.isDeleted(cell);
  if (tester == null && ranges != null)   tester=ranges.inOrderTester();
  return tester != null && tester.isDeleted(cell);
}
