{
  lookup.remove(desc);
  if (readMeterSyncFuture != null)   readMeterSyncFuture.cancel(true);
  if (isCompacted.get())   SystemKeyspace.clearSSTableReadMeter(desc.ksname,desc.cfname,desc.generation);
  CLibrary.trySkipCache(desc.filenameFor(Component.DATA),0,0);
  CLibrary.trySkipCache(desc.filenameFor(Component.PRIMARY_INDEX),0,0);
}
