{
  SSTableMetadata sstableMetadata=openMetadata(descriptor,components,partitioner);
  SSTableReader sstable=new SSTableReader(descriptor,components,metadata,partitioner,System.currentTimeMillis(),sstableMetadata);
  sstable.load(false,false);
  sstable.bf=FilterFactory.AlwaysPresent;
  return sstable;
}
