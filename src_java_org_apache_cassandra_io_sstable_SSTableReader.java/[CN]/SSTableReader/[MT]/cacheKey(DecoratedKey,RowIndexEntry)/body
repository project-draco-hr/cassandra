{
  CFMetaData.Caching caching=metadata.getCaching();
  if (caching == CFMetaData.Caching.NONE || caching == CFMetaData.Caching.ROWS_ONLY || keyCache == null || keyCache.getCapacity() == 0) {
    return;
  }
  KeyCacheKey cacheKey=new KeyCacheKey(metadata.cfId,descriptor,key.key);
  logger.trace("Adding cache entry for {} -> {}",cacheKey,info);
  keyCache.put(cacheKey,info);
}
