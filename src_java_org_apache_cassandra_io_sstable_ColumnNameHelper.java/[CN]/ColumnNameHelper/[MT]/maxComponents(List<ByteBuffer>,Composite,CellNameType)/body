{
  int size=Math.min(candidate.size(),comparator.clusteringPrefixSize());
  if (maxSeen.isEmpty())   return getComponents(candidate,size);
  maxSeen=maybeGrow(maxSeen,size);
  for (int i=0; i < size; i++)   maxSeen.set(i,max(maxSeen.get(i),candidate.get(i),comparator.subtype(i)));
  return maxSeen;
}
