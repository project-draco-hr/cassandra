{
  int size=Math.min(candidate.size(),comparator.clusteringPrefixSize());
  if (minSeen.isEmpty())   return getComponents(candidate,size);
  minSeen=maybeGrow(minSeen,size);
  for (int i=0; i < size; i++)   minSeen.set(i,min(minSeen.get(i),candidate.get(i),comparator.subtype(i)));
  return minSeen;
}
