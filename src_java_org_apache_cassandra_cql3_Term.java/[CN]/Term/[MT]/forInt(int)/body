{
  if (type == CqlParser.STRING_LITERAL)   return STRING;
 else   if (type == CqlParser.INTEGER)   return INTEGER;
 else   if (type == CqlParser.UUID)   return UUID;
 else   if (type == CqlParser.FLOAT)   return FLOAT;
 else   if (type == CqlParser.BOOLEAN)   return BOOLEAN;
 else   if (type == CqlParser.HEXNUMBER)   return HEX;
 else   if (type == CqlParser.QMARK)   return QMARK;
  throw new AssertionError();
}
