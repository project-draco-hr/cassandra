{
  if (o1.remaining() == 0) {
    return o2.remaining() == 0 ? 0 : -1;
  }
  if (o2.remaining() == 0) {
    return 1;
  }
  int diff=o1.array()[o1.position() + o1.arrayOffset()] - o2.array()[o2.position() + o2.arrayOffset()];
  if (diff != 0)   return diff;
  return ByteBufferUtil.compare(o1,o2);
}
