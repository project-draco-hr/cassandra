{
  if (exhausted) {
    throw new IndexOutOfBoundsException();
  }
  bufOut.reset();
  if (reader.isEOF()) {
    reader.close();
    exhausted=true;
    return;
  }
  long bytesread=reader.next(bufOut);
  if (bytesread == -1) {
    reader.close();
    exhausted=true;
    return;
  }
  bufIn.reset(bufOut.getData(),bufOut.getLength());
  key=bufIn.readUTF();
  if (key.equals(SSTable.blockIndexKey_)) {
    reader.close();
    exhausted=true;
  }
}
