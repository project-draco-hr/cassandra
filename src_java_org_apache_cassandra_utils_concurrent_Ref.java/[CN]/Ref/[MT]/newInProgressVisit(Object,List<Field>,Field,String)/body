{
  Preconditions.checkNotNull(o);
  InProgressVisit ipv=inProgressVisitPool.pollLast();
  if (ipv == null)   ipv=new InProgressVisit();
  ipv.o=o;
  if (o instanceof Object[])   ipv.collectionIterator=Arrays.asList((Object[])o).iterator();
 else   if (o instanceof ConcurrentMap) {
    ipv.isMapIterator=true;
    ipv.collectionIterator=((Map)o).entrySet().iterator();
  }
 else   if (concurrentIterables.contains(o.getClass()) | o instanceof BlockingQueue)   ipv.collectionIterator=((Iterable)o).iterator();
  ipv.fields=fields;
  ipv.field=field;
  ipv.name=name;
  return ipv;
}
