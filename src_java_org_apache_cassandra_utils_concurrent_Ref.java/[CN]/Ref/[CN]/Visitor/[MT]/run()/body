{
  try {
    for (    GlobalState globalState : globallyExtant) {
      if (globalState.tidy == null)       continue;
      path.clear();
      visited.clear();
      visited.add(globalState);
      visiting=globalState;
      visit(globalState.tidy);
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
 finally {
    path.clear();
    visited.clear();
  }
}
