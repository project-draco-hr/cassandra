{
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE_CACHED,KeyspaceParams.simple(1),SchemaLoader.standardCFMD(KEYSPACE_CACHED,CF_CACHED).caching(CachingParams.CACHE_EVERYTHING),SchemaLoader.standardCFMD(KEYSPACE_CACHED,CF_CACHEDINT,1,IntegerType.instance).caching(new CachingParams(true,100)));
}
