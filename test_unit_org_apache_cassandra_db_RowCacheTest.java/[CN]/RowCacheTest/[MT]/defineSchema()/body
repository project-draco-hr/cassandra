{
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE_CACHED,KeyspaceParams.simple(1),SchemaLoader.standardCFMD(KEYSPACE_CACHED,CF_CACHED).caching(CachingOptions.ALL),SchemaLoader.standardCFMD(KEYSPACE_CACHED,CF_CACHEDINT,1,IntegerType.instance).caching(new CachingOptions(new CachingOptions.KeyCache(CachingOptions.KeyCache.Type.ALL),new CachingOptions.RowCache(CachingOptions.RowCache.Type.HEAD,100))));
}
