{
  if (current >= composite.types.size())   throw new IllegalStateException("Composite column is already fully constructed");
  AbstractType currentType=composite.types.get(current++);
  ByteBuffer buffer=t.getByteBuffer(currentType,variables);
  ByteBufferUtil.writeWithShortLength(buffer,out);
switch (op) {
case LT:
    out.write((byte)-1);
  break;
case GT:
case LTE:
out.write((byte)1);
break;
default :
out.write((byte)0);
break;
}
return this;
}
