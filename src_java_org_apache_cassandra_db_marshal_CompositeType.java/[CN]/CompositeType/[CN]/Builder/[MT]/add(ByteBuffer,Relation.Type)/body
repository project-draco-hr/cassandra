{
  if (components.size() >= composite.types.size())   throw new IllegalStateException("Composite column is already fully constructed");
  int current=components.size();
  components.add(buffer);
switch (op) {
case LT:
    endOfComponents[current]=(byte)-1;
  break;
case GT:
case LTE:
endOfComponents[current]=(byte)1;
break;
default :
endOfComponents[current]=(byte)0;
break;
}
return this;
}
