{
  if (column.type.isMultiCell()) {
    if (value == null)     return;
    for (    ByteBuffer buffer : ((Value)value).elements) {
      ByteBuffer uuid=ByteBuffer.wrap(UUIDGen.getTimeUUIDBytes());
      params.addCell(clustering,column,writer,CellPath.create(uuid),buffer);
    }
  }
 else {
    if (value == null)     params.addTombstone(column,writer);
 else     params.addCell(clustering,column,writer,value.get(Server.CURRENT_VERSION));
  }
}
