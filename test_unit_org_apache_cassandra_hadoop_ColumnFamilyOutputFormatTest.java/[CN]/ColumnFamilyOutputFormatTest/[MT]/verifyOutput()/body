{
  List<KeySlice> keySlices=thriftClient.get_range_slices(new ColumnParent().setColumn_family(COLUMN_FAMILY),new SlicePredicate().setColumn_names(new ArrayList<byte[]>()),new KeyRange().setStart_key("".getBytes()).setEnd_key("".getBytes()),ConsistencyLevel.ONE);
  for (  KeySlice keySlice : keySlices) {
    Integer key=Integer.parseInt(new String(keySlice.getKey()));
    List<Integer> columnValues=new ArrayList<Integer>();
    for (    ColumnOrSuperColumn cosc : keySlice.getColumns())     columnValues.add(Integer.parseInt(new String(cosc.getColumn().getValue())));
    verifyKeyValues(key,columnValues);
  }
}
