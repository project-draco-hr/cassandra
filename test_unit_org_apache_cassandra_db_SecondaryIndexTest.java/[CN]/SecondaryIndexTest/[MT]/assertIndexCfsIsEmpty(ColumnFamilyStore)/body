{
  PartitionRangeReadCommand command=(PartitionRangeReadCommand)Util.cmd(indexCfs).build();
  try (ReadOrderGroup orderGroup=command.startOrderGroup();PartitionIterator iter=UnfilteredPartitionIterators.filter(Util.executeLocally(command,indexCfs,orderGroup),FBUtilities.nowInSeconds())){
    assertFalse(iter.hasNext());
  }
 }
