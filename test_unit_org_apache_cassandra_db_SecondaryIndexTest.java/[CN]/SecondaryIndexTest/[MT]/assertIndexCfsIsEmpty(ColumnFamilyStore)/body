{
  PartitionRangeReadCommand command=(PartitionRangeReadCommand)Util.cmd(indexCfs).build();
  try (ReadExecutionController controller=command.executionController();PartitionIterator iter=UnfilteredPartitionIterators.filter(Util.executeLocally(command,indexCfs,controller),FBUtilities.nowInSeconds())){
    assertFalse(iter.hasNext());
  }
 }
