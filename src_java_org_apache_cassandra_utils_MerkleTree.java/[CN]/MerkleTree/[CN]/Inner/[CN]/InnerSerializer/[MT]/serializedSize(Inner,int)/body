{
  int size=inner.hash == null ? DBTypeSizes.NATIVE.sizeof(-1) : DBTypeSizes.NATIVE.sizeof(inner.hash().length) + inner.hash().length;
  size+=Token.serializer.serializedSize(inner.token,DBTypeSizes.NATIVE) + Hashable.serializer.serializedSize(inner.lchild,version) + Hashable.serializer.serializedSize(inner.rchild,version);
  return size;
}
