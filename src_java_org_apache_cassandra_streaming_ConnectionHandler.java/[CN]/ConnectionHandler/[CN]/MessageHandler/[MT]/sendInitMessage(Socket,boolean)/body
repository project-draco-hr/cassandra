{
  StreamInitMessage message=new StreamInitMessage(FBUtilities.getBroadcastAddress(),session.sessionIndex(),session.planId(),session.description(),isForOutgoing,session.keepSSTableLevel());
  ByteBuffer messageBuf=message.createMessage(false,protocolVersion);
  DataOutputStreamPlus out=getWriteChannel(socket);
  out.write(messageBuf);
  out.flush();
}
