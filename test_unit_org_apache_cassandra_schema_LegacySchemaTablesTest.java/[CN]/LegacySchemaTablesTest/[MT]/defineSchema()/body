{
  SchemaLoader.prepareServer();
  SchemaLoader.startGossiper();
  SchemaLoader.createKeyspace(KEYSPACE1,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD1),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD2));
  SchemaLoader.createKeyspace(KEYSPACE3,true,false,SimpleStrategy.class,KSMetaData.optsWithRF(5),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD1),SchemaLoader.indexCFMD(KEYSPACE3,CF_INDEXED,true));
  SchemaLoader.createKeyspace(KEYSPACE6,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.indexCFMD(KEYSPACE6,CF_INDEXED,true));
}
