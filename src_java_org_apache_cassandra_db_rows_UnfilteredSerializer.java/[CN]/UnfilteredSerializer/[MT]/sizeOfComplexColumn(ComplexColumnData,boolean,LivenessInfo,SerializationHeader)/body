{
  long size=0;
  if (hasComplexDeletion)   size+=header.deletionTimeSerializedSize(data == null ? DeletionTime.LIVE : data.complexDeletion());
  size+=TypeSizes.sizeofUnsignedVInt(data.cellsCount());
  for (  Cell cell : data)   size+=Cell.serializer.serializedSize(cell,rowLiveness,header);
  return size;
}
