{
  if (header.isForSSTable()) {
    in.readUnsignedVInt();
    in.readUnsignedVInt();
  }
  if (bound.isBoundary())   return new RangeTombstoneBoundaryMarker(bound,header.readDeletionTime(in),header.readDeletionTime(in));
 else   return new RangeTombstoneBoundMarker(bound,header.readDeletionTime(in));
}
