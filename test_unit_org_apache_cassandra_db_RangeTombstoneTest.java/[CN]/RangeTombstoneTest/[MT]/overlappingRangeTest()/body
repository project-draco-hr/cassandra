{
  CompactionManager.instance.disableAutoCompaction();
  Keyspace keyspace=Keyspace.open(KSNAME);
  ColumnFamilyStore cfs=keyspace.getColumnFamilyStore(CFNAME);
  String key="k2";
  UpdateBuilder builder=UpdateBuilder.create(cfs.metadata,key).withTimestamp(0);
  for (int i=0; i < 20; i++)   builder.newRow(i).add("val",i);
  builder.applyUnsafe();
  cfs.forceBlockingFlush();
  new RowUpdateBuilder(cfs.metadata,1,key).addRangeTombstone(5,15).build().applyUnsafe();
  cfs.forceBlockingFlush();
  new RowUpdateBuilder(cfs.metadata,1,key).addRangeTombstone(5,10).build().applyUnsafe();
  cfs.forceBlockingFlush();
  new RowUpdateBuilder(cfs.metadata,2,key).addRangeTombstone(5,8).build().applyUnsafe();
  cfs.forceBlockingFlush();
  Partition partition=Util.getOnlyPartitionUnfiltered(Util.cmd(cfs,key).build());
  for (int i=0; i < 5; i++)   assertTrue("Row " + i + " should be live",partition.getRow(new SimpleClustering(bb(i))).hasLiveData(FBUtilities.nowInSeconds()));
  for (int i=16; i < 20; i++)   assertTrue("Row " + i + " should be live",partition.getRow(new SimpleClustering(bb(i))).hasLiveData(FBUtilities.nowInSeconds()));
  for (int i=5; i <= 15; i++)   assertTrue("Row " + i + " shouldn't be live",partition.getRow(new SimpleClustering(bb(i))) == null);
  CompactionManager.instance.performMaximal(cfs,false);
  partition=Util.getOnlyPartitionUnfiltered(Util.cmd(cfs,key).build());
  for (int i=0; i < 5; i++)   assertTrue("Row " + i + " should be live",partition.getRow(new SimpleClustering(bb(i))).hasLiveData(FBUtilities.nowInSeconds()));
  for (int i=16; i < 20; i++)   assertTrue("Row " + i + " should be live",partition.getRow(new SimpleClustering(bb(i))).hasLiveData(FBUtilities.nowInSeconds()));
  for (int i=5; i <= 15; i++)   assertTrue("Row " + i + " shouldn't be live",partition.getRow(new SimpleClustering(bb(i))) == null);
}
