{
  if (!reducer.hasNext())   return endOfData();
  CompactedRowContainer container=reducer.next();
  AbstractCompactedRow compactedRow;
  try {
    compactedRow=container.future == null ? container.row : new PrecompactedRow(container.key,controller,container.future.get());
  }
 catch (  InterruptedException e) {
    throw new AssertionError(e);
  }
catch (  ExecutionException e) {
    throw new RuntimeException(e);
  }
  if (compactedRow.isEmpty()) {
    controller.invalidateCachedRow(compactedRow.key);
    return null;
  }
 else {
    controller.removeDeletedInCache(compactedRow.key);
    return compactedRow;
  }
}
