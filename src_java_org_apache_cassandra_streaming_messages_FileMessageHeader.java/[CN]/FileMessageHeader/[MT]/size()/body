{
  long size=0;
  if (compressionInfo != null) {
    for (    CompressionMetadata.Chunk chunk : compressionInfo.chunks)     size+=chunk.length + 4;
  }
 else   if (compressionMetadata != null) {
    size=compressionMetadata.getTotalSizeForSections(sections);
  }
 else {
    for (    Pair<Long,Long> section : sections)     size+=section.right - section.left;
  }
  return size;
}
