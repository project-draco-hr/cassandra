{
  CFMetaData metadata=CFMetaData.Builder.createDense(KEYSPACE,TABLE,false,false).addPartitionKey("key",AsciiType.instance).addClusteringColumn("col",BytesType.instance).addRegularColumn("val",BytesType.instance).build().compressionParameters(SchemaLoader.getCompressionParameters());
  SchemaLoader.loadSchema();
  SchemaLoader.createKeyspace(KEYSPACE,KeyspaceParams.simple(1),metadata);
}
