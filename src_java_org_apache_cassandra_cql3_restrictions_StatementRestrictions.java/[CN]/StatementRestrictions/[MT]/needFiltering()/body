{
  int numberOfRestrictedColumns=0;
  for (  Restrictions restrictions : indexRestrictions)   numberOfRestrictedColumns+=restrictions.size();
  return numberOfRestrictedColumns > 1 || (numberOfRestrictedColumns == 0 && !clusteringColumnsRestrictions.isEmpty()) || (numberOfRestrictedColumns != 0 && nonPrimaryKeyRestrictions.hasMultipleContains());
}
