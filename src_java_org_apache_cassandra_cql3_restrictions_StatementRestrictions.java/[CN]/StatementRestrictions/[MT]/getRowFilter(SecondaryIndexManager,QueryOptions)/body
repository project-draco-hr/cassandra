{
  if (indexRestrictions.isEmpty())   return RowFilter.NONE;
  RowFilter filter=RowFilter.create();
  for (  Restrictions restrictions : indexRestrictions.getRestrictions())   restrictions.addRowFilterTo(filter,indexManager,options);
  for (  CustomIndexExpression expression : indexRestrictions.getCustomIndexExpressions())   expression.addToRowFilter(filter,cfm,options);
  return filter;
}
