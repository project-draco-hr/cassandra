{
  CommitLog commitLog=instances_.get(table);
  if (commitLog == null) {
    CommitLog.lock_.lock();
    try {
      commitLog=instances_.get(table);
      if (commitLog == null) {
        commitLog=new CommitLog(table,false);
        instances_.put(table,commitLog);
      }
    }
  finally {
      CommitLog.lock_.unlock();
    }
  }
  return commitLog;
}
