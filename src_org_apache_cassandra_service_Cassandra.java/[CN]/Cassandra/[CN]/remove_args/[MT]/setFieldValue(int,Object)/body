{
switch (fieldID) {
case TABLENAME:
    setTablename((String)value);
  break;
case KEY:
setKey((String)value);
break;
case COLUMNFAMILY_COLUMN:
setColumnFamily_column((String)value);
break;
case TIMESTAMP:
setTimestamp((Long)value);
break;
case BLOCK:
setBlock((Boolean)value);
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
