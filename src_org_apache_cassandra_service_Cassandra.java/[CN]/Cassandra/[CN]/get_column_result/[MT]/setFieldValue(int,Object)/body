{
switch (fieldID) {
case SUCCESS:
    setSuccess((column_t)value);
  break;
case IRE:
setIre((InvalidRequestException)value);
break;
case NFE:
setNfe((NotFoundException)value);
break;
case CE:
setCe((CassandraException)value);
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
