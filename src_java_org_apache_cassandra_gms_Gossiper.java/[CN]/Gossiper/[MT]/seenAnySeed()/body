{
  for (  Map.Entry<InetAddress,EndpointState> entry : endpointStateMap.entrySet()) {
    if (seeds.contains(entry.getKey()))     return true;
    try {
      if (entry.getValue().getApplicationStateMap().keySet().contains(ApplicationState.INTERNAL_IP) && seeds.contains(InetAddress.getByName(entry.getValue().getApplicationState(ApplicationState.INTERNAL_IP).value)))       return true;
    }
 catch (    UnknownHostException e) {
      throw new RuntimeException(e);
    }
  }
  return false;
}
