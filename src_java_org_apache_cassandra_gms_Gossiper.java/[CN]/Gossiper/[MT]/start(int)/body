{
  Set<InetAddress> seedHosts=DatabaseDescriptor.getSeeds();
  for (  InetAddress seed : seedHosts) {
    if (seed.equals(localEndpoint_))     continue;
    seeds_.add(seed);
  }
  maybeInitializeLocalState(generationNbr);
  EndpointState localState=endpointStateMap_.get(localEndpoint_);
  DatabaseDescriptor.getEndpointSnitch().gossiperStarting();
  scheduledGossipTask=executor.scheduleWithFixedDelay(new GossipTask(),Gossiper.intervalInMillis_,Gossiper.intervalInMillis_,TimeUnit.MILLISECONDS);
}
