{
  CommitLogReplayer.setIgnoreErrors(true);
  CommitLog.instance.recover(new File[]{logFile});
  CommitLogReplayer.setIgnoreErrors(false);
  try {
    CommitLog.instance.recover(new File[]{logFile});
    Assert.assertFalse(true);
  }
 catch (  Throwable t) {
    if (!(t instanceof MalformedCommitLogException))     throw t;
  }
}
