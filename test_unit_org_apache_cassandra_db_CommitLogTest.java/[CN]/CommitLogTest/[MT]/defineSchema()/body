{
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE1,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE1,STANDARD1,0,AsciiType.instance,BytesType.instance),SchemaLoader.standardCFMD(KEYSPACE1,STANDARD2,0,AsciiType.instance,BytesType.instance));
  SchemaLoader.createKeyspace(KEYSPACE2,false,true,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE1,STANDARD1,0,AsciiType.instance,BytesType.instance),SchemaLoader.standardCFMD(KEYSPACE1,STANDARD2,0,AsciiType.instance,BytesType.instance));
  System.setProperty("cassandra.commitlog.stop_on_errors","true");
  CompactionManager.instance.disableAutoCompaction();
}
