{
  Preconditions.checkNotNull(fd);
  if (SKIP_SYNC) {
    boolean closed=false;
    try {
      if (fdClosedField != null)       closed=fdClosedField.getBoolean(fd);
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
    int useCount=1;
    try {
      if (fdUseCountField != null)       useCount=((AtomicInteger)fdUseCountField.get(fd)).get();
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
    if (closed || !fd.valid() || useCount < 0)     throw new SyncFailedException("Closed " + closed + " valid "+ fd.valid()+ " useCount "+ useCount);
  }
 else {
    fd.sync();
  }
}
