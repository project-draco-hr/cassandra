{
  byte[] t=s.getBytes();
  ByteBuffer bb=ByteBufferUtil.bytes(s);
  assertArrayEquals(t,ByteBufferUtil.getArray(bb));
  bb=fromStringWithPosition(s,10,false);
  assertArrayEquals(t,ByteBufferUtil.getArray(bb));
  bb=fromStringWithPosition(s,10,true);
  assertArrayEquals(t,ByteBufferUtil.getArray(bb));
}
