{
  StringBuilder sb=new StringBuilder();
  sb.append('(');
  boolean first=true;
  for (  Map.Entry<ByteBuffer,CollectionType> entry : collections.entrySet()) {
    if (!first) {
      sb.append(',');
    }
    first=false;
    sb.append(ByteBufferUtil.bytesToHex(entry.getKey())).append(":");
    entry.getValue().appendToStringBuilder(sb);
  }
  sb.append(')');
  return sb.toString();
}
