{
  StringBuilder sb=new StringBuilder();
  sb.append('(');
  boolean first=true;
  for (  Map.Entry<ByteBuffer,? extends CollectionType> entry : collections.entrySet()) {
    if (!first)     sb.append(',');
    first=false;
    sb.append(ByteBufferUtil.bytesToHex(entry.getKey())).append(":");
    sb.append(entry.getValue());
  }
  sb.append(')');
  return sb.toString();
}
