{
  String tableName=createTable("CREATE TABLE IF NOT EXISTS %s (" + "pk1 varint PRIMARY KEY," + "reg1 int,"+ "reg2 int)"+ " WITH COMPACT STORAGE");
  ColumnFamilyStore cfs=Keyspace.open(keyspace()).getColumnFamilyStore(tableName);
  assertTrue(ColumnFamilyStoreCQLHelper.getCFMetadataAsCQL(cfs.metadata,true).startsWith("CREATE TABLE IF NOT EXISTS " + keyspace() + "."+ tableName+ " (\n"+ "\tpk1 varint PRIMARY KEY,\n"+ "\treg1 int,\n"+ "\treg2 int)\n"+ "\tWITH ID = "+ cfs.metadata.cfId+ "\n"+ "\tAND COMPACT STORAGE"));
}
