{
  if (!atomIterator.hasNext())   return endOfData();
  OnDiskAtom column;
  try {
    file.reset(mark);
    column=atomIterator.next();
  }
 catch (  IOException e) {
    throw new CorruptSSTableException(e,file.getPath());
  }
  if (finishColumn.remaining() > 0 && comparator.compare(column.name(),finishColumn) > 0)   return endOfData();
  mark=file.mark();
  return column;
}
