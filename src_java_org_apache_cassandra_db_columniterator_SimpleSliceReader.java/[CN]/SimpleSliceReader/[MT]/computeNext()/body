{
  if (i++ >= columns)   return endOfData();
  OnDiskAtom column;
  try {
    file.reset(mark);
    column=atomSerializer.deserializeFromSSTable(file,sstable.descriptor.version);
  }
 catch (  IOException e) {
    throw new RuntimeException("error reading " + i + " of "+ columns,e);
  }
  if (finishColumn.remaining() > 0 && comparator.compare(column.name(),finishColumn) > 0)   return endOfData();
  mark=file.mark();
  return column;
}
