{
  ChecksumManager chksumMgr=chksumMgrs_.get(dataFile);
  if (chksumMgr == null) {
    lock_.lock();
    try {
      if (chksumMgr == null) {
        chksumMgr=new ChecksumManager(dataFile);
        chksumMgrs_.put(dataFile,chksumMgr);
      }
    }
  finally {
      lock_.unlock();
    }
  }
  return chksumMgr;
}
