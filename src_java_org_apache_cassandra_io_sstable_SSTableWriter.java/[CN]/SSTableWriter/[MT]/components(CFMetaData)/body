{
  Set<Component> components=new HashSet<Component>(Arrays.asList(Component.DATA,Component.FILTER,Component.PRIMARY_INDEX,Component.STATS,Component.SUMMARY,Component.TOC));
  if (metadata.compressionParameters().sstableCompressor != null)   components.add(Component.COMPRESSION_INFO);
 else   components.add(Component.DIGEST);
  return components;
}
