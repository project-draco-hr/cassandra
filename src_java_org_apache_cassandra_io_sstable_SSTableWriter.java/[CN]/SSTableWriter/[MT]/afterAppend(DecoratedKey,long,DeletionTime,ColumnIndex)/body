{
  lastWrittenKey=decoratedKey;
  last=lastWrittenKey;
  if (first == null)   first=lastWrittenKey;
  if (logger.isTraceEnabled())   logger.trace("wrote " + decoratedKey + " at "+ dataPosition);
  RowIndexEntry entry=RowIndexEntry.create(dataPosition,deletion,index);
  iwriter.append(decoratedKey,entry);
  dbuilder.addPotentialBoundary(dataPosition);
  return entry;
}
