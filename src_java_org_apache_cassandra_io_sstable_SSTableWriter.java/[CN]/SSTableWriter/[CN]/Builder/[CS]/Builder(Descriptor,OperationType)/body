{
  this.desc=desc;
  cfs=Table.open(desc.ksname).getColumnFamilyStore(desc.cfname);
  try {
    if (cfs.metadata.getDefaultValidator().isCommutative())     indexer=new CommutativeRowIndexer(desc,cfs.metadata);
 else     indexer=new RowIndexer(desc,cfs.metadata);
  }
 catch (  IOException e) {
    throw new IOError(e);
  }
}
