{
  this.desc=desc;
  this.dfile=dfile;
  this.type=type;
  this.cfs=cfs;
  try {
    this.estimatedRows=SSTable.estimateRowsFromData(desc,dfile);
    this.iwriter=new IndexWriter(desc,StorageService.getPartitioner(),estimatedRows);
  }
 catch (  IOException e) {
    dfile.close();
    throw e;
  }
}
