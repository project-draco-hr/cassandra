{
  int prevStart=-2;
  int prevEnd=-1;
  for (  RangeTombstone rt : l) {
    int curStart=i(rt.min);
    int curEnd=i(rt.max);
    assertTrue("Invalid " + toString(l),prevEnd <= curStart);
    assertTrue("Invalid " + toString(l),curStart <= curEnd);
    if (curStart == curEnd && prevEnd == curStart)     assertTrue("Invalid " + toString(l),prevStart != prevEnd);
    prevStart=curStart;
    prevEnd=curEnd;
  }
}
