{
  String ks="Keyspace1";
  ByteBuffer key=ByteBufferUtil.bytes(keyString);
  for (int i=0; i < 100; ++i) {
    Mutation mutation=new Mutation(ks,key);
    mutation.add("Standard1",Util.cellname("name"),ByteBufferUtil.bytes("value" + i),System.currentTimeMillis());
    CommitLog.instance.add(mutation);
  }
}
