{
  Map<MetadataType,MetadataComponent> components=Maps.newHashMap();
  components.put(MetadataType.VALIDATION,new ValidationMetadata(partitioner,bloomFilterFPChance));
  components.put(MetadataType.STATS,new StatsMetadata(estimatedPartitionSize,estimatedCellPerPartitionCount,replayPosition,timestampTracker.min(),timestampTracker.max(),localDeletionTimeTracker.min(),localDeletionTimeTracker.max(),ttlTracker.min(),ttlTracker.max(),compressionRatio,estimatedTombstoneDropTime,sstableLevel,makeList(minClusteringValues),makeList(maxClusteringValues),hasLegacyCounterShards,repairedAt,totalColumnsSet,totalRows));
  components.put(MetadataType.COMPACTION,new CompactionMetadata(cardinality));
  components.put(MetadataType.HEADER,header.toComponent());
  return components;
}
