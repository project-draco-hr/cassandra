{
  if (maxDataAge < 0)   maxDataAge=System.currentTimeMillis();
  StatsMetadata stats=statsMetadata();
  IndexSummary indexSummary=iwriter.summary.build(partitioner);
  SegmentedFile ifile=iwriter.builder.buildIndex(desc,indexSummary);
  SegmentedFile dfile=dbuilder.buildData(desc,stats);
  SSTableReader sstable=SSTableReader.internalOpen(desc,components,this.metadata,partitioner,ifile,dfile,indexSummary,iwriter.bf.sharedCopy(),maxDataAge,stats,openReason,header);
  sstable.first=getMinimalKey(first);
  sstable.last=getMinimalKey(last);
  return sstable;
}
