{
  if (exhausted) {
    throw new IndexOutOfBoundsException();
  }
  if (file.isEOF()) {
    file.close();
    exhausted=true;
    return;
  }
  row=new IteratingRow(file,sstable);
  if (materialize) {
    while (row.hasNext()) {
      IColumn column=row.next();
      row.getEmptyColumnFamily().addColumn(column);
    }
  }
 else {
    row.skipRemaining();
  }
}
