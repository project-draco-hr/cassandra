{
  Map<String,String> options=cd.getIndexOptions();
  if (options.containsKey(SecondaryIndex.INDEX_KEYS_OPTION_NAME))   return KEYS;
 else   if (options.containsKey(SecondaryIndex.INDEX_ENTRIES_OPTION_NAME))   return KEYS_AND_VALUES;
 else   if (cd.type.isCollection() && !cd.type.isMultiCell())   return FULL;
 else   return VALUES;
}
