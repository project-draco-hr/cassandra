{
  ClusteringIndexFilter clusteringIndexFilter=makeClusteringIndexFilter(options);
  if (clusteringIndexFilter == null)   return ReadQuery.EMPTY;
  RowFilter rowFilter=getRowFilter(options);
  AbstractBounds<PartitionPosition> keyBounds=restrictions.getPartitionKeyBounds(options);
  return keyBounds == null ? ReadQuery.EMPTY : new PartitionRangeReadCommand(cfm,nowInSec,queriedColumns,rowFilter,limit,new DataRange(keyBounds,clusteringIndexFilter),Optional.empty());
}
