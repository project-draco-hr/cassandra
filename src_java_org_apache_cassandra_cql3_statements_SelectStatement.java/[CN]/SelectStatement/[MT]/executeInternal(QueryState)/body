{
  List<ByteBuffer> variables=Collections.<ByteBuffer>emptyList();
  int limit=getLimit(variables);
  List<Row> rows=isKeyRange || usesSecondaryIndexing ? RangeSliceVerbHandler.executeLocally(getRangeCommand(variables,limit)) : readLocally(keyspace(),getSliceCommands(variables,limit));
  return processResults(rows,variables,limit);
}
