{
  List<ByteBuffer> variables=Collections.emptyList();
  int limit=getLimit(variables);
  long now=System.currentTimeMillis();
  List<Row> rows=isKeyRange || usesSecondaryIndexing ? RangeSliceVerbHandler.executeLocally(getRangeCommand(variables,limit,now)) : readLocally(keyspace(),getSliceCommands(variables,limit,now));
  return processResults(rows,variables,limit,now);
}
