{
  try {
    List<Row> rows=isKeyRange || usesSecondaryIndexing ? RangeSliceVerbHandler.executeLocally(getRangeCommand(Collections.<ByteBuffer>emptyList())) : readLocally(keyspace(),getSliceCommands(Collections.<ByteBuffer>emptyList()));
    return processResults(rows,Collections.<ByteBuffer>emptyList());
  }
 catch (  ExecutionException e) {
    throw new RuntimeException(e);
  }
catch (  InterruptedException e) {
    throw new RuntimeException(e);
  }
}
