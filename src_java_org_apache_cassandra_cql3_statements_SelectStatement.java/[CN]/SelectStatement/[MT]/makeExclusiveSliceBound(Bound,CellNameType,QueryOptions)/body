{
  if (sliceRestriction.isInclusive(bound))   return null;
  ByteBuffer value=sliceRestriction.isMultiColumn() ? ((MultiColumnRestriction.Slice)sliceRestriction).componentBounds(bound,options).get(0) : sliceRestriction.bound(bound,options);
  return value.hasRemaining() ? type.makeCellName(value) : null;
}
