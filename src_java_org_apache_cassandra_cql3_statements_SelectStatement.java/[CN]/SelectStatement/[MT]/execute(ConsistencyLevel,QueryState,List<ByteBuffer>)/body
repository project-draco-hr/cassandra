{
  if (cl == null)   throw new InvalidRequestException("Invalid empty consistency level");
  cl.validateForRead(keyspace());
  List<Row> rows=isKeyRange || usesSecondaryIndexing ? StorageProxy.getRangeSlice(getRangeCommand(variables),cl) : StorageProxy.read(getSliceCommands(variables),cl);
  return processResults(rows,variables);
}
