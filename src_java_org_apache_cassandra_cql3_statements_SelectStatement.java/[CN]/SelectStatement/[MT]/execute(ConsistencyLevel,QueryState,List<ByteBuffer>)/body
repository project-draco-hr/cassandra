{
  if (cl == null)   throw new InvalidRequestException("Invalid empty consistency level");
  cl.validateForRead(keyspace());
  int limit=getLimit(variables);
  long now=System.currentTimeMillis();
  List<Row> rows=isKeyRange || usesSecondaryIndexing ? StorageProxy.getRangeSlice(getRangeCommand(variables,limit,now),cl) : StorageProxy.read(getSliceCommands(variables,limit,now),cl);
  return processResults(rows,variables,limit,now);
}
