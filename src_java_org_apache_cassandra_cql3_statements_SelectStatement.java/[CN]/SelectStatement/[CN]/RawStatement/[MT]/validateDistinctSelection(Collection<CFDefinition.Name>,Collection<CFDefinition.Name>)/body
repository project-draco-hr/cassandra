{
  for (  CFDefinition.Name name : requestedColumns)   if (!partitionKey.contains(name))   throw new InvalidRequestException(String.format("SELECT DISTINCT queries must only request partition key columns (not %s)",name));
  for (  CFDefinition.Name name : partitionKey)   if (!requestedColumns.contains(name))   throw new InvalidRequestException(String.format("SELECT DISTINCT queries must request all the partition key columns (missing %s)",name));
}
