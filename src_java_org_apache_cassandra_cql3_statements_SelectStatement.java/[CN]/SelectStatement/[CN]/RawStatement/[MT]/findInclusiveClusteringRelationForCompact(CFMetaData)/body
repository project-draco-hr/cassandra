{
  for (  Relation r : whereClause) {
    SingleColumnRelation rel=(SingleColumnRelation)r;
    if (cfm.getColumnDefinition(rel.getEntity()).kind == ColumnDefinition.Kind.CLUSTERING_COLUMN && (rel.operator() == Operator.GT || rel.operator() == Operator.LT))     return rel;
  }
  throw new AssertionError();
}
