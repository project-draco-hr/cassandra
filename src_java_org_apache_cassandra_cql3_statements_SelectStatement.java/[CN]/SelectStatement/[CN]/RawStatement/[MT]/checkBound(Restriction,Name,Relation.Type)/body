{
  Restriction.Slice existingSlice=(Restriction.Slice)existing;
  if (existingSlice.hasBound(Bound.START) && (relType == Relation.Type.GT || relType == Relation.Type.GTE))   throw new InvalidRequestException(String.format("More than one restriction was found for the start bound on %s",name.name));
  if (existingSlice.hasBound(Bound.END) && (relType == Relation.Type.LT || relType == Relation.Type.LTE))   throw new InvalidRequestException(String.format("More than one restriction was found for the end bound on %s",name.name));
}
