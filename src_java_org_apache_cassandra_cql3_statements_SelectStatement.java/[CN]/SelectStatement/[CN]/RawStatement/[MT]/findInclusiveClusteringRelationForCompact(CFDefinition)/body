{
  for (  Relation r : whereClause) {
    SingleColumnRelation rel=(SingleColumnRelation)r;
    if (cfDef.get(rel.getEntity()).kind == CFDefinition.Name.Kind.COLUMN_ALIAS && (rel.operator() == Relation.Type.GT || rel.operator() == Relation.Type.LT))     return rel;
  }
  throw new AssertionError();
}
