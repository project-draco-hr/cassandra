{
  ReadCommand readCommandDigestOnly=constructReadMessage(true);
  try {
    Message message=readCommandDigestOnly.makeReadMessage();
    if (logger_.isDebugEnabled())     logger_.debug("Reading consistency digest for " + readCommand_.key + " from "+ message.getMessageId()+ "@["+ StringUtils.join(replicas_,", ")+ "]");
    MessagingService.instance().sendRR(message,replicas_.toArray(new EndPoint[replicas_.size()]),new DigestResponseHandler());
  }
 catch (  IOException ex) {
    throw new RuntimeException(ex);
  }
}
