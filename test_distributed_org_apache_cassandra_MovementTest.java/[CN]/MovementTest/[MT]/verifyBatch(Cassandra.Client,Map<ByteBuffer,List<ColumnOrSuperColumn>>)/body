{
  for (  Map.Entry<ByteBuffer,List<ColumnOrSuperColumn>> entry : batch.entrySet()) {
    SlicePredicate sp=new SlicePredicate();
    sp.setSlice_range(new SliceRange(ByteBuffer.wrap(new byte[0]),ByteBuffer.wrap(new byte[0]),false,1000));
    assertEquals(client.get_slice(entry.getKey(),STANDARD,sp,ConsistencyLevel.ONE),entry.getValue());
  }
}
