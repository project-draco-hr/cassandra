{
  return clheader.lastFlushedAt.size() == 0 ? 0 : Collections.min(clheader.lastFlushedAt.values(),new Comparator<Integer>(){
    public int compare(    Integer o1,    Integer o2){
      if (o1 == 0)       return 1;
 else       if (o2 == 0)       return -1;
 else       return o1 - o2;
    }
  }
);
}
