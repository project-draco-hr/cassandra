{
  StringBuilder sb=new StringBuilder("");
  sb.append("CLH(dirty+flushed={");
  for (  Map.Entry<Integer,Integer> entry : lastFlushedAt.entrySet()) {
    sb.append(CFMetaData.getName(entry.getKey())).append(": ").append(entry.getValue()).append(", ");
  }
  sb.append("})");
  return sb.toString();
}
