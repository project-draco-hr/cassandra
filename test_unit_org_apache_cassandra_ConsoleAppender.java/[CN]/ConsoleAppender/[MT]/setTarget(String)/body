{
  if (!(target.equals("System.out") || target.equals("System.err"))) {
    Status status=new WarnStatus("[" + target + "] should be one of System.out or System.err",this);
    status.add(new WarnStatus("Using default target System.out",this));
    addStatus(status);
    return;
  }
  this.target=target;
}
