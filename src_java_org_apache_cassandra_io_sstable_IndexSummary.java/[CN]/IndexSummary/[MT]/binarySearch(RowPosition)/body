{
  int low=0, mid=keys.length, high=mid - 1, result=-1;
  while (low <= high) {
    mid=(low + high) >> 1;
    result=-partitioner.decorateKey(ByteBuffer.wrap(keys[mid])).compareTo(key);
    if (result > 0) {
      low=mid + 1;
    }
 else     if (result == 0) {
      return mid;
    }
 else {
      high=mid - 1;
    }
  }
  return -mid - (result < 0 ? 1 : 2);
}
