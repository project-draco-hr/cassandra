{
  if (!(column.type instanceof MapType))   throw new InvalidRequestException(String.format("Invalid operation, %s is not of map type",column.name));
  MapType mt=(MapType)column.type;
  for (  Map.Entry<Term,Term> entry : values.entrySet()) {
    Term key=entry.getKey();
    Term value=entry.getValue();
    if (key.isBindMarker())     boundNames[key.bindIndex]=keySpecOf(column,mt);
    if (value.isBindMarker())     boundNames[value.bindIndex]=valueSpecOf(column,mt);
  }
}
