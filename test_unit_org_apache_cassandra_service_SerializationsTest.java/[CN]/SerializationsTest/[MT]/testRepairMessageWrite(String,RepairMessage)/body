{
  try (DataOutputStreamAndChannel out=getOutput(fileName)){
    for (    RepairMessage message : messages) {
      testSerializedSize(message,RepairMessage.serializer);
      RepairMessage.serializer.serialize(message,out,getVersion());
    }
