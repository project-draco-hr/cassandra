{
  ByteBuffer result=ByteBuffer.allocate(dataSize() + 3 * size() + (isStatic() ? 2 : 0));
  if (isStatic())   AbstractCompositeType.putShortLength(result,CompositeType.STATIC_MARKER);
  for (int i=0; i < size(); i++) {
    ByteBuffer bb=get(i);
    AbstractCompositeType.putShortLength(result,bb.remaining());
    result.put(bb.duplicate());
    result.put((byte)0);
  }
  result.flip();
  return result;
}
