{
  if (this == o)   return true;
  if (!(o instanceof Composite))   return false;
  Composite c=(Composite)o;
  if (size() != c.size() || isStatic() != c.isStatic())   return false;
  for (int i=0; i < size(); i++) {
    if (!get(i).equals(c.get(i)))     return false;
  }
  return eoc() == c.eoc();
}
