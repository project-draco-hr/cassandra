{
  while (true) {
    Entry entry=backlog.peek();
    if (entry == null || entry.timestamp >= System.currentTimeMillis() - DatabaseDescriptor.getRpcTimeout())     break;
    Entry entry2=backlog.poll();
    if (entry2 != entry) {
      if (entry2 != null)       active.add(entry2);
      break;
    }
    dropped.incrementAndGet();
  }
}
