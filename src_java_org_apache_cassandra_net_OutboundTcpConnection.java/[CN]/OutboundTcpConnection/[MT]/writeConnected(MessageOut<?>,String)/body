{
  try {
    write(message,id,out);
    completed++;
    if (active.peek() == null) {
      out.flush();
    }
  }
 catch (  Exception e) {
    if (!(e instanceof IOException))     logger.error("error writing to " + poolReference.endPoint(),e);
 else     if (logger.isDebugEnabled())     logger.debug("error writing to " + poolReference.endPoint(),e);
    disconnect();
  }
}
