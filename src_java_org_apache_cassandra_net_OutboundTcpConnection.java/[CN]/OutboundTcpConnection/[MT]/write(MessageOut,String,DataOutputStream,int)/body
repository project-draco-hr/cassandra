{
  int header=0;
  header|=MessagingService.serializerType.ordinal();
  if (false)   header|=4;
  header|=(version << 8);
  out.writeInt(MessagingService.PROTOCOL_MAGIC);
  out.writeInt(header);
  if (version <= MessagingService.VERSION_11)   out.writeInt(-1);
  out.writeUTF(id);
  message.serialize(out,version);
}
