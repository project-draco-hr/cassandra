{
switch (fieldID) {
case COLUMN:
    if (value == null) {
      unsetColumn();
    }
 else {
      setColumn((Column)value);
    }
  break;
case SUPER_COLUMN:
if (value == null) {
  unsetSuper_column();
}
 else {
  setSuper_column((SuperColumn)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
