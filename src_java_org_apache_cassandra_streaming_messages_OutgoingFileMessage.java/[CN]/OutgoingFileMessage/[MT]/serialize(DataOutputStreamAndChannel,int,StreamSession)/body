{
  if (completed) {
    return;
  }
  FileMessageHeader.serializer.serialize(header,out,version);
  final SSTableReader reader=ref.get();
  StreamWriter writer=header.compressionInfo == null ? new StreamWriter(reader,header.sections,session) : new CompressedStreamWriter(reader,header.sections,header.compressionInfo,session);
  writer.write(out.getChannel());
}
