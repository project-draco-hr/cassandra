{
  ColumnFamily columnFamily=ArrayBackedSortedColumns.factory.create(keyspace,cf);
  IPartitioner<?> partitioner=DatabaseDescriptor.getPartitioner();
  int importedKeys=(isSorted) ? importSorted(jsonFile,columnFamily,ssTablePath,partitioner) : importUnsorted(jsonFile,columnFamily,ssTablePath,partitioner);
  if (importedKeys != -1)   System.out.printf("%d keys imported successfully.%n",importedKeys);
  return importedKeys;
}
