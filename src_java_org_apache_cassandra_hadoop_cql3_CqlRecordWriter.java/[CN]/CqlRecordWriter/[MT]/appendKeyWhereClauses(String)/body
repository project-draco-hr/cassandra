{
  String keyWhereClause="";
  for (  String partitionKey : partitionKeyColumns)   keyWhereClause+=String.format("%s = ?",keyWhereClause.isEmpty() ? partitionKey : (" AND " + partitionKey));
  for (  String clusterColumn : clusterColumns)   keyWhereClause+=" AND " + clusterColumn + " = ?";
  return cqlQuery + " WHERE " + keyWhereClause;
}
