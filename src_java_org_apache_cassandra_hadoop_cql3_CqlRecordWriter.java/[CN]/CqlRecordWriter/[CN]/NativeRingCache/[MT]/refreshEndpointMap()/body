{
  String keyspace=ConfigHelper.getOutputKeyspace(conf);
  try (Cluster cluster=CqlConfigHelper.getOutputCluster(ConfigHelper.getOutputInitialAddress(conf),conf);Session session=cluster.connect(keyspace)){
    rangeMap=new HashMap<>();
    metadata=session.getCluster().getMetadata();
    Set<TokenRange> ranges=metadata.getTokenRanges();
    for (    TokenRange range : ranges)     rangeMap.put(range,metadata.getReplicas(keyspace,range));
  }
 }
