{
switch (field) {
case HOST:
    if (value == null) {
      unsetHost();
    }
 else {
      setHost((String)value);
    }
  break;
case DATACENTER:
if (value == null) {
  unsetDatacenter();
}
 else {
  setDatacenter((String)value);
}
break;
case RACK:
if (value == null) {
unsetRack();
}
 else {
setRack((String)value);
}
break;
}
}
