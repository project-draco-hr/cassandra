{
switch (field) {
case HOST:
    if (value == null) {
      unsetHost();
    }
 else {
      setHost((String)value);
    }
  break;
case PORT:
if (value == null) {
  unsetPort();
}
 else {
  setPort((Integer)value);
}
break;
case DATACENTER:
if (value == null) {
unsetDatacenter();
}
 else {
setDatacenter((String)value);
}
break;
}
}
