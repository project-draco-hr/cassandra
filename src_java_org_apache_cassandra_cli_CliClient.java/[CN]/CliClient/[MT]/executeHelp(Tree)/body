{
  if (tree.getChildCount() > 0) {
    String token=tree.getChild(0).getText();
    for (    CliCommandHelp ch : getHelp().commands) {
      if (token.equals(ch.name)) {
        sessionState.out.println(ch.help);
        break;
      }
    }
  }
 else {
    sessionState.out.println(getHelp().help);
  }
}
