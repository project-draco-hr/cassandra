{
  sw=Util.switchPartitioner(Murmur3Partitioner.instance);
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE1,KeyspaceParams.simple(1),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD1,1,BytesType.instance),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD2,1,BytesType.instance),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD3,1,BytesType.instance),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD4,1,BytesType.instance),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD5,1,BytesType.instance));
}
