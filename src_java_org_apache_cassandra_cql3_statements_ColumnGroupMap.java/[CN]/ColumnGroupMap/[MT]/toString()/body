{
  List<String> fullStringPath=new ArrayList<>(fullPath.length);
  for (  ByteBuffer buffer : fullPath)   fullStringPath.add(ByteBufferUtil.bytesToHex(buffer));
  List<String> stringMap=new ArrayList<>(fullPath.length);
  for (  Map.Entry<ByteBuffer,Value> entry : map.entrySet())   stringMap.add(ByteBufferUtil.bytesToHex(entry.getKey()) + ": " + entry.getValue());
  return Objects.toStringHelper(this).add("fullPath","[" + StringUtils.join(fullStringPath,", ") + "]").add("map","{" + StringUtils.join(stringMap,", ") + "}").add("isStatic",isStatic).toString();
}
