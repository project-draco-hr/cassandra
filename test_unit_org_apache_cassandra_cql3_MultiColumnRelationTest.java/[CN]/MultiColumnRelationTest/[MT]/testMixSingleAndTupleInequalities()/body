{
  String[] queries=new String[]{"SELECT * FROM %s.multiple_clustering WHERE a=0 AND (b, c, d) > (0, 1, 0) AND b < 1","SELECT * FROM %s.multiple_clustering WHERE a=0 AND (b, c, d) > (0, 1, 0) AND c < 1","SELECT * FROM %s.multiple_clustering WHERE a=0 AND b > 1 AND (b, c, d) < (1, 1, 0)","SELECT * FROM %s.multiple_clustering WHERE a=0 AND c > 1 AND (b, c, d) < (1, 1, 0)"};
  for (  String query : queries) {
    try {
      execute(query);
      fail(String.format("Expected query \"%s\" to throw an InvalidRequestException",query));
    }
 catch (    InvalidRequestException e) {
    }
  }
}
