{
  ByteBufferUtil.arrayCopy(context,context.position() + bodyOffset,other.context,other.context.position() + other.bodyOffset,STEP_LENGTH);
  if (currentIsGlobal)   other.context.putShort(other.context.position() + other.headerOffset,(short)(other.getElementIndex() + Short.MIN_VALUE));
 else   if (currentIsLocal)   other.context.putShort(other.context.position() + other.headerOffset,(short)other.getElementIndex());
  other.currentIsGlobal=currentIsGlobal;
  other.currentIsLocal=currentIsLocal;
  other.moveToNext();
}
