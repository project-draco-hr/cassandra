{
  EnumMap<T,Object> options=new EnumMap<T,Object>(klass);
  int n=body.readUnsignedShort();
  for (int i=0; i < n; i++) {
    T opt=fromId(body.readUnsignedShort());
    Object value=opt.readValue(body);
    if (options.containsKey(opt))     throw new ProtocolException(String.format("Duplicate option %s in message",opt.name()));
    options.put(opt,value);
  }
  return options;
}
