{
  if (!(column instanceof Column))   throw new UnsupportedOperationException("A super column can only contain simple columns.");
  byte[] name=column.name();
  IColumn oldColumn=columns_.putIfAbsent(name,column);
  if (oldColumn != null) {
    while (((Column)oldColumn).comparePriority((Column)column) <= 0) {
      if (columns_.replace(name,oldColumn,column))       break;
      oldColumn=columns_.get(name);
    }
  }
}
