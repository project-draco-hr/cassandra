{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  SuperColumn sc=(SuperColumn)o;
  if (!name.equals(sc.name))   return false;
  if (getMarkedForDeleteAt() != sc.getMarkedForDeleteAt())   return false;
  if (getLocalDeletionTime() != sc.getLocalDeletionTime())   return false;
  return Iterables.elementsEqual(columns,sc.columns);
}
