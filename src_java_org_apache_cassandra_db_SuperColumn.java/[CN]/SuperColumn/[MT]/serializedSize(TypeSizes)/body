{
  int nameSize=name.remaining();
  int subColumnsSize=0;
  for (  IColumn subColumn : getSubColumns())   subColumnsSize+=subColumn.serializedSize(sizes);
  return sizes.sizeof((short)nameSize) + nameSize + sizes.sizeof(getLocalDeletionTime())+ sizes.sizeof(getMarkedForDeleteAt())+ sizes.sizeof(subColumnsSize)+ subColumnsSize;
}
