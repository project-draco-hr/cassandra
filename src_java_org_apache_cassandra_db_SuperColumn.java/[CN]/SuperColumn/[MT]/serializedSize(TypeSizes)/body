{
  int nameSize=name.remaining();
  int subColumnsSize=0;
  for (  IColumn subColumn : getSubColumns())   subColumnsSize+=subColumn.serializedSize(typeSizes);
  int size=typeSizes.sizeof((short)nameSize) + nameSize + typeSizes.sizeof(getLocalDeletionTime())+ typeSizes.sizeof(getMarkedForDeleteAt())+ typeSizes.sizeof(subColumnsSize)+ subColumnsSize;
  return size;
}
