{
  TestableTransaction txn;
  txn=newTest();
  txn.assertInProgress();
  txn.testing.prepareToCommit();
  txn.assertPrepared();
  txn.testing.close();
  txn.assertAborted();
  txn=newTest();
  txn.assertInProgress();
  txn.testing.prepareToCommit();
  txn.assertPrepared();
  Assert.assertNull(txn.testing.abort(null));
  txn.assertAborted();
}
