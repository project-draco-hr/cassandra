{
  while (next == null && super.hasNext()) {
    Unfiltered unfiltered=super.next();
    if (unfiltered.isRow()) {
      Row row=computeNext((Row)unfiltered);
      if (row != null && !row.isEmpty())       next=row;
    }
 else {
      next=computeNext((RangeTombstoneMarker)unfiltered);
    }
  }
  return next != null;
}
