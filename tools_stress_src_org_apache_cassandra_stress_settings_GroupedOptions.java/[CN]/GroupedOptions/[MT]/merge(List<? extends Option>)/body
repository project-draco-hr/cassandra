{
  ImmutableList.Builder<Option> builder=ImmutableList.builder();
  for (  List<? extends Option> options : optionss)   for (  Option option : options)   if (option instanceof OptionSimple && ((OptionSimple)option).isRequired())   builder.add(option);
  for (  List<? extends Option> options : optionss)   for (  Option option : options)   if (!(option instanceof OptionSimple && ((OptionSimple)option).isRequired()))   builder.add(option);
  return builder.build();
}
