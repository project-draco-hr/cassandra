{
  if (!dis.readBoolean())   return null;
  int cfId=dis.readInt();
  if (Schema.instance.getCF(cfId) == null)   throw new UnserializableColumnFamilyException("Couldn't find cfId=" + cfId,cfId);
  ColumnFamily cf=ColumnFamily.create(cfId,factory);
  deserializeFromSSTableNoColumns(cf,dis);
  deserializeColumns(dis,cf,fromRemote);
  return cf;
}
