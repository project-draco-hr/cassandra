{
  ColumnFamily cf=deserializeFromSSTableNoColumns(dis.readUTF(),dis.readUTF(),readComparator(dis),readComparator(dis),dis);
  int size=dis.readInt();
  IColumn column;
  for (int i=0; i < size; ++i) {
    column=cf.getColumnSerializer().deserialize(dis);
    cf.addColumn(column);
  }
  return cf;
}
