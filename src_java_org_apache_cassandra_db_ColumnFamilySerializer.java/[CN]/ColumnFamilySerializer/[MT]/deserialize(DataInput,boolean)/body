{
  if (!dis.readBoolean())   return null;
  int cfId=dis.readInt();
  if (CFMetaData.getCF(cfId) == null)   throw new UnserializableColumnFamilyException("Couldn't find cfId=" + cfId,cfId);
  ColumnFamily cf=ColumnFamily.create(cfId);
  deserializeFromSSTableNoColumns(cf,dis);
  deserializeColumns(dis,cf,intern);
  return cf;
}
