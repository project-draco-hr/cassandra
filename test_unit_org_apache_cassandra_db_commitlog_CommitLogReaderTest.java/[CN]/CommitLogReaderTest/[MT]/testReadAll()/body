{
  int samples=1000;
  populateData(samples);
  ArrayList<File> toCheck=getCommitLogs();
  CommitLogReader reader=new CommitLogReader();
  TestCLRHandler testHandler=new TestCLRHandler(currentTableMetadata());
  for (  File f : toCheck)   reader.readCommitLogSegment(testHandler,f,CommitLogReader.ALL_MUTATIONS,false);
  Assert.assertEquals("Expected 1000 seen mutations, got: " + testHandler.seenMutationCount(),1000,testHandler.seenMutationCount());
  confirmReadOrder(testHandler,0);
}
