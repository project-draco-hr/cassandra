{
  if (apartments_.get(cfName) == null) {
    apartments_.put(cfName,new DebuggableThreadPoolExecutor(1,1,Integer.MAX_VALUE,TimeUnit.SECONDS,new LinkedBlockingQueue<Runnable>(),new ThreadFactoryImpl("FAST-MEMTABLE-POOL")));
  }
  condition_=lock_.newCondition();
  table_=table;
  cfName_=cfName;
  creationTime_=System.currentTimeMillis();
}
