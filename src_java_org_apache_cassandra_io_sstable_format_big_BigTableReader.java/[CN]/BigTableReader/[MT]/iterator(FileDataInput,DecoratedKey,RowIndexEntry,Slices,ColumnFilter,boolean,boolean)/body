{
  if (indexEntry == null)   return UnfilteredRowIterators.noRowsIterator(metadata,key,Rows.EMPTY_STATIC_ROW,DeletionTime.LIVE,reversed);
  return reversed ? new SSTableReversedIterator(this,file,key,indexEntry,slices,selectedColumns,isForThrift) : new SSTableIterator(this,file,key,indexEntry,slices,selectedColumns,isForThrift);
}
