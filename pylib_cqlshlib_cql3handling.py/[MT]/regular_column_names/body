def regular_column_names(table_meta):
    if ((not table_meta) or (not table_meta.columns)):
        return []
    regular_coulmns = list(((set(table_meta.columns.keys()) - set([key.name for key in table_meta.partition_key])) - set([key.name for key in table_meta.clustering_key])))
    return regular_coulmns
