def udf_name_completer(ctxt, cass):
    ks = ctxt.get_binding('ksname', None)
    if (ks is not None):
        ks = dequote_name(ks)
    try:
        udfnames = cass.get_userfunction_names(ks)
    except Exception:
        if (ks is None):
            return ()
        raise
    return map(maybe_escape_name, udfnames)
