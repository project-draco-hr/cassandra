@classmethod
def from_layout(cls, layout):
    result = {}
    for row in layout:
        ksname = row[u'keyspace_name']
        if (ksname not in result):
            result[ksname] = {}
        utname = row[u'type_name']
        result[ksname][utname] = zip(row[u'column_names'], row[u'column_types'])
    return cls(meta=result)
