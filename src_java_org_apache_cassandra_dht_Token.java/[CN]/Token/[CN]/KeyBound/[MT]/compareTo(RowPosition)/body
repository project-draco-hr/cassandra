{
  if (this == pos)   return 0;
  int cmp=getToken().compareTo(pos.getToken());
  if (cmp != 0)   return cmp;
  if (isMinimumBound)   return ((pos instanceof KeyBound) && ((KeyBound)pos).isMinimumBound) ? 0 : -1;
 else   return ((pos instanceof KeyBound) && !((KeyBound)pos).isMinimumBound) ? 0 : 1;
}
