{
  current.add(isDead(c) ? null : value(c));
  if (timestamps != null) {
    timestamps[current.size() - 1]=isDead(c) ? -1 : c.timestamp();
  }
  if (ttls != null) {
    int ttl=-1;
    if (!isDead(c) && c instanceof ExpiringColumn)     ttl=c.getLocalDeletionTime() - (int)(System.currentTimeMillis() / 1000);
    ttls[current.size() - 1]=ttl;
  }
}
