{
  checkIndex(column);
  TypedColumn col=values.get(column);
  if (col.getNameType() == IntegerType.instance || col.getNameType() == LongType.instance)   return true;
 else   if (col.getNameType() instanceof ColumnMetaData)   return ((ColumnMetaData)col.getNameType()).isSigned();
 else   return false;
}
