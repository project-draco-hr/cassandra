{
  column--;
  checkIndex(column);
  TypedColumn col=values.get(column);
  if (col.getValueType() == IntegerType.instance || col.getValueType() == LongType.instance)   return true;
 else   if (col.getValueType() instanceof ColumnMetaData)   return ((ColumnMetaData)col.getValueType()).isSigned();
 else   return false;
}
