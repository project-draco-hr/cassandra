{
  return new Iterator<Cell>(){
    int idx=size() - 1;
    boolean shouldCallNext=true;
    public boolean hasNext(){
      return idx >= 0;
    }
    public Cell next(){
      shouldCallNext=false;
      return cells.get(idx--);
    }
    public void remove(){
      if (shouldCallNext)       throw new IllegalStateException();
      cells.remove(idx + 1);
      shouldCallNext=true;
    }
  }
;
}
