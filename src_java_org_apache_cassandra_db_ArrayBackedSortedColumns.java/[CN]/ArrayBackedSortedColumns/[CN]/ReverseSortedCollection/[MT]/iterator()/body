{
  return new Iterator<IColumn>(){
    int idx=size() - 1;
    boolean shouldCallNext=true;
    public boolean hasNext(){
      return idx >= 0;
    }
    public IColumn next(){
      shouldCallNext=false;
      return columns.get(idx--);
    }
    public void remove(){
      if (shouldCallNext)       throw new IllegalStateException();
      columns.remove(idx + 1);
      shouldCallNext=true;
    }
  }
;
}
