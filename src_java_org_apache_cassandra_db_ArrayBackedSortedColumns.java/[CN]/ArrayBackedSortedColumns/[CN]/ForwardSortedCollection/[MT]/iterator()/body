{
  return new Iterator<Cell>(){
    int idx=0;
    boolean shouldCallNext=true;
    public boolean hasNext(){
      return idx < size;
    }
    public Cell next(){
      shouldCallNext=false;
      return cells[idx++];
    }
    public void remove(){
      if (shouldCallNext)       throw new IllegalStateException();
      internalRemove(--idx);
      shouldCallNext=true;
      sortedSize--;
    }
  }
;
}
