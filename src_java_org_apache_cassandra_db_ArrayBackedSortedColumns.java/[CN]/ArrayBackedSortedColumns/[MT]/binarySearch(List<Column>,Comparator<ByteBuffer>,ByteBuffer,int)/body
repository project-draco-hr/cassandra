{
  int low=start;
  int mid=columns.size();
  int high=mid - 1;
  int result=-1;
  while (low <= high) {
    mid=(low + high) >> 1;
    if ((result=comparator.compare(name,columns.get(mid).name())) > 0) {
      low=mid + 1;
    }
 else     if (result == 0) {
      return mid;
    }
 else {
      high=mid - 1;
    }
  }
  return -mid - (result < 0 ? 1 : 2);
}
