{
  try {
    connectAttempt();
    stream();
    StreamOutSession session=StreamOutSession.get(to,header.sessionId);
    if (session == null) {
      logger.info("Found no stream out session at end of file stream task - this is expected if the receiver went down");
    }
 else     if (session.getFiles().size() == 0) {
      receiveReply();
      logger.info("Finished streaming session to {}",to);
    }
  }
 catch (  IOException e) {
    StreamOutSession session=StreamOutSession.get(to,header.sessionId);
    if (session != null)     session.close(false);
    throw e;
  }
 finally {
    try {
      close();
    }
 catch (    IOException e) {
      if (logger.isDebugEnabled())       logger.debug("error closing socket",e);
    }
  }
  if (logger.isDebugEnabled())   logger.debug("Done streaming " + header.file);
}
