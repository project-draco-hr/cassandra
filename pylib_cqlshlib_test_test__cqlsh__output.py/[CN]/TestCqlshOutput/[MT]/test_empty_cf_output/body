def test_empty_cf_output(self):
    self.assertCqlverQueriesGiveColoredOutput((('select * from empty_table;', '\n            (0 rows)\n            '),), cqlver=3)
    q = 'select * from has_all_types where num = 999;'
    self.assertQueriesGiveColoredOutput(((q, '\n            (0 rows)\n            '),), cqlver=3)
