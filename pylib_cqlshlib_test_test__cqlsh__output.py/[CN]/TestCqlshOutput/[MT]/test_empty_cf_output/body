def test_empty_cf_output(self):
    self.assertCqlverQueriesGiveColoredOutput((('select * from empty_table;', '\n            '),), cqlver=(2, 3))
    q = 'select * from has_all_types where num = 999;'
    self.assertQueriesGiveColoredOutput(((q, '\n             num\n             MMM\n            -----\n\n             999\n             GGG\n\n            '),), cqlver=2)
    self.assertQueriesGiveColoredOutput(((q, '\n            '),), cqlver=3)
