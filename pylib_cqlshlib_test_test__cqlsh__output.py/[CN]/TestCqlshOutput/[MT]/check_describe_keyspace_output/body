def check_describe_keyspace_output(self, output, qksname, fullcqlver):
    expected_bits = [('(?im)^CREATE KEYSPACE %s WITH\\b' % re.escape(qksname)), '(?im)^USE \\S+;$', ';\\s*$']
    if (fullcqlver == '3.0.0'):
        expected_bits.append("\\breplication = {\\n  \\'class\\':")
    else:
        expected_bits.append('\\bstrategy_class =')
    for expr in expected_bits:
        self.assertRegexpMatches(output, expr)
