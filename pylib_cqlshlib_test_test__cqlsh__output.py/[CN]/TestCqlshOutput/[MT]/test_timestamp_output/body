def test_timestamp_output(self):
    self.assertQueriesGiveColoredOutput((('select timestampcol from has_all_types where num = 0;', '\n             timestampcol\n             MMMMMMMMMMMM\n            --------------------------\n\n             2012-05-14 12:53:20+0000\n             GGGGGGGGGGGGGGGGGGGGGGGG\n\n\n            (1 rows)\n            nnnnnnnn\n            '),), env={'TZ': 'Etc/UTC', })
    try:
        import pytz
        self.assertQueriesGiveColoredOutput((('select timestampcol from has_all_types where num = 0;', '\n                 timestampcol\n                 MMMMMMMMMMMM\n                --------------------------\n\n                 2012-05-14 09:53:20-0300\n                 GGGGGGGGGGGGGGGGGGGGGGGG\n\n\n                (1 rows)\n                nnnnnnnn\n                '),), env={'TZ': 'America/Sao_Paulo', })
    except ImportError:
        pass
