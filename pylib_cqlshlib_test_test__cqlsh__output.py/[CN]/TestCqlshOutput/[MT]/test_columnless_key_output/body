def test_columnless_key_output(self):
    q = "select a from twenty_rows_table where a in ('1', '2', '-9192');"
    self.assertQueriesGiveColoredOutput(((q, '\n             a\n             M\n            ---\n\n             1\n             Y\n             2\n             Y\n\n\n            (2 rows)\n            nnnnnnnn\n            '),), cqlver='3.1.6')
