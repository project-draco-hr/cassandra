def test_null_output(self):
    self.assertCqlverQueriesGiveColoredOutput((("select k, c, notthere from undefined_values_table where k in ('k1', 'k2');", '\n             k  | c  | notthere\n             M    M    MMMMMMMM\n            ----+----+----------\n\n             k1 | c1 |     null\n             YY   YY       RRRR\n             k2 | c2 |     null\n             YY   YY       RRRR\n\n            '),), cqlver=(2, 3))
    self.assertQueriesGiveColoredOutput((("select * from undefined_values_table where k in ('k1', 'k2');", '\n             k  | c  | notthere\n             M    M    MMMMMMMM\n            ----+----+----------\n\n             k1 | c1 |     null\n             YY   YY       RRRR\n             k2 | c2 |     null\n             YY   YY       RRRR\n\n            '),), cqlver=3)
    self.assertQueriesGiveColoredOutput((("select * from undefined_values_table where k in ('k1', 'k2');", '\n             k  | c\n             M    M\n            ----+----\n\n             k1 | c1\n             YY   YY\n             k2 | c2\n             YY   YY\n\n            '),), cqlver=2)
    self.assertQueriesGiveColoredOutput((('select num, fakecol from has_all_types where num in (1, 2);', '\n             num\n             MMM\n            -----\n\n               1\n             GGG\n               2\n             GGG\n\n            '),), cqlver=2)
