def test_null_output(self):
    self.assertCqlverQueriesGiveColoredOutput((("select k, c, notthere from undefined_values_table where k in ('k1', 'k2');", '\n             k  | c  | notthere\n             M    M    MMMMMMMM\n            ----+----+----------\n\n             k1 | c1 |     null\n             YY   YY       RRRR\n             k2 | c2 |     null\n             YY   YY       RRRR\n\n\n            (2 rows)\n            nnnnnnnn\n            '),), cqlver='3.1.5')
    self.assertQueriesGiveColoredOutput((("select * from undefined_values_table where k in ('k1', 'k2');", '\n             k  | c  | notthere\n             M    M    MMMMMMMM\n            ----+----+----------\n\n             k1 | c1 |     null\n             YY   YY       RRRR\n             k2 | c2 |     null\n             YY   YY       RRRR\n\n\n            (2 rows)\n            nnnnnnnn\n            '),), cqlver='3.1.5')
