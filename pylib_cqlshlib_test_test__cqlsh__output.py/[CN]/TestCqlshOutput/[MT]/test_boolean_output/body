def test_boolean_output(self):
    self.assertCqlverQueriesGiveColoredOutput((('select num, booleancol from has_all_types where num in (0, 1, 2, 3);', '\n             num | booleancol\n             MMM   MMMMMMMMMM\n            -----+------------\n\n               0 |       True\n               G        GGGGG\n               1 |       True\n               G        GGGGG\n               2 |      False\n               G        GGGGG\n               3 |      False\n               G        GGGGG\n\n            '),), cqlver=(2, 3))
