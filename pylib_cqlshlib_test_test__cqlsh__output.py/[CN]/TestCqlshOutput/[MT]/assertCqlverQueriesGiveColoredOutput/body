def assertCqlverQueriesGiveColoredOutput(self, queries_and_expected_outputs, cqlver=(cqlsh.DEFAULT_CQLVER,), **kwargs):
    if (not isinstance(cqlver, (tuple, list))):
        cqlver = (cqlver,)
    for ver in cqlver:
        self.assertQueriesGiveColoredOutput(queries_and_expected_outputs, cqlver=ver, **kwargs)
