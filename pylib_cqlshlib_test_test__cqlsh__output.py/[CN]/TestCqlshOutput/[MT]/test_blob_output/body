def test_blob_output(self):
    self.assertCqlverQueriesGiveColoredOutput((('select num, blobcol from has_all_types where num in (0, 1, 2, 3);', '\n             num | blobcol\n             MMM   MMMMMMM\n            -----+--------------------\n\n               0 | 000102030405fffefd\n               G   mmmmmmmmmmmmmmmmmm\n               1 | ffffffffffffffffff\n               G   mmmmmmmmmmmmmmmmmm\n               2 |\n               G   mmmmmmmmmmmmmmmmmm\n               3 |                 80\n               G   mmmmmmmmmmmmmmmmmm\n\n            '),), cqlver=(2, 3))
