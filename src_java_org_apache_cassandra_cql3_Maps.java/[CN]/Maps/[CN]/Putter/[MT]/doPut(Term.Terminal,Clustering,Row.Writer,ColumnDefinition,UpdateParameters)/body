{
  if (column.type.isMultiCell()) {
    if (value == null)     return;
    Map<ByteBuffer,ByteBuffer> elements=((Value)value).map;
    for (    Map.Entry<ByteBuffer,ByteBuffer> entry : elements.entrySet())     params.addCell(clustering,column,writer,CellPath.create(entry.getKey()),entry.getValue());
  }
 else {
    if (value == null)     params.addTombstone(column,writer);
 else     params.addCell(clustering,column,writer,value.get(Server.CURRENT_VERSION));
  }
}
