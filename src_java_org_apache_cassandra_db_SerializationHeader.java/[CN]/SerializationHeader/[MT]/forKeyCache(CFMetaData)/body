{
  int size=metadata.clusteringColumns().size();
  List<AbstractType<?>> clusteringTypes=new ArrayList<>(size);
  for (int i=0; i < size; i++)   clusteringTypes.add(BytesType.instance);
  return new SerializationHeader(BytesType.instance,clusteringTypes,PartitionColumns.NONE,EncodingStats.NO_STATS,Collections.<ByteBuffer,AbstractType<?>>emptyMap());
}
