{
  try {
    create(Collections.<String,String>emptyMap());
    return true;
  }
 catch (  Exception e) {
    JVMStabilityInspector.inspectThrowable(e);
    return false;
  }
catch (  NoClassDefFoundError e) {
    return false;
  }
catch (  SnappyError e) {
    return false;
  }
catch (  UnsatisfiedLinkError e) {
    return false;
  }
}
