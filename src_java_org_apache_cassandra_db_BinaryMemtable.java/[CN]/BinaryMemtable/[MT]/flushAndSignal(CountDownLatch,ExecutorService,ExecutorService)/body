{
  sorter.execute(new Runnable(){
    public void run(){
      final List<DecoratedKey> sortedKeys=getSortedKeys();
      writer.execute(new WrappedRunnable(){
        public void runMayThrow() throws IOException {
          cfs.addSSTable(writeSortedContents(sortedKeys));
          latch.countDown();
        }
      }
);
    }
  }
);
}
