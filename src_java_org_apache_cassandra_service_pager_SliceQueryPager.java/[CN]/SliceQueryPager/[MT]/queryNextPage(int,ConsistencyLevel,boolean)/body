{
  SliceQueryFilter filter=command.filter.withUpdatedCount(pageSize);
  if (lastReturned != null)   filter=filter.withUpdatedStart(lastReturned,cfm.comparator);
  ReadCommand pageCmd=command.withUpdatedFilter(filter);
  return localQuery ? Collections.singletonList(pageCmd.getRow(Keyspace.open(command.ksName))) : StorageProxy.read(Collections.singletonList(pageCmd),consistencyLevel);
}
