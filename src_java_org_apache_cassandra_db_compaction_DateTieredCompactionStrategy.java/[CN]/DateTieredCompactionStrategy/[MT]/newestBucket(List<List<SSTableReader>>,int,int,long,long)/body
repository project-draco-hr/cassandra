{
  Target incomingWindow=getInitialTarget(now,baseTime);
  for (  List<SSTableReader> bucket : buckets) {
    if (bucket.size() >= minThreshold || (bucket.size() >= 2 && !incomingWindow.onTarget(bucket.get(0).getMinTimestamp())))     return trimToThreshold(bucket,maxThreshold);
  }
  return Collections.emptyList();
}
