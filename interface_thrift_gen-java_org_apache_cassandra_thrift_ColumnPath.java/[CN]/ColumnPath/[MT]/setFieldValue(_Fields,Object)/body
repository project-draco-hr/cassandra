{
switch (field) {
case COLUMN_FAMILY:
    if (value == null) {
      unsetColumn_family();
    }
 else {
      setColumn_family((String)value);
    }
  break;
case SUPER_COLUMN:
if (value == null) {
  unsetSuper_column();
}
 else {
  setSuper_column((ByteBuffer)value);
}
break;
case COLUMN:
if (value == null) {
unsetColumn();
}
 else {
setColumn((ByteBuffer)value);
}
break;
}
}
