{
switch (field) {
case TIMESTAMP:
    if (value == null) {
      unsetTimestamp();
    }
 else {
      setTimestamp((Long)value);
    }
  break;
case SUPER_COLUMN:
if (value == null) {
  unsetSuper_column();
}
 else {
  setSuper_column((byte[])value);
}
break;
case PREDICATE:
if (value == null) {
unsetPredicate();
}
 else {
setPredicate((SlicePredicate)value);
}
break;
}
}
