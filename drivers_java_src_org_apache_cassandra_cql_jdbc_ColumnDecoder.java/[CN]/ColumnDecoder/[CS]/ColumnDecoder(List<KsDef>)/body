{
  for (  KsDef ks : defs) {
    for (    CfDef cf : ks.getCf_defs()) {
      cfDefs.put(String.format("%s.%s",ks.getName(),cf.getName()),cf);
      for (      ColumnDef cd : cf.getColumn_metadata()) {
        try {
          comparators.put(String.format(MapFormatString,ks.getName(),cf.getName(),Specifier.ColumnSpecific.name(),ByteBufferUtil.bytesToHex(cd.bufferForName())),FBUtilities.getComparator(cd.getValidation_class()));
        }
 catch (        ConfigurationException ex) {
          throw new RuntimeException(ex);
        }
      }
    }
  }
}
