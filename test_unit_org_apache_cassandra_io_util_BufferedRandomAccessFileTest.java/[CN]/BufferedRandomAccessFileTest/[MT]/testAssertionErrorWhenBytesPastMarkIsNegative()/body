{
  try (SequentialWriter w=createTempFile("brafAssertionErrorWhenBytesPastMarkIsNegative")){
    w.write(new byte[30]);
    w.flush();
    try (ChannelProxy channel=new ChannelProxy(w.getPath());RandomAccessReader r=RandomAccessReader.open(channel)){
      r.seek(10);
      r.mark();
      r.seek(0);
      r.bytesPastMark();
    }
   }
 }
