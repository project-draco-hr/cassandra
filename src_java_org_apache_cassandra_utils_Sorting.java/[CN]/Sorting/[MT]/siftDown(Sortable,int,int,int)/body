{
  int root=lo;
  while (true) {
    int child=2 * root + 1;
    if (child >= hi)     return;
    if (child + 1 < hi && data.compare(first + child,first + child + 1) < 0)     child++;
    if (data.compare(first + root,first + child) >= 0)     return;
    data.swap(first + root,first + child);
    root=child;
  }
}
