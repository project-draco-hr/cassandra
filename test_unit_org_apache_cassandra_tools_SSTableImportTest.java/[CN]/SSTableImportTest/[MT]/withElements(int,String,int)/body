{
  return new TypeSafeMatcher<UntypedResultSet.Row>(){
    @Override public boolean matchesSafely(    Row input){
      if (!input.has("k") || !input.has("v1") || !input.has("v2"))       return false;
      return input.getInt("k") == key && input.getString("v1").equals(v1) && input.getInt("v2") == v2;
    }
    @Override public void describeTo(    Description description){
      description.appendText(String.format("a row containing: %s, %s, %s",key,v1,v2));
    }
  }
;
}
