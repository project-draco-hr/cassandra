{
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE1,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD),CFMetaData.denseCFMetaData(KEYSPACE1,CF_COUNTER,BytesType.instance).defaultValidator(CounterColumnType.instance),SchemaLoader.standardCFMD(KEYSPACE1,"AsciiKeys").keyValidator(AsciiType.instance),CFMetaData.compile("CREATE TABLE table1 (k int PRIMARY KEY, v1 text, v2 int)",KEYSPACE1));
}
