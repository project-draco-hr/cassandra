{
switch (field) {
case NAME:
    if (value == null) {
      unsetName();
    }
 else {
      setName((ByteBuffer)value);
    }
  break;
case VALIDATION_CLASS:
if (value == null) {
  unsetValidation_class();
}
 else {
  setValidation_class((String)value);
}
break;
case INDEX_TYPE:
if (value == null) {
unsetIndex_type();
}
 else {
setIndex_type((IndexType)value);
}
break;
case INDEX_NAME:
if (value == null) {
unsetIndex_name();
}
 else {
setIndex_name((String)value);
}
break;
case INDEX_OPTIONS:
if (value == null) {
unsetIndex_options();
}
 else {
setIndex_options((Map<String,String>)value);
}
break;
}
}
