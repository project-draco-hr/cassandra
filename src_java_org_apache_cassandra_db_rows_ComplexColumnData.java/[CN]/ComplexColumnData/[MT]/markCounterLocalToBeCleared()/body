{
  Cell[] newCells=null;
  for (int i=0; i < cells.length; i++) {
    Cell cell=cells[i];
    Cell marked=cell.markCounterLocalToBeCleared();
    if (marked != cell) {
      if (newCells == null)       newCells=Arrays.copyOf(cells,cells.length);
      newCells[i]=marked;
    }
  }
  return newCells == null ? this : new ComplexColumnData(column,newCells,complexDeletion);
}
