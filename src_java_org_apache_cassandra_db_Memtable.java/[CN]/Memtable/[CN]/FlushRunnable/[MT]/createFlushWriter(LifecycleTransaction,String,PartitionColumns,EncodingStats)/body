{
  MetadataCollector sstableMetadataCollector=new MetadataCollector(cfs.metadata.comparator).commitLogLowerBound(commitLogLowerBound.get()).commitLogUpperBound(commitLogUpperBound.get());
  return cfs.createSSTableMultiWriter(Descriptor.fromFilename(filename),(long)toFlush.size(),ActiveRepairService.UNREPAIRED_SSTABLE,sstableMetadataCollector,new SerializationHeader(true,cfs.metadata,columns,stats),txn);
}
