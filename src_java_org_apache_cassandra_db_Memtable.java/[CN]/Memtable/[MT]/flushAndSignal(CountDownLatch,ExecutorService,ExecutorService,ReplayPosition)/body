{
  writer.execute(new WrappedRunnable(){
    public void runMayThrow() throws IOException {
      cfs.flushLock.lock();
      try {
        if (!cfs.isDropped()) {
          SSTableReader sstable=writeSortedContents(context);
          cfs.replaceFlushed(Memtable.this,sstable);
        }
      }
  finally {
        cfs.flushLock.unlock();
      }
      latch.countDown();
    }
  }
);
}
