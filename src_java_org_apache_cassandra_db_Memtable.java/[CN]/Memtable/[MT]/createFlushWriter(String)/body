{
  MetadataCollector sstableMetadataCollector=new MetadataCollector(cfs.metadata.comparator).commitLogLowerBound(commitLogLowerBound.get()).commitLogUpperBound(commitLogUpperBound.get());
  return SSTableWriter.create(Descriptor.fromFilename(filename),(long)rows.size(),ActiveRepairService.UNREPAIRED_SSTABLE,cfs.metadata,cfs.partitioner,sstableMetadataCollector);
}
