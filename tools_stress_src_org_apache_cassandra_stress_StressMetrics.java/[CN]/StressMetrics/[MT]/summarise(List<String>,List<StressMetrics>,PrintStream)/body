{
  int idLen=0;
  for (  String id : ids)   idLen=Math.max(id.length(),idLen);
  String formatstr="%" + idLen + "s, ";
  printHeader(String.format(formatstr,"id"),out);
  for (int i=0; i < ids.size(); i++)   printRow(String.format(formatstr,ids.get(i)),summarise.get(i).timing.getHistory(),summarise.get(i).timing.getHistory(),summarise.get(i).rowRateUncertainty,out);
}
