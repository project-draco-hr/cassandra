{
  currentClustering=clustering;
  for (int i=0; i < versions.length; i++) {
    Row version=versions[i];
    if (version == null || mergedInfo.supersedes(version.primaryKeyLivenessInfo()))     currentRow(i).writePartitionKeyLivenessInfo(mergedInfo);
    if (version == null || mergedDeletion.supersedes(version.deletion()))     currentRow(i).writeRowDeletion(mergedDeletion);
  }
}
