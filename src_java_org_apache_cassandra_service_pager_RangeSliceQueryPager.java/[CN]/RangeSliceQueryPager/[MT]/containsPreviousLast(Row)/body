{
  if (lastReturnedKey == null || !lastReturnedKey.equals(first.key))   return false;
  Column firstColumn=isReversed() ? lastColumn(first.cf) : firstNonStaticColumn(first.cf);
  return !first.cf.deletionInfo().isDeleted(firstColumn) && firstColumn.isLive(timestamp()) && lastReturnedName.equals(firstColumn.name());
}
