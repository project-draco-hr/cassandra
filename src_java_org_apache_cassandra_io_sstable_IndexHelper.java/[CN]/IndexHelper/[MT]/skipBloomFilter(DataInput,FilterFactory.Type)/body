{
  int size=in.readInt();
switch (type) {
case SHA:
    FileUtils.skipBytesFully(in,size);
  break;
case MURMUR2:
case MURMUR3:
long bitLength=in.readInt() * 8;
FileUtils.skipBytesFully(in,bitLength);
break;
default :
throw new IllegalStateException("Unknown filterfactory type " + type.toString());
}
}
