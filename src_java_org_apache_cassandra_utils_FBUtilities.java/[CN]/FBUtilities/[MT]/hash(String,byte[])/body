{
  byte[] result=null;
  try {
    MessageDigest messageDigest=MessageDigest.getInstance(type);
    for (    byte[] block : data)     messageDigest.update(block);
    result=messageDigest.digest();
  }
 catch (  Exception e) {
    if (logger_.isDebugEnabled())     logger_.debug(LogUtil.throwableToString(e));
  }
  return result;
}
