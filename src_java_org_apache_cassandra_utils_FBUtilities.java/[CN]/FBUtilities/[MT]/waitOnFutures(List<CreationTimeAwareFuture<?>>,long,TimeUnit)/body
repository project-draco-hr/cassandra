{
  for (  Future<?> future : hintFutures) {
    try {
      future.get(timeout,timeUnit);
    }
 catch (    InterruptedException ex) {
      throw new AssertionError(ex);
    }
catch (    ExecutionException e) {
      throw new RuntimeException(e);
    }
catch (    TimeoutException e) {
      throw e;
    }
  }
}
