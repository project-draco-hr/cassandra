{
  byte[] result;
  try {
    MessageDigest messageDigest=MessageDigest.getInstance(type);
    for (    ByteBuffer block : data) {
      messageDigest.update(ByteBufferUtil.clone(block));
    }
    result=messageDigest.digest();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return result;
}
