{
  if (left == null || right == null)   return null;
  if (left.length > right.length) {
    byte[] swap=left;
    left=right;
    right=swap;
  }
  byte[] out=Arrays.copyOf(right,right.length);
  for (int i=0; i < left.length; i++) {
    out[i]=(byte)((left[i] & 0xFF) ^ (right[i] & 0xFF));
  }
  return out;
}
