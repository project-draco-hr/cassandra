      int j=random.nextInt(columns) + 1;
      random.nextBytes(bytes);
      if (ts == Integer.MAX_VALUE) {
        ts=0;
      }
      ts++;
      applyThrift(tablename_,stringKey,columnFamilySuperColumn_ + ":" + superColumnFix_+ i+ ":"+ columnFix_+ j,bytes,ts);
    }
