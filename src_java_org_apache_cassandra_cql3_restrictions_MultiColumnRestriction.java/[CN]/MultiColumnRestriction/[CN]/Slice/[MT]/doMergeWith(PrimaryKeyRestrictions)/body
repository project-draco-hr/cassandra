{
  checkTrue(otherRestriction.isSlice(),"Column \"%s\" cannot be restricted by both an equality and an inequality relation",getColumnsInCommons(otherRestriction));
  Slice otherSlice=(Slice)otherRestriction;
  checkFalse(hasBound(Bound.START) && otherSlice.hasBound(Bound.START),"More than one restriction was found for the start bound on %s",getColumnsInCommons(otherRestriction));
  checkFalse(hasBound(Bound.END) && otherSlice.hasBound(Bound.END),"More than one restriction was found for the end bound on %s",getColumnsInCommons(otherRestriction));
  List<ColumnDefinition> newColumnDefs=size() >= otherSlice.size() ? columnDefs : otherSlice.columnDefs;
  return new Slice(ctype,newColumnDefs,slice.merge(otherSlice.slice));
}
