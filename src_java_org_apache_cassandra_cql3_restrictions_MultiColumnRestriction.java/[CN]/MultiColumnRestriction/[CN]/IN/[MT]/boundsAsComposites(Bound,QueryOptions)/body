{
  CBuilder builder=ctype.builder();
  List<List<ByteBuffer>> splitInValues=splitValues(options);
  TreeSet<Composite> inValues=new TreeSet<>(ctype);
  for (  List<ByteBuffer> components : splitInValues) {
    for (int i=0; i < components.size(); i++)     checkNotNull(components.get(i),"Invalid null value in condition for column %s",columnDefs.get(i).name);
    Composite prefix=builder.buildWith(components);
    inValues.add(bound.isEnd() && builder.remainingCount() - components.size() > 0 ? prefix.end() : prefix);
  }
  return new ArrayList<>(inValues);
}
