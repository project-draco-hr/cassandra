{
  if (start < 0 || end < 0 || end < start)   throw new IllegalArgumentException("Invalid values for start and end.");
  RandomAccessFile file=new RandomAccessFile(filename_,"rw");
  try {
    if (end == 0)     end=file.length();
    buffer_=file.getChannel().map(FileChannel.MapMode.READ_ONLY,start,end);
    buffer_.load();
  }
  finally {
    file.close();
  }
}
