{
  if (btree.length == 0)   return build(updateWith,comparator,updateWithIsSorted,updateF);
  if (!updateWithIsSorted)   updateWith=sorted(updateWith,comparator,updateWith.size());
  Queue<Builder> queue=modifier.get();
  Builder builder=queue.poll();
  if (builder == null)   builder=new Builder();
  btree=builder.update(btree,comparator,updateWith,updateF);
  queue.add(builder);
  return btree;
}
