{
  if (btree.length == 0)   return build(updateWith,updateWithLength,updateF);
  Queue<Builder> queue=modifier.get();
  Builder builder=queue.poll();
  if (builder == null)   builder=new Builder();
  btree=builder.update(btree,comparator,updateWith,updateF);
  queue.add(builder);
  return btree;
}
