{
  long length=new File(path).length();
  long prev=boundaries[0];
  for (int i=1; i <= boundaries.length && prev < length; i++) {
    long boundary=i == boundaries.length ? length : boundaries[i];
    Assert.assertEquals(String.format("[%d, %d), %d of %d",boundary,prev,i,boundaries.length),expectMmappable,boundary - prev <= Integer.MAX_VALUE);
    prev=boundary;
  }
}
