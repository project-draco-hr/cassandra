def test_missing_super(self):
    "verifies that inserting doesn't yield false positives."
    self._set_keyspace('Keyspace1')
    avro_utils.assert_raises(AvroRemoteException, self.client.request, 'get', _make_read_params('key1', 'Super1', 'sc1', avro_utils.i64(1), 'ONE'))
    self._insert_super()
    avro_utils.assert_raises(AvroRemoteException, self.client.request, 'get', _make_read_params('key1', 'Super1', 'sc1', avro_utils.i64(1), 'ONE'))
