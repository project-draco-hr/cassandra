{
  RowMutation rm=new RowMutation(batchMutation.table,batchMutation.key.trim());
  for (  String cfname : batchMutation.cfmap.keySet()) {
    List<column_t> list=batchMutation.cfmap.get(cfname);
    for (    column_t columnData : list) {
      rm.add(cfname + ":" + columnData.columnName,columnData.value,columnData.timestamp);
    }
  }
  return rm;
}
