{
  Table table=Table.open(table_);
  Set<String> cfNames=modifications_.keySet();
  for (  String cfName : cfNames) {
    if (!table.isValidColumnFamily(cfName))     throw new ColumnFamilyNotDefinedException("Column Family " + cfName + " has not been defined.");
    row.addColumnFamily(modifications_.get(cfName));
  }
  table.load(row);
}
