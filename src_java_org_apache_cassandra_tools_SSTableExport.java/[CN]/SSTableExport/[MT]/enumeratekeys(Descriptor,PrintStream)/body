{
  KeyIterator iter=new KeyIterator(desc);
  try {
    DecoratedKey lastKey=null;
    while (iter.hasNext()) {
      DecoratedKey key=iter.next();
      if (lastKey != null && lastKey.compareTo(key) > 0)       throw new IOException("Key out of order! " + lastKey + " > "+ key);
      lastKey=key;
      outs.println(bytesToHex(key.getKey()));
      checkStream(outs);
    }
  }
  finally {
    iter.close();
  }
}
