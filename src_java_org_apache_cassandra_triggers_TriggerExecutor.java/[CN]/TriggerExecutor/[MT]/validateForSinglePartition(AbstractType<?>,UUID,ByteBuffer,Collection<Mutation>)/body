{
  for (  Mutation mutation : tmutations) {
    if (keyValidator.compare(mutation.key(),key) != 0)     throw new InvalidRequestException("Partition key of additional mutation does not match primary update key");
    for (    ColumnFamily cf : mutation.getColumnFamilies()) {
      if (!cf.id().equals(cfId))       throw new InvalidRequestException("Column family of additional mutation does not match primary update cf");
    }
  }
  validate(tmutations);
}
