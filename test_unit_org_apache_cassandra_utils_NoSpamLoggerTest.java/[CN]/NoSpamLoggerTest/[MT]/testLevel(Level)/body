{
  setUp();
  now=5;
  NoSpamLogger.log(mock,l,5,TimeUnit.NANOSECONDS,statement,param);
  assertEquals(1,logged.get(l).size());
  NoSpamLogger.log(mock,l,5,TimeUnit.NANOSECONDS,statement,param);
  assertEquals(1,logged.get(l).size());
  now+=5;
  NoSpamLogger.log(mock,l,5,TimeUnit.NANOSECONDS,statement,param);
  assertEquals(2,logged.get(l).size());
}
