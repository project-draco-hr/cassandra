{
  setUp();
  now=5;
  NoSpamLogger.clearWrappedLoggersForTest();
  NoSpamLogger.log(mock,l,5,TimeUnit.NANOSECONDS,"swizzle{}","a");
  assertEquals(1,logged.get(l).size());
  NoSpamLogger.log(mock,l,5,TimeUnit.NANOSECONDS,"swizzle{}","a");
  assertEquals(1,logged.get(l).size());
  now+=5;
  NoSpamLogger.log(mock,l,5,TimeUnit.NANOSECONDS,"swizzle{}","a");
  assertEquals(2,logged.get(l).size());
}
