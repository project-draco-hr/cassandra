{
  List<AbstractType<?>> args=fun.argsType();
  StringBuilder sb=new StringBuilder();
  sb.append("(");
  for (int i=0; i < args.size(); i++) {
    if (i > 0)     sb.append(", ");
    sb.append(args.get(i).asCQL3Type());
  }
  sb.append(") -> ");
  sb.append(fun.returnType().asCQL3Type());
  return sb.toString();
}
