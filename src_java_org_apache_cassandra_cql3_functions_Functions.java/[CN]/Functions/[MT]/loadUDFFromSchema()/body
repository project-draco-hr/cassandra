{
  logger.debug("Loading UDFs");
  for (  UntypedResultSet.Row row : QueryProcessor.executeOnceInternal(SELECT_UD_FUNCTION))   addFunction(UDFunction.fromSchema(row));
  for (  UntypedResultSet.Row row : QueryProcessor.executeOnceInternal(SELECT_UD_AGGREGATE))   addFunction(UDAggregate.fromSchema(row));
}
