{
  if (!receiver.type.equals(fun.returnType()))   return false;
  if (providedArgs.size() != fun.argsType().size())   return false;
  for (int i=0; i < providedArgs.size(); i++) {
    AssignementTestable provided=providedArgs.get(i);
    if (provided == null)     continue;
    ColumnSpecification expected=makeArgSpec(receiver,fun,i);
    if (!provided.isAssignableTo(expected))     return false;
  }
  return true;
}
