{
  if (buffer == null)   throw new AssertionError("Attempted to read from closed RAR");
  if (length == 0)   return 0;
  if (!buffer.hasRemaining()) {
    if (isEOF())     return -1;
    reBuffer();
  }
  int toCopy=Math.min(length,buffer.remaining());
  buffer.get(buff,offset,toCopy);
  return toCopy;
}
