{
  if (buffer == null)   throw new IllegalStateException("Attempted to read from closed RAR");
  if (length == 0)   return 0;
  if (isEOF())   return -1;
  if (!buffer.hasRemaining())   reBuffer();
  int toCopy=Math.min(length,buffer.remaining());
  buffer.get(buff,offset,toCopy);
  return toCopy;
}
