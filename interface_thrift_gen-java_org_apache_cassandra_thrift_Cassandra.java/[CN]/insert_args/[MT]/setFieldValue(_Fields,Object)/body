{
switch (field) {
case KEY:
    if (value == null) {
      unsetKey();
    }
 else {
      setKey((byte[])value);
    }
  break;
case COLUMN_PARENT:
if (value == null) {
  unsetColumn_parent();
}
 else {
  setColumn_parent((ColumnParent)value);
}
break;
case COLUMN:
if (value == null) {
unsetColumn();
}
 else {
setColumn((Column)value);
}
break;
case CONSISTENCY_LEVEL:
if (value == null) {
unsetConsistency_level();
}
 else {
setConsistency_level((ConsistencyLevel)value);
}
break;
}
}
