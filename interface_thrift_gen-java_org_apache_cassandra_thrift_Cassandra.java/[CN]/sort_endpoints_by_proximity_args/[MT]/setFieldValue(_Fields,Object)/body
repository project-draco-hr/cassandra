{
switch (field) {
case ENDPOINT:
    if (value == null) {
      unsetEndpoint();
    }
 else {
      setEndpoint((String)value);
    }
  break;
case ENDPOINTS:
if (value == null) {
  unsetEndpoints();
}
 else {
  setEndpoints((List<String>)value);
}
break;
case RESTRICT_TO_SAME_DC:
if (value == null) {
unsetRestrictToSameDC();
}
 else {
setRestrictToSameDC((Boolean)value);
}
break;
}
}
