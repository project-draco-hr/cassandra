{
  StringBuilder sb=new StringBuilder("prepare_cql_query_args(");
  boolean first=true;
  sb.append("query:");
  if (this.query == null) {
    sb.append("null");
  }
 else {
    TBaseHelper.toString(this.query,sb);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("compression:");
  if (this.compression == null) {
    sb.append("null");
  }
 else {
    sb.append(this.compression);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
