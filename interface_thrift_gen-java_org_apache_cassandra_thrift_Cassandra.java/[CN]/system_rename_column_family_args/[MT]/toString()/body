{
  StringBuilder sb=new StringBuilder("system_rename_column_family_args(");
  boolean first=true;
  sb.append("old_name:");
  if (this.old_name == null) {
    sb.append("null");
  }
 else {
    sb.append(this.old_name);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("new_name:");
  if (this.new_name == null) {
    sb.append("null");
  }
 else {
