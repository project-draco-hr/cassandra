{
  if (field == null) {
    throw new IllegalArgumentException();
  }
switch (field) {
case KEY:
    return isSetKey();
case COLUMN_FAMILY:
  return isSetColumn_family();
case EXPECTED:
return isSetExpected();
case UPDATES:
return isSetUpdates();
case CONSISTENCY_LEVEL:
return isSetConsistency_level();
}
throw new IllegalStateException();
}
