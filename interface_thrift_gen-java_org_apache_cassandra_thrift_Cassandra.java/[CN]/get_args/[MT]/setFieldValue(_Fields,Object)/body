{
switch (field) {
case KEY:
    if (value == null) {
      unsetKey();
    }
 else {
      setKey((ByteBuffer)value);
    }
  break;
case COLUMN_PATH:
if (value == null) {
  unsetColumn_path();
}
 else {
  setColumn_path((ColumnPath)value);
}
break;
case CONSISTENCY_LEVEL:
if (value == null) {
unsetConsistency_level();
}
 else {
setConsistency_level((ConsistencyLevel)value);
}
break;
}
}
