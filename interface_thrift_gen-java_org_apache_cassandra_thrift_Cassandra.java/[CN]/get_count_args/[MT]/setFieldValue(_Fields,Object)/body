{
switch (field) {
case KEYSPACE:
    if (value == null) {
      unsetKeyspace();
    }
 else {
      setKeyspace((String)value);
    }
  break;
case KEY:
if (value == null) {
  unsetKey();
}
 else {
  setKey((byte[])value);
}
break;
case COLUMN_PARENT:
if (value == null) {
unsetColumn_parent();
}
 else {
setColumn_parent((ColumnParent)value);
}
break;
case CONSISTENCY_LEVEL:
if (value == null) {
unsetConsistency_level();
}
 else {
setConsistency_level((ConsistencyLevel)value);
}
break;
}
}
