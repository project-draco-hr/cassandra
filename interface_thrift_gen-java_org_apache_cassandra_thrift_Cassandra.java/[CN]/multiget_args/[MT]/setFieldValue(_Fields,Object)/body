{
switch (field) {
case KEYSPACE:
    if (value == null) {
      unsetKeyspace();
    }
 else {
      setKeyspace((String)value);
    }
  break;
case KEYS:
if (value == null) {
  unsetKeys();
}
 else {
  setKeys((List<byte[]>)value);
}
break;
case COLUMN_PATH:
if (value == null) {
unsetColumn_path();
}
 else {
setColumn_path((ColumnPath)value);
}
break;
case CONSISTENCY_LEVEL:
if (value == null) {
unsetConsistency_level();
}
 else {
setConsistency_level((ConsistencyLevel)value);
}
break;
}
}
