  if (this.cfName == null) {
    sb.append("null");
  }
 else {
    sb.append(this.cfName);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("start_token:");
  if (this.start_token == null) {
    sb.append("null");
  }
 else {
    sb.append(this.start_token);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("end_token:");
  if (this.end_token == null) {
    sb.append("null");
  }
 else {
    sb.append(this.end_token);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("keys_per_split:");
  sb.append(this.keys_per_split);
  first=false;
  sb.append(")");
