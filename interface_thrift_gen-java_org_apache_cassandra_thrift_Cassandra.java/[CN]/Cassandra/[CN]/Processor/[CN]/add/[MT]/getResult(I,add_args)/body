{
  add_result result=new add_result();
  try {
    iface.add(args.key,args.column_parent,args.column,args.consistency_level);
  }
 catch (  InvalidRequestException ire) {
    result.ire=ire;
  }
catch (  UnavailableException ue) {
    result.ue=ue;
  }
catch (  TimedOutException te) {
    result.te=te;
  }
  return result;
}
