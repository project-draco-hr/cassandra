{
  TMessage msg=iprot_.readMessageBegin();
  if (msg.type == TMessageType.EXCEPTION) {
    TApplicationException x=TApplicationException.read(iprot_);
    iprot_.readMessageEnd();
    throw x;
  }
  if (msg.seqid != seqid_) {
    throw new TApplicationException(TApplicationException.BAD_SEQUENCE_ID,"prepare_cql_query failed: out of sequence response");
  }
  prepare_cql_query_result result=new prepare_cql_query_result();
  result.read(iprot_);
  iprot_.readMessageEnd();
  if (result.isSetSuccess()) {
    return result.success;
  }
  if (result.ire != null) {
    throw result.ire;
  }
  throw new TApplicationException(TApplicationException.MISSING_RESULT,"prepare_cql_query failed: unknown result");
}
