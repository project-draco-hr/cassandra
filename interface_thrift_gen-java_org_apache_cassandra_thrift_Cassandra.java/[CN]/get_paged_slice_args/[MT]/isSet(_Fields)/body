{
  if (field == null) {
    throw new IllegalArgumentException();
  }
switch (field) {
case COLUMN_FAMILY:
    return isSetColumn_family();
case RANGE:
  return isSetRange();
case START_COLUMN:
return isSetStart_column();
case CONSISTENCY_LEVEL:
return isSetConsistency_level();
}
throw new IllegalStateException();
}
