{
switch (field) {
case COLUMN_FAMILY:
    if (value == null) {
      unsetColumn_family();
    }
 else {
      setColumn_family((String)value);
    }
  break;
case RANGE:
if (value == null) {
  unsetRange();
}
 else {
  setRange((KeyRange)value);
}
break;
case START_COLUMN:
if (value == null) {
unsetStart_column();
}
 else {
setStart_column((ByteBuffer)value);
}
break;
case CONSISTENCY_LEVEL:
if (value == null) {
unsetConsistency_level();
}
 else {
setConsistency_level((ConsistencyLevel)value);
}
break;
}
}
