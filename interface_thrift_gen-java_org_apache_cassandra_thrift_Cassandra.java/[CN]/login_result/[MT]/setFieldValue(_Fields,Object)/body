{
switch (field) {
case SUCCESS:
    if (value == null) {
      unsetSuccess();
    }
 else {
      setSuccess((AccessLevel)value);
    }
  break;
case AUTHNX:
if (value == null) {
  unsetAuthnx();
}
 else {
  setAuthnx((AuthenticationException)value);
}
break;
case AUTHZX:
if (value == null) {
unsetAuthzx();
}
 else {
setAuthzx((AuthorizationException)value);
}
break;
}
}
