{
switch (field) {
case KEY:
    if (value == null) {
      unsetKey();
    }
 else {
      setKey((ByteBuffer)value);
    }
  break;
case COLUMN_PARENT:
if (value == null) {
  unsetColumn_parent();
}
 else {
  setColumn_parent((ColumnParent)value);
}
break;
case PREDICATE:
if (value == null) {
unsetPredicate();
}
 else {
setPredicate((SlicePredicate)value);
}
break;
case CONSISTENCY_LEVEL:
if (value == null) {
unsetConsistency_level();
}
 else {
setConsistency_level((ConsistencyLevel)value);
}
break;
}
}
