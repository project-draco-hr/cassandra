{
switch (field) {
case QUERY:
    if (value == null) {
      unsetQuery();
    }
 else {
      setQuery((ByteBuffer)value);
    }
  break;
case COMPRESSION:
if (value == null) {
  unsetCompression();
}
 else {
  setCompression((Compression)value);
}
break;
case CONSISTENCY:
if (value == null) {
unsetConsistency();
}
 else {
setConsistency((ConsistencyLevel)value);
}
break;
}
}
