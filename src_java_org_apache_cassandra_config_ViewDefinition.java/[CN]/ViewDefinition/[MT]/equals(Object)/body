{
  if (this == o)   return true;
  if (!(o instanceof ViewDefinition))   return false;
  ViewDefinition other=(ViewDefinition)o;
  return Objects.equals(ksName,other.ksName) && Objects.equals(viewName,other.viewName) && Objects.equals(baseTableId,other.baseTableId)&& Objects.equals(includeAllColumns,other.includeAllColumns)&& Objects.equals(whereClause,other.whereClause)&& Objects.equals(metadata,other.metadata);
}
