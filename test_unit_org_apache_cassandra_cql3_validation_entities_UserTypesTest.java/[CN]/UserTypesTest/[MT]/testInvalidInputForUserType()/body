{
  String myType=createType("CREATE TYPE %s (f int)");
  createTable("CREATE TABLE %s(pk int PRIMARY KEY, t frozen<" + myType + ">)");
  assertInvalidMessage("Not enough bytes to read 0th field f","INSERT INTO %s (pk, t) VALUES (?, ?)",1,"test");
  assertInvalidMessage("Not enough bytes to read 0th field f","INSERT INTO %s (pk, t) VALUES (?, ?)",1,Long.MAX_VALUE);
}
