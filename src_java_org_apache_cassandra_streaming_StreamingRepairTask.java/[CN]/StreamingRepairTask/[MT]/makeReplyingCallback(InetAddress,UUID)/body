{
  return new Runnable(){
    private final AtomicInteger outstanding=new AtomicInteger(2);
    public void run(){
      if (outstanding.decrementAndGet() > 0)       return;
      try {
        StreamingRepairResponse.reply(taskOwner,taskId);
      }
 catch (      IOException e) {
        throw new IOError(e);
      }
    }
  }
;
}
