{
  SchemaLoader.loadSchema();
  SchemaLoader.createKeyspace(KEYSPACE,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE,CF),SchemaLoader.standardCFMD(KEYSPACE,CF2),SchemaLoader.standardCFMD(KEYSPACE,CF3),CFMetaData.denseCFMetaData(KEYSPACE,COUNTER_CF,BytesType.instance).defaultValidator(CounterColumnType.instance),SchemaLoader.standardCFMD(KEYSPACE,CF_UUID).keyValidator(UUIDType.instance));
}
