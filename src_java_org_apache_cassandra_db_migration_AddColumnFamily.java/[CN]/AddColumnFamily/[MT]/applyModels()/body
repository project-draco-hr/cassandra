{
  KSMetaData ksm=schema.getTableDefinition(cfm.ksName);
  ksm=makeNewKeyspaceDefinition(ksm);
  try {
    schema.load(cfm);
  }
 catch (  ConfigurationException ex) {
    throw new IOException(ex);
  }
  Table.open(cfm.ksName,schema);
  schema.setTableDefinition(ksm,newVersion);
  schema.fixCFMaxId();
  if (!StorageService.instance.isClientMode())   Table.open(ksm.name,schema).initCf(cfm.cfId,cfm.cfName);
}
