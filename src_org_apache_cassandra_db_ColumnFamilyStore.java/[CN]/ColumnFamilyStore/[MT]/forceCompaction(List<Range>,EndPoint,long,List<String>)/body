{
  Future<Boolean> futurePtr=null;
  if (ranges != null)   futurePtr=MinorCompactionManager.instance().submit(ColumnFamilyStore.this,ranges,target,fileList);
 else   MinorCompactionManager.instance().submitMajor(ColumnFamilyStore.this,skip);
  boolean result=true;
  try {
    if (futurePtr != null)     result=futurePtr.get();
    logger_.debug("Done forcing compaction ...");
  }
 catch (  ExecutionException ex) {
    logger_.debug(LogUtil.throwableToString(ex));
  }
catch (  InterruptedException ex2) {
    logger_.debug(LogUtil.throwableToString(ex2));
  }
  return result;
}
