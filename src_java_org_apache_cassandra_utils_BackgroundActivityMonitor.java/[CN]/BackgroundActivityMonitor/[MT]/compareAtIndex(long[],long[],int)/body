{
  long total1=0, total2=0;
  for (int i=0; i <= SOFTIRQ_INDEX; i++) {
    total1+=reading1[i];
    total2+=reading2[i];
  }
  float totalDiff=total2 - total1;
  long intrested1=reading1[index], intrested2=reading2[index];
  float diff=intrested2 - intrested1;
  if (diff == 0)   return 0f;
  return (diff / totalDiff) * 100;
}
