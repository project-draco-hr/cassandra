{
  Message message=new Message(FBUtilities.getLocalAddress(),StorageService.Verb.BOOTSTRAP_TOKEN,ArrayUtils.EMPTY_BYTE_ARRAY,Gossiper.instance.getVersion(maxEndpoint));
  BootstrapTokenCallback btc=new BootstrapTokenCallback();
  MessagingService.instance().sendRR(message,maxEndpoint,btc);
  return btc.getToken();
}
