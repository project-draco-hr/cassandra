{
  long position=in.readLong();
  if (version.hasPromotedIndexes) {
    int size=in.readInt();
    if (size > 0) {
      DeletionInfo delInfo=DeletionInfo.serializer().deserializeFromSSTable(in,version);
      int entries=in.readInt();
      List<IndexHelper.IndexInfo> columnsIndex=new ArrayList<IndexHelper.IndexInfo>(entries);
      for (int i=0; i < entries; i++)       columnsIndex.add(IndexHelper.IndexInfo.deserialize(in));
      if (version.hasRowLevelBF)       IndexHelper.skipBloomFilter(in,version.filterType);
      return new IndexedEntry(position,delInfo,columnsIndex);
    }
 else {
      return new RowIndexEntry(position);
    }
  }
 else {
    return new RowIndexEntry(position);
  }
}
