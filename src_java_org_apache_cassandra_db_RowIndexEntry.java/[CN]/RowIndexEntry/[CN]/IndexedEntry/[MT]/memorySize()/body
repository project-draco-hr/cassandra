{
  long entrySize=0;
  for (  IndexHelper.IndexInfo idx : columnsIndex)   entrySize+=idx.memorySize();
  return ObjectSizes.getSuperClassFieldSize(TypeSizes.NATIVE.sizeof(position)) + ObjectSizes.getFieldSize(ObjectSizes.getReferenceSize() + ObjectSizes.getReferenceSize()) + deletionTime.memorySize()+ ObjectSizes.getArraySize(columnsIndex.size(),ObjectSizes.getReferenceSize())+ entrySize+ 4;
}
