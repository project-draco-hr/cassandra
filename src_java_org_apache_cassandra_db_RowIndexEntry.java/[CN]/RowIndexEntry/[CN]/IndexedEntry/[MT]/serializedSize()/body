{
  TypeSizes typeSizes=TypeSizes.NATIVE;
  int size=typeSizes.sizeof(deletionInfo.localDeletionTime) + typeSizes.sizeof(deletionInfo.markedForDeleteAt);
  size+=typeSizes.sizeof(columnsIndex.size());
  for (  IndexHelper.IndexInfo info : columnsIndex)   size+=info.serializedSize(typeSizes);
  return size + (int)FilterFactory.serializedSize(bloomFilter);
}
