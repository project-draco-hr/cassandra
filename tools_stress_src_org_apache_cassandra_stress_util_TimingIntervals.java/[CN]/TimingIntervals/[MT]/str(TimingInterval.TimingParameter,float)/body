{
  StringBuilder sb=new StringBuilder("[");
  for (  Map.Entry<String,TimingInterval> entry : intervals.entrySet()) {
    sb.append(entry.getKey());
    sb.append(":");
    sb.append(entry.getValue().getStringValue(value,rank));
    sb.append(", ");
  }
  sb.setLength(sb.length() - 2);
  sb.append("]");
  return sb.toString();
}
