{
  setBackground(bgColor);
  setFont(new Font(fontName,Font.PLAIN,fontSize));
  this.ringModel=ringModel;
  nodeImageOk=new ImageIcon(getClass().getClassLoader().getResource(nodeImageFileOk)).getImage();
