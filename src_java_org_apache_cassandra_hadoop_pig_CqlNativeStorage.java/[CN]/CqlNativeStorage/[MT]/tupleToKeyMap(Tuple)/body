{
  Map<String,ByteBuffer> keys=new HashMap<String,ByteBuffer>();
  for (int i=0; i < t.size(); i++) {
    if (t.getType(i) != DataType.TUPLE)     throw new IOException("keys was not a tuple");
    Tuple inner=(Tuple)t.get(i);
    if (inner.size() != 2)     throw new IOException("Keys were not in name and value pairs");
    Object name=inner.get(0);
    if (name == null)     throw new IOException("Key name was empty");
    keys.put(name.toString(),objToBB(inner.get(1)));
  }
  return keys;
}
