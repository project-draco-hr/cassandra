{
  Map<ByteBuffer,AbstractType> validators=getValidatorMap(cfDef);
  ByteBuffer cellName=col.name().toByteBuffer();
  if (validators.get(cellName) == null)   return cassandraToObj(getDefaultMarshallers(cfDef).get(MarshallerType.DEFAULT_VALIDATOR),col.value());
 else   return cassandraToObj(validators.get(cellName),col.value());
}
