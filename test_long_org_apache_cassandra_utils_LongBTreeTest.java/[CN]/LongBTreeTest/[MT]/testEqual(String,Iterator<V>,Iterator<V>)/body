{
  while (btree.hasNext() && canon.hasNext()) {
    Object i=btree.next();
    Object j=canon.next();
    if (!i.equals(j))     System.out.println(String.format("%s: Expected %d, Got %d",id,j,i));
  }
  while (btree.hasNext())   System.out.println(String.format("%s: Expected <Nil>, Got %d",id,btree.next()));
  while (canon.hasNext())   System.out.println(String.format("%s: Expected %d, Got Nil",id,canon.next()));
}
