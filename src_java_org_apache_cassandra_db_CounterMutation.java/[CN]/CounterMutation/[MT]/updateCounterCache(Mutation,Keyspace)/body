{
  if (CacheService.instance.counterCache.getCapacity() == 0)   return;
  for (  ColumnFamily cf : applied.getColumnFamilies()) {
    ColumnFamilyStore cfs=keyspace.getColumnFamilyStore(cf.id());
    for (    Cell cell : cf)     if (cell instanceof CounterCell)     cfs.putCachedCounter(key(),cell.name(),CounterContext.instance().getLocalClockAndCount(cell.value()));
  }
}
