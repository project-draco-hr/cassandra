{
  StringBuilder str=new StringBuilder();
  str.append('[');
  str.append(getFileName());
  str.append(" in ");
  str.append(replicas.getDirectories());
  str.append(']');
  if (showContents) {
    str.append(System.lineSeparator());
    str.append("Files and contents follow:");
    str.append(System.lineSeparator());
    replicas.printContentsWithAnyErrors(str);
  }
  return str.toString();
}
