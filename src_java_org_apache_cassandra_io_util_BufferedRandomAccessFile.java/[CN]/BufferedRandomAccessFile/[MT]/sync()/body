{
  if (syncNeeded) {
    flush();
    channel.force(true);
    if (skipCache && bytesSinceCacheFlush > 0) {
      long startAt=0;
      if (pageCacheMigrate)       startAt=minBufferOffset;
      CLibrary.trySkipCache(this.fd,startAt,0);
      minBufferOffset=Long.MAX_VALUE;
      bytesSinceCacheFlush=0;
    }
    syncNeeded=false;
  }
}
