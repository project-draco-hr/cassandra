{
  if (pf == null)   return DBTypeSizes.NATIVE.sizeof(0);
  long size=FBUtilities.serializedUTF8Size(pf.desc.filenameFor(pf.component));
  size+=FBUtilities.serializedUTF8Size(pf.component);
  size+=DBTypeSizes.NATIVE.sizeof(pf.sections.size());
  for (  Pair<Long,Long> section : pf.sections)   size+=DBTypeSizes.NATIVE.sizeof(section.left + DBTypeSizes.NATIVE.sizeof(section.right));
  if (version > MessagingService.VERSION_07)   size+=FBUtilities.serializedUTF8Size(pf.type.name());
  if (version > MessagingService.VERSION_080)   size+=DBTypeSizes.NATIVE.sizeof(pf.estimatedKeys);
  return size;
}
