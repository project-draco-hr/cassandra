{
  this.sstable=sstable;
  this.desc=desc;
  this.component=component;
  this.sections=sections;
  this.type=type;
  long tempSize=0;
  if (compressionInfo == null) {
    for (    Pair<Long,Long> section : sections)     tempSize+=section.right - section.left;
  }
 else {
    for (    CompressionMetadata.Chunk chunk : compressionInfo.chunks)     tempSize+=chunk.length + 4;
  }
  size=tempSize;
  this.estimatedKeys=estimatedKeys;
  this.compressionInfo=compressionInfo;
}
