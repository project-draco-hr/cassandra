{
  if (boundary - currentStart <= MAX_SEGMENT_SIZE) {
    currentSize=boundary - currentStart;
    return;
  }
  if (currentSize > 0) {
    currentStart+=currentSize;
    boundaries.add(currentStart);
  }
  currentSize=boundary - currentStart;
  if (currentSize > MAX_SEGMENT_SIZE) {
    currentStart=boundary;
    boundaries.add(currentStart);
    currentSize=0;
  }
}
