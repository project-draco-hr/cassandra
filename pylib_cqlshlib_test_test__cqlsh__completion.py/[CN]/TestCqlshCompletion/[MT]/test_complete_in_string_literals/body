def test_complete_in_string_literals(self):
    self.trycompletions('insert into system."Index', 'Info"')
    self.trycompletions('USE "', choices=('system', self.cqlsh.keyspace), other_choices_ok=True)
    self.trycompletions("create keyspace blah with replication = {'class': 'Sim", "pleStrategy'")
