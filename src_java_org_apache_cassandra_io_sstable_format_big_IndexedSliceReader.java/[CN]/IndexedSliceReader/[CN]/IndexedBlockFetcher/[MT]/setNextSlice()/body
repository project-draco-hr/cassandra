{
  while (++currentSliceIdx < slices.length) {
    nextIndexIdx=IndexHelper.indexFor(slices[currentSliceIdx].start,indexes,comparator,reversed,nextIndexIdx);
    if (nextIndexIdx < 0 || nextIndexIdx >= indexes.size())     continue;
    IndexInfo info=indexes.get(nextIndexIdx);
    if (reversed) {
      if (!isBeforeSliceStart(info.lastName))       return true;
    }
 else {
      if (!isAfterSliceFinish(info.firstName))       return true;
    }
  }
  nextIndexIdx=-1;
  return false;
}
