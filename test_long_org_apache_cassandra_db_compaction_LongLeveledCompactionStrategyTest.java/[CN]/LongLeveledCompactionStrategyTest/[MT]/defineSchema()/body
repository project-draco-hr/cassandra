{
  Map<String,String> leveledOptions=new HashMap<>();
  leveledOptions.put("sstable_size_in_mb","1");
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE1,KeyspaceParams.simple(1),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARDLVL).compaction(CompactionParams.lcs(leveledOptions)));
}
