{
  List<Accumulator> all=resolverInput(count,false);
  List<List<Accumulator>> result=new ArrayList<>();
  while (!all.isEmpty()) {
    List<Accumulator> is=new ArrayList<>();
    int prev=-1;
    for (    Accumulator i : new ArrayList<>(all)) {
      if (i.base == prev)       continue;
      is.add(i);
      all.remove(i);
      prev=i.base;
    }
    result.add(is);
  }
  return result;
}
