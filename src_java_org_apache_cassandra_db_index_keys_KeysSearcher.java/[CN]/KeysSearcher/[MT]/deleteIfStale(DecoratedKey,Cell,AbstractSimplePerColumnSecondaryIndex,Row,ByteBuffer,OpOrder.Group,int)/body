{
  if (cell == null || !cell.isLive(nowInSec) || index.indexedColumn().type.compare(indexedValue,cell.value()) != 0) {
    index.delete(partitionKey.getKey(),new SimpleClustering(index.indexedColumn().name.bytes),indexedValue,null,new SimpleDeletionTime(indexHit.primaryKeyLivenessInfo().timestamp(),nowInSec),writeOp);
    return true;
  }
  return false;
}
