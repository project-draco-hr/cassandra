{
  String fmt=getFormat(hasEffectiveOwns,isTokenPerNode);
  String owns=hasEffectiveOwns ? "Owns (effective)" : "Owns";
  if (isTokenPerNode)   System.out.printf(fmt,"-","-","Address","Load",owns,"Host ID","Token","Rack");
 else   System.out.printf(fmt,"-","-","Address","Load","Tokens",owns,"Host ID","Rack");
}
