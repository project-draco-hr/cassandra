{
  SchemaLoader.prepareServer();
  new EmbeddedCassandraService().start();
  ThriftSessionManager.instance.setCurrentSocket(new InetSocketAddress(9160));
  SchemaLoader.createKeyspace(KEYSPACE1,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.perRowIndexedCFMD(KEYSPACE1,CF_INDEXED));
  server=new CassandraServer();
  server.set_keyspace(KEYSPACE1);
}
