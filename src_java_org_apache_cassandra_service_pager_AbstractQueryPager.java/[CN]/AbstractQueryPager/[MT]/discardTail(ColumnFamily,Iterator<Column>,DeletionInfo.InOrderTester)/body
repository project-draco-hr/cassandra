{
  ColumnFamily copy=cf.cloneMeShallow();
  int liveCount=columnCounter().countAll(cf).live();
  ColumnCounter counter=columnCounter();
  while (iter.hasNext()) {
    Column c=iter.next();
    counter.count(c,tester);
    if (counter.live() >= liveCount)     break;
    copy.addColumn(c);
  }
  return copy;
}
