{
  ColumnCounter counter=columnCounter();
  while (iter.hasNext()) {
    Column c=iter.next();
    counter.count(c,tester);
    if (counter.live() > toDiscard) {
      copy.addColumn(c);
      while (iter.hasNext())       copy.addColumn(iter.next());
    }
  }
  return Math.min(counter.live(),toDiscard);
}
