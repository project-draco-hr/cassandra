{
  validateKeyspace(keyspace);
  String cfName=parent.column_family.toString();
  String cfType=validateColumnFamily(keyspace,cfName);
  if (cfType.equals("Standard"))   if (parent.super_column != null)   throw newInvalidRequestException("super column specified for standard column family");
  if (parent.super_column != null)   validateColumns(keyspace,cfName,null,Arrays.asList(parent.super_column));
}
