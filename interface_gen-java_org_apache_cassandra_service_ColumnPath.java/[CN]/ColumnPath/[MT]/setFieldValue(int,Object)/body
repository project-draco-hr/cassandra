{
switch (fieldID) {
case COLUMN_FAMILY:
    if (value == null) {
      unsetColumn_family();
    }
 else {
      setColumn_family((String)value);
    }
  break;
case SUPER_COLUMN:
if (value == null) {
  unsetSuper_column();
}
 else {
  setSuper_column((byte[])value);
}
break;
case COLUMN:
if (value == null) {
unsetColumn();
}
 else {
setColumn((byte[])value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
