{
  CBuilder builder=getIndexComparator().prefixBuilder();
  builder.add(rowKey);
  for (int i=0; i < Math.min(columnDef.position(),cellName.size()); i++)   builder.add(cellName.get(i));
  if (cellName.size() >= columnDef.position() + 1)   builder.add(cellName.get(columnDef.position() + 1));
  return builder.build();
}
