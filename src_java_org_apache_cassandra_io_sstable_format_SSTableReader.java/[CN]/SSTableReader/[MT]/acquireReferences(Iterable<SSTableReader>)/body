{
  SSTableReader failed=null;
  for (  SSTableReader sstable : sstables) {
    if (!sstable.acquireReference()) {
      failed=sstable;
      break;
    }
  }
  if (failed == null)   return true;
  for (  SSTableReader sstable : sstables) {
    if (sstable == failed)     break;
    sstable.releaseReference();
  }
  return false;
}
