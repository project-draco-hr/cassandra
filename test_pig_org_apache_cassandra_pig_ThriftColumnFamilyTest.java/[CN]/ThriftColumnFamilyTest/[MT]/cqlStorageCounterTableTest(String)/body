{
  pig.registerQuery(initialQuery);
  Iterator<Tuple> it=pig.openIterator("cc_data");
  int count=0;
  while (it.hasNext()) {
    count++;
    Tuple t=it.next();
    if ("chuck".equals(t.get(0)) && "fist".equals(t.get(1)))     Assert.assertEquals(t.get(2),1L);
 else     if ("chuck".equals(t.get(0)) && "kick".equals(t.get(1)))     Assert.assertEquals(t.get(2),3L);
  }
  Assert.assertEquals(count,2);
}
