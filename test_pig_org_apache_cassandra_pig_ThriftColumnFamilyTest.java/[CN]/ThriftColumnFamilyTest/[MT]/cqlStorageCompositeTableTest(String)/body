{
  pig.registerQuery(initialQuery);
  Iterator<Tuple> it=pig.openIterator("compo_data");
  int count=0;
  while (it.hasNext()) {
    count++;
    Tuple t=it.next();
    if ("kick".equals(t.get(0)) && "bruce".equals(t.get(1)) && "bruce".equals(t.get(2)))     Assert.assertEquals(t.get(3),"watch it, mate");
 else     if ("kick".equals(t.get(0)) && "bruce".equals(t.get(1)) && "lee".equals(t.get(2)))     Assert.assertEquals(t.get(3),"oww");
 else     if ("punch".equals(t.get(0)) && "bruce".equals(t.get(1)) && "bruce".equals(t.get(2)))     Assert.assertEquals(t.get(3),"hunh?");
 else     if ("punch".equals(t.get(0)) && "bruce".equals(t.get(1)) && "lee".equals(t.get(2)))     Assert.assertEquals(t.get(3),"ouch");
  }
  Assert.assertEquals(count,4);
}
