{
  List<SecondaryIndexSearcher> indexSearchers=getIndexSearchersForQuery(clause);
  if (indexSearchers.isEmpty())   return Collections.emptyList();
  if (indexSearchers.size() > 1)   throw new RuntimeException("Unable to search across multiple secondary index types");
  return indexSearchers.get(0).search(clause,range,maxResults,dataFilter,countCQL3Rows);
}
