{
  executeSchemaChange("CREATE TABLE %s.compact_table_with_clustering_columns " + "(a int, b int , c int, PRIMARY KEY (a, b))" + " WITH COMPACT STORAGE;");
  assertInvalid("Secondary indexes are not supported on PRIMARY KEY columns in COMPACT STORAGE tables","CREATE INDEX ON %s.compact_table_with_clustering_columns (a);");
  assertInvalid("Secondary indexes are not supported on PRIMARY KEY columns in COMPACT STORAGE tables","CREATE INDEX ON %s.compact_table_with_clustering_columns (b);");
  assertInvalid("Secondary indexes are not supported on COMPACT STORAGE tables that have clustering columns","CREATE INDEX ON %s.compact_table_with_clustering_columns (c);");
}
