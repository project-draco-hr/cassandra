{
switch (field) {
case NAME:
    if (value == null) {
      unsetName();
    }
 else {
      setName((byte[])value);
    }
  break;
case VALUE:
if (value == null) {
  unsetValue();
}
 else {
  setValue((byte[])value);
}
break;
case TIMESTAMP:
if (value == null) {
unsetTimestamp();
}
 else {
setTimestamp((Long)value);
}
break;
}
}
