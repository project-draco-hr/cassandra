{
  for (  String compactOption : new String[]{""," WITH COMPACT STORAGE"}) {
    createTable("CREATE TABLE %s (a int, b int, c int, PRIMARY KEY (a, b))" + compactOption);
    assertInvalidSyntax("SELECT * FROM %s WHERE () = (?, ?)",1,2);
    assertInvalidMessage("Column \"b\" cannot be restricted by an equality relation and an inequality relation","SELECT * FROM %s WHERE a = 0 AND (b) = (?) AND (b) > (?)",0,0);
    assertInvalidMessage("More than one restriction was found for the start bound on b","SELECT * FROM %s WHERE a = 0 AND (b) > (?) AND (b) > (?)",0,1);
    assertInvalidMessage("Multi-column relations can only be applied to clustering columns: a","SELECT * FROM %s WHERE (a, b) = (?, ?)",0,0);
  }
}
