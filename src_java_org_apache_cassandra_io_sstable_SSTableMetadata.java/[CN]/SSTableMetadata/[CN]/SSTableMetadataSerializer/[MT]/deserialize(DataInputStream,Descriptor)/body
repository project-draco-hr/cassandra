{
  EstimatedHistogram rowSizes=EstimatedHistogram.serializer.deserialize(dis);
  EstimatedHistogram columnCounts=EstimatedHistogram.serializer.deserialize(dis);
  ReplayPosition replayPosition=desc.metadataIncludesReplayPosition ? ReplayPosition.serializer.deserialize(dis) : ReplayPosition.NONE;
  long maxTimestamp=desc.containsTimestamp() ? dis.readLong() : Long.MIN_VALUE;
  if (!desc.tracksMaxTimestamp)   maxTimestamp=Long.MIN_VALUE;
  double compressionRatio=desc.hasCompressionRatio ? dis.readDouble() : Double.MIN_VALUE;
  String partitioner=desc.hasPartitioner ? dis.readUTF() : null;
  StreamingHistogram tombstoneHistogram=desc.tracksTombstones ? StreamingHistogram.serializer.deserialize(dis) : defaultTombstoneDropTimeHistogram();
  return new SSTableMetadata(rowSizes,columnCounts,replayPosition,maxTimestamp,compressionRatio,partitioner,tombstoneHistogram);
}
