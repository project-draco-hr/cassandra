    String threadId=null;
    int lastUpdated=0;
    int isDeleted=0;
    int folder=0;
    int uid=0;
    String user=null;
    while (st.hasMoreElements()) {
switch (i) {
case 0:
        user=(String)st.nextElement();
      if (!isNumeric(user))       continue;
    break;
case 1:
  folder=Integer.parseInt((String)st.nextElement());
break;
case 2:
