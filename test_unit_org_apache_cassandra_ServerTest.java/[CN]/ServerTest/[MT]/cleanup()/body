{
  cleanUpFilesystem();
  Table table=Table.open("Table1");
  for (  String cfName : table.getColumnFamilies()) {
    ColumnFamilyStore cfs=table.getColumnFamilyStore(cfName);
    try {
      cfs.reset();
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
  CommitLog.reset();
  cleanUpFilesystem();
}
