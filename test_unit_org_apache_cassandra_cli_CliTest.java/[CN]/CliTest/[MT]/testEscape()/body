{
  String escaped="backspace \\b tab \\t linefeed \\n form feed \\f carriage return \\r duble quote \\\" " + "single quote \\' backslash \\\\";
  String unescaped="backspace \b tab \t linefeed \n form feed \f carriage return \r duble quote \" " + "single quote ' backslash \\";
  assertEquals(unescaped,CliUtils.unescapeSQLString("'" + escaped + "'"));
  assertEquals(escaped,CliUtils.escapeSQLString(unescaped));
}
