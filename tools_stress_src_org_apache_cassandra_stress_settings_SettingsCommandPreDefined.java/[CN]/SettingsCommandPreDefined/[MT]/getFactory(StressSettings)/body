{
  final SeedManager seeds=new SeedManager(settings);
  return new OpDistributionFactory(){
    public OpDistribution get(    Timing timing,    int sampleCount){
      return new FixedOpDistribution(PredefinedOperation.operation(type,timing.newTimer(type.toString(),sampleCount),newGenerator(settings),seeds,settings,add));
    }
    public String desc(){
      return type.toString();
    }
    public Iterable<OpDistributionFactory> each(){
      return Collections.<OpDistributionFactory>singleton(this);
    }
  }
;
}
