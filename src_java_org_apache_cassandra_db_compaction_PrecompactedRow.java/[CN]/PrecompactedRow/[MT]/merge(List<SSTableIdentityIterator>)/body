{
  ColumnFamily cf=null;
  for (  SSTableIdentityIterator row : rows) {
    ColumnFamily thisCF;
    try {
      thisCF=row.getColumnFamilyWithColumns();
    }
 catch (    IOException e) {
      logger.error("Skipping row " + row.getKey() + " in "+ row.getPath(),e);
      continue;
    }
    if (cf == null) {
      cf=thisCF;
    }
 else {
      cf.addAll(thisCF,HeapAllocator.instance);
    }
  }
  return cf;
}
