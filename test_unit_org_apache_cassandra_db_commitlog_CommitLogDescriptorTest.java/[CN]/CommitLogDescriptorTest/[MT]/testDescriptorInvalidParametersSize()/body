{
  Map<String,String> params=new HashMap<>();
  for (int i=0; i < 6000; ++i)   params.put("key" + i,Integer.toString(i,16));
  try {
    CommitLogDescriptor desc=new CommitLogDescriptor(CommitLogDescriptor.VERSION_22,21,new ParameterizedClass("LZ4Compressor",params));
    ByteBuffer buf=ByteBuffer.allocate(1024000);
    CommitLogDescriptor.writeHeader(buf,desc);
    fail("Parameter object too long should fail on writing descriptor.");
  }
 catch (  ConfigurationException e) {
  }
}
