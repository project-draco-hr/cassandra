{
  if (isMarkedForDelete())   return timestamp() < column.timestamp() ? column : this;
  if (column.isMarkedForDelete())   return timestamp() > column.timestamp() ? this : column;
  if (timestamp() == column.timestamp())   return value().compareTo(column.value()) < 0 ? column : this;
  return timestamp() < column.timestamp() ? column : this;
}
