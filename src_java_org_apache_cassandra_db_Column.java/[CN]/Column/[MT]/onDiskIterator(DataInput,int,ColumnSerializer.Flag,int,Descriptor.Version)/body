{
  return new AbstractIterator<OnDiskAtom>(){
    int i=0;
    protected OnDiskAtom computeNext(){
      if (i++ >= count)       return endOfData();
      OnDiskAtom atom;
      try {
        atom=onDiskSerializer().deserializeFromSSTable(in,flag,expireBefore,version);
      }
 catch (      IOException e) {
        throw new IOError(e);
      }
      if (atom == null)       return endOfData();
      return atom;
    }
  }
;
}
