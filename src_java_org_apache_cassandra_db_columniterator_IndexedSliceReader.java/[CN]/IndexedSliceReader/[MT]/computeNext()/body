{
  while (true) {
    if (reversed) {
      OnDiskAtom column=rangeTombstonesReversed.poll();
      if (column != null)       return column;
    }
    OnDiskAtom column=blockColumns.poll();
    if (column == null) {
      if (!fetcher.fetchMoreData())       return endOfData();
    }
 else {
      return column;
    }
  }
}
