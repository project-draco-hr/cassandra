{
  ColumnFamily cf;
  ColumnFamilyStore cfStore=table.getColumnFamilyStore(cfName);
  if (cfStore == null) {
    return null;
  }
  cf=cfStore.getColumnFamily(QueryFilter.getNamesFilter(Util.dk(keyName),new QueryPath(cfName),ByteBuffer.wrap(columnName.getBytes())));
  if (cf == null) {
    return null;
  }
  return cf.getColumn(ByteBuffer.wrap(columnName.getBytes()));
}
