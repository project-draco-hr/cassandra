{
  ColumnFamily cf;
  ColumnFamilyStore cfStore=keyspace.getColumnFamilyStore(cfName);
  if (cfStore == null) {
    return null;
  }
  cf=cfStore.getColumnFamily(Util.namesQueryFilter(cfStore,Util.dk(keyName),columnName));
  if (cf == null) {
    return null;
  }
  return cf.getColumn(Util.cellname(columnName));
}
