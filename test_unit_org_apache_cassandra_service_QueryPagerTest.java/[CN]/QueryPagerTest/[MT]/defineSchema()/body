{
  SchemaLoader.prepareServer();
  SchemaLoader.createKeyspace(KEYSPACE1,SimpleStrategy.class,KSMetaData.optsWithRF(1),SchemaLoader.standardCFMD(KEYSPACE1,CF_STANDARD));
  SchemaLoader.createKeyspace(KEYSPACE_CQL,SimpleStrategy.class,KSMetaData.optsWithRF(1),CFMetaData.compile("CREATE TABLE " + CF_CQL + " ("+ "k text,"+ "c text,"+ "v text,"+ "PRIMARY KEY (k, c))",KEYSPACE_CQL));
  addData();
}
