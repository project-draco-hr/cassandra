{
  assertEquals(key,string(r.key.key));
  assertNotNull(r.cf);
  assertEquals(toString(r.cf),names.length,r.cf.getColumnCount());
  int i=0;
  for (  Cell c : r.cf) {
    String expected=names[i++];
    assertEquals("column " + i + " doesn't match: "+ toString(r.cf),expected,string(c.name()));
  }
}
