{
  cfs().clearUnsafe();
  int nbKeys=10;
  int nbCols=10;
  for (int i=0; i < nbKeys; i++) {
    Mutation rm=new Mutation(KEYSPACE1,bytes("k" + i));
    ColumnFamily cf=rm.addOrGet(CF_STANDARD);
    for (int j=0; j < nbCols; j++)     cf.addColumn(Util.column("c" + j,"",0));
    rm.applyUnsafe();
  }
}
