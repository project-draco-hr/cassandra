{
  try {
    cliClient_.executeCLIStmt(query);
  }
 catch (  InvalidRequestException ire) {
    css_.err.println(ire.why);
    if (css_.debug)     ire.printStackTrace(css_.err);
    if (css_.batch)     System.exit(4);
  }
catch (  Exception e) {
    css_.err.println("Exception " + e.getMessage());
    if (css_.debug)     e.printStackTrace(css_.err);
    if (css_.batch)     System.exit(8);
  }
}
