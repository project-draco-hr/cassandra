{
  try {
    cliClient.executeCLIStmt(query);
  }
 catch (  InvalidRequestException ire) {
    sessionState.err.println(ire.why);
    if (sessionState.debug)     ire.printStackTrace(sessionState.err);
    if (sessionState.batch)     System.exit(4);
  }
catch (  Throwable e) {
    sessionState.err.println((e.getCause() == null) ? e.getMessage() : e.getCause().getMessage());
    if (sessionState.debug)     e.printStackTrace(sessionState.err);
    if (sessionState.batch)     System.exit(8);
  }
}
