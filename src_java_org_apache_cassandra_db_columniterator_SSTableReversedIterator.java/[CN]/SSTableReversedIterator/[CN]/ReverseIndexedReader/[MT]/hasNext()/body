{
  if (!isInit) {
    seekToPosition(indexEntry.position);
    ByteBufferUtil.skipShortLength(file);
    DeletionTime.serializer.skip(file);
    if (sstable.header.hasStatic())     UnfilteredSerializer.serializer.skipStaticRow(file,sstable.header,helper);
    isInit=true;
  }
  if (partition == null) {
    partition=createBuffer(indexes.size());
    partition.populateFrom(this,null,null,new Tester(){
      public boolean isDone(){
        return false;
      }
    }
);
    iterator=partition.unfilteredIterator(columns,Slices.ALL,true);
  }
  return iterator.hasNext();
}
