{
  super(metadata,key,holder.deletionInfo.getPartitionDeletion(),selection.fetchedColumns(),makeStatic(selection,holder,allocator),isReversed,holder.stats);
  Iterator<Row> rowIter=rowIter(metadata,holder,slice,!isReversed,allocator);
  this.iterator=new RowAndTombstoneMergeIterator(metadata.comparator,isReversed).setTo(rowIter,holder.deletionInfo.rangeIterator(slice,isReversed));
  this.row=new ReusableFilteringRow(selection.fetchedColumns().regulars,selection).setDeletionTimestamp(partitionLevelDeletion.markedForDeleteAt());
}
