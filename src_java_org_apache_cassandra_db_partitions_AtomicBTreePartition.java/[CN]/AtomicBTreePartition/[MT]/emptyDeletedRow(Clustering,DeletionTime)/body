      return LivenessInfo.NONE;
    }
    public DeletionTime deletion(){
      return deletion;
    }
    public boolean isEmpty(){
      return true;
    }
    public boolean hasComplexDeletion(){
      return false;
    }
    public Clustering clustering(){
      return clustering;
    }
    public Cell getCell(    ColumnDefinition c){
      return null;
    }
