{
  StringBuilder sb=new StringBuilder("");
  if (file == null) {
    sb.append(table);
    sb.append("@");
    sb.append(columnFamilies.toString());
    sb.append("@");
    sb.append(target);
    sb.append("------->");
    for (    Range<Token> range : ranges) {
      sb.append(range);
      sb.append(" ");
    }
    sb.append(type);
  }
 else {
    sb.append(file.toString());
  }
  return sb.toString();
}
