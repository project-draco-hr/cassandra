{
  StringBuilder sb=new StringBuilder("");
  if (file == null) {
    sb.append(table);
    sb.append("@");
    sb.append(target);
    sb.append("------->");
    for (    Range range : ranges) {
      sb.append(range);
      sb.append(" ");
    }
  }
 else {
    sb.append(file.toString());
  }
  return sb.toString();
}
