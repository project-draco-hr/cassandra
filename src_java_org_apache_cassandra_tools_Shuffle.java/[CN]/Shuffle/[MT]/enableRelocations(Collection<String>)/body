{
  for (  String endpoint : endpoints) {
    try {
      JMXConnection conn=new JMXConnection(endpoint,port);
      getSSProxy(conn.getMbeanServerConn()).enableScheduledRangeXfers();
      conn.close();
    }
 catch (    IOException e) {
      writeln("Failed to enable shuffling on %s!",endpoint);
    }
  }
}
