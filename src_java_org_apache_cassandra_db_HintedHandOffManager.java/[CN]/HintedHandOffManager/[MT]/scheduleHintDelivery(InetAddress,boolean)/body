{
  if (!queuedDeliveries.add(to))   return;
  logger.debug("Scheduling delivery of Hints to {}",to);
  executor.execute(new Runnable(){
    public void run(){
      try {
        if (precompact)         compact();
        deliverHintsToEndpoint(to);
      }
  finally {
        queuedDeliveries.remove(to);
      }
    }
  }
);
}
