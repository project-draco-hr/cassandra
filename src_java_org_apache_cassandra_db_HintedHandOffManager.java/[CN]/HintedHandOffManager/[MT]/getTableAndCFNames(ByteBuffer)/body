{
  int index=ByteBufferUtil.lastIndexOf(joined,SEPARATOR.getBytes(UTF_8)[0],joined.limit());
  if (index == -1 || index < (joined.position() + 1))   throw new RuntimeException("Corrupted hint name " + ByteBufferUtil.bytesToHex(joined));
  try {
    return new String[]{ByteBufferUtil.string(joined,joined.position(),index - joined.position()),ByteBufferUtil.string(joined,index + 1,joined.limit() - (index + 1))};
  }
 catch (  CharacterCodingException e) {
    throw new RuntimeException(e);
  }
}
