{
  ArrayList<Descriptor> descriptors=new ArrayList<>();
  for (  SSTable sstable : hintStore.getDataTracker().getUncompactingSSTables())   descriptors.add(sstable.descriptor);
  try {
    CompactionManager.instance.submitUserDefined(hintStore,descriptors,(int)(System.currentTimeMillis() / 1000)).get();
  }
 catch (  InterruptedException|ExecutionException e) {
    throw new RuntimeException(e);
  }
}
