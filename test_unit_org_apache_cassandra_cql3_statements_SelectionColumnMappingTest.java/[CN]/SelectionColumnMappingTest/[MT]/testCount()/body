{
  SelectionColumns expected=SelectionColumnMapping.newMapping().addMapping(columnSpecification("count",LongType.instance),NULL_DEF);
  verify(expected,"SELECT count(*) FROM %s");
  verify(expected,"SELECT count(1) FROM %s");
  expected=SelectionColumnMapping.newMapping().addMapping(columnSpecification("other_count",LongType.instance),NULL_DEF);
  verify(expected,"SELECT count(*) AS other_count FROM %s");
  verify(expected,"SELECT count(1) AS other_count FROM %s");
}
