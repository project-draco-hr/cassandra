{
  ColumnDefinition kDef=columnDefinition("k");
  ColumnDefinition v1Def=columnDefinition("v1");
  ColumnDefinition v2Def=columnDefinition("v2");
  ColumnDefinition v3Def=columnDefinition("v3");
  SelectionColumnMapping expected=SelectionColumnMapping.newMapping().addMapping(kDef,columnDefinition("k")).addMapping(v1Def,columnDefinition("v1")).addMapping(v2Def,columnDefinition("v2")).addMapping(v3Def,columnDefinition("v3"));
  verify(expected,"SELECT * FROM %s");
}
