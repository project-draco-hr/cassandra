@staticmethod
def create(type, i, counts, latencies):
    if (type == 'read'):
        return Reader(i, counts, latencies)
    elif (type == 'insert'):
        return Inserter(i, counts, latencies)
    elif (type == 'rangeslice'):
        return RangeSlicer(i, counts, latencies)
    else:
        raise RuntimeError, 'Unsupported op!'
