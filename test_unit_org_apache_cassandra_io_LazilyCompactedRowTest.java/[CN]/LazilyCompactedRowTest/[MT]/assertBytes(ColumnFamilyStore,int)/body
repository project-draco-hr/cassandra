{
  Collection<SSTableReader> sstables=cfs.getSSTables();
  AbstractCompactionIterable eager=new CompactionIterable(OperationType.UNKNOWN,sstables,new PreCompactingController(cfs,sstables,gcBefore,false));
  AbstractCompactionIterable lazy=new CompactionIterable(OperationType.UNKNOWN,sstables,new LazilyCompactingController(cfs,sstables,gcBefore,false));
  assertBytes(sstables,eager,lazy);
  eager=new CompactionIterable(OperationType.UNKNOWN,sstables,new PreCompactingController(cfs,sstables,gcBefore,false));
  AbstractCompactionIterable parallel=new ParallelCompactionIterable(OperationType.UNKNOWN,sstables,new CompactionController(cfs,sstables,gcBefore,false),0);
  assertBytes(sstables,eager,parallel);
}
