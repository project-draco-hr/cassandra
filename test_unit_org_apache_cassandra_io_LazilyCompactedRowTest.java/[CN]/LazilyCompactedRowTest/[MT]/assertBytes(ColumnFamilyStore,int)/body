{
  AbstractCompactionStrategy strategy=cfs.getCompactionStrategy();
  Collection<SSTableReader> sstables=cfs.getSSTables();
  AbstractCompactionIterable eager=new CompactionIterable(OperationType.UNKNOWN,strategy.getScanners(sstables),new PreCompactingController(cfs,sstables,gcBefore));
  AbstractCompactionIterable lazy=new CompactionIterable(OperationType.UNKNOWN,strategy.getScanners(sstables),new LazilyCompactingController(cfs,sstables,gcBefore));
  assertBytes(cfs,eager,lazy);
  eager=new CompactionIterable(OperationType.UNKNOWN,strategy.getScanners(sstables),new PreCompactingController(cfs,sstables,gcBefore));
  AbstractCompactionIterable parallel=new ParallelCompactionIterable(OperationType.UNKNOWN,strategy.getScanners(sstables),new CompactionController(cfs,new HashSet<SSTableReader>(sstables),gcBefore),0);
  assertBytes(cfs,eager,parallel);
}
