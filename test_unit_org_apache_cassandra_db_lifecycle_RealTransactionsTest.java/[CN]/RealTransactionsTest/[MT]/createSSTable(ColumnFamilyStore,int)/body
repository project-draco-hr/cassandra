{
  cfs.truncateBlocking();
  String query="INSERT INTO \"%s\".\"%s\" (key, name, val) VALUES (?, ?, ?)";
  try (CQLSSTableWriter writer=CQLSSTableWriter.builder().withCfs(cfs).using(String.format(query,cfs.keyspace.getName(),cfs.name)).build()){
    for (int j=0; j < numPartitions; j++)     writer.addRow(String.format("key%d",j),"col1","0");
  }
   cfs.loadNewSSTables();
}
