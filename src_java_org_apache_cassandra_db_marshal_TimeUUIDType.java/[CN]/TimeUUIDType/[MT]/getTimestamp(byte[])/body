{
  long low=0;
  int mid=0;
  int hi=0;
  for (int i=0; i < 4; i++)   low=(low << 8) | (bytes[i] & 0xff);
  for (int i=4; i < 6; i++)   mid=(mid << 8) | (bytes[i] & 0xff);
  for (int i=6; i < 8; i++)   hi=(hi << 8) | (bytes[i] & 0xff);
  return low + ((long)mid << 32) + ((long)(hi & 0x0FFF) << 48);
}
