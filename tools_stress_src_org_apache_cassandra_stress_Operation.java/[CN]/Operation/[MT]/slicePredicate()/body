{
  final SlicePredicate predicate=new SlicePredicate();
  if (state.settings.columns.slice) {
    int count=state.rowGen.count(index);
    int start=sliceStart(count);
    predicate.setSlice_range(new SliceRange().setStart(state.settings.columns.names.get(start)).setFinish(new byte[]{}).setReversed(false).setCount(count));
  }
 else   predicate.setColumn_names(randomNames());
  return predicate;
}
