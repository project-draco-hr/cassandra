{
  if (!settings.columns.useSuperColumns)   return Collections.singletonList(new ColumnParent(type.table));
 else {
    ColumnParent[] cp=new ColumnParent[settings.columns.superColumns];
    for (int i=0; i < cp.length; i++)     cp[i]=new ColumnParent(type.supertable).setSuper_column(ByteBufferUtil.bytes("S" + i));
    return Arrays.asList(cp);
  }
}
