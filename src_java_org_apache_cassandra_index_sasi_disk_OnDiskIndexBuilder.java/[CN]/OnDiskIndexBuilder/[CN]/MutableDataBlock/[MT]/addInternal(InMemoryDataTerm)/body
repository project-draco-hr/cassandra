{
  TokenTreeBuilder keys=term.keys;
  if (mode == Mode.SPARSE && keys.getTokenCount() <= 5) {
    writeTerm(term,keys);
    sparseValueTerms++;
  }
 else {
    writeTerm(term,offset);
    offset+=keys.serializedSize();
    containers.add(keys);
  }
  if (mode == Mode.SPARSE)   combinedIndex.add(keys.getTokens());
}
