{
  super.flushAndClear(out);
  out.writeInt(mode == Mode.SPARSE ? offset : -1);
  if (containers.size() > 0) {
    for (    TokenTreeBuilder tokens : containers)     tokens.write(out);
  }
  if (mode == Mode.SPARSE && combinedIndex != null)   combinedIndex.finish().write(out);
  alignToBlock(out);
  containers.clear();
  combinedIndex=initCombinedIndex();
  offset=0;
}
