{
  int sidx=binarySearch(slice.start(),next,rows);
  final int start=sidx >= 0 ? sidx : -sidx - 1;
  if (start >= rows)   return Collections.emptyIterator();
  int eidx=binarySearch(slice.end(),start,rows);
  final int end=eidx >= 0 ? eidx : -eidx - 2;
  next=end;
  if (start > end)   return Collections.emptyIterator();
  return new AbstractIterator<Row>(){
    private int i=start;
    protected Row computeNext(){
      if (i >= rows || i > end)       return endOfData();
      return setRowTo(i++);
    }
  }
;
}
