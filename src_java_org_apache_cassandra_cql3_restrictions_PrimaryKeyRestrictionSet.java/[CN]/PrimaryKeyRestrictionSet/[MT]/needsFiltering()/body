{
  int position=0;
  Restriction slice=null;
  for (  Restriction restriction : restrictions) {
    if (handleInFilter(restriction,position))     return true;
    if (slice != null && !slice.getFirstColumn().equals(restriction.getFirstColumn()))     return true;
    if (slice == null && restriction.isSlice())     slice=restriction;
 else     position=restriction.getLastColumn().position() + 1;
  }
  return false;
}
