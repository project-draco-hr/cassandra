{
  this.tablename=tablename;
  this.cfname=cfname;
  this.sstableDirectories=new File[dataFileLocations.length];
  for (int i=0; i < dataFileLocations.length; ++i)   sstableDirectories[i]=new File(dataFileLocations[i],join(tablename,directoryName));
  if (!StorageService.instance.isClientMode()) {
    try {
      for (      File dir : sstableDirectories)       FileUtils.createDirectory(dir);
    }
 catch (    IOException e) {
      throw new IOError(e);
    }
  }
}
