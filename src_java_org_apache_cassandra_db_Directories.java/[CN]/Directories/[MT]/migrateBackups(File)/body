{
  File backupDir=new File(ksDir,BACKUPS_SUBDIR);
  if (!backupDir.exists())   return;
  File[] files=backupDir.listFiles();
  if (files != null) {
    for (    File f : files)     migrateFile(f,ksDir,BACKUPS_SUBDIR);
  }
  if (!backupDir.delete())   logger.info("Old directory {} not deleted by migration as it is not empty",backupDir);
}
