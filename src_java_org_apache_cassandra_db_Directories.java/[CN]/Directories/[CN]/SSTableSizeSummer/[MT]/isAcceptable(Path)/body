{
  String fileName=file.toFile().getName();
  Pair<Descriptor,Component> pair=SSTable.tryComponentFromFilename(file.getParent().toFile(),fileName);
  return pair != null && pair.left.ksname.equals(metadata.ksName) && pair.left.cfname.equals(metadata.cfName) && !visited.contains(fileName) && !alive.contains(fileName);
}
