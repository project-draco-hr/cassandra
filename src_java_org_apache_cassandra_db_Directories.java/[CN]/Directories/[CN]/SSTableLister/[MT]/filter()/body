{
  if (filtered)   return;
  for (  File location : dataPaths) {
    if (BlacklistedDirectories.isUnreadable(location))     continue;
    if (snapshotName != null) {
      getSnapshotDirectory(location,snapshotName).listFiles(getFilter(location));
      continue;
    }
    if (!onlyBackups)     location.listFiles(getFilter(location));
    if (includeBackups)     getBackupsDirectory(location).listFiles(getFilter(location));
  }
  filtered=true;
}
