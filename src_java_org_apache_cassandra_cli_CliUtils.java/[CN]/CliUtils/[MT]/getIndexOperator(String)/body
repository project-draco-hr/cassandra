{
  if (operator.equals("=")) {
    return IndexOperator.EQ;
  }
 else   if (operator.equals(">=")) {
    return IndexOperator.GTE;
  }
 else   if (operator.equals(">")) {
    return IndexOperator.GT;
  }
 else   if (operator.equals("<")) {
    return IndexOperator.LT;
  }
 else   if (operator.equals("<=")) {
    return IndexOperator.LTE;
  }
  return null;
}
