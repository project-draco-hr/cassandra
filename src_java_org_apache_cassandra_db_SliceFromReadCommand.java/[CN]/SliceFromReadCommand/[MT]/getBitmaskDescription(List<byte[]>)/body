{
  StringBuffer bitmaskBuf=new StringBuffer("[");
  if (masks != null) {
    bitmaskBuf.append(masks.size()).append(" bitmasks: ");
    for (    byte[] bitmask : masks) {
      for (      byte b : bitmask) {
        bitmaskBuf.append(String.format("0x%02x ",b));
      }
      bitmaskBuf.append("; ");
    }
  }
  bitmaskBuf.append("]");
  return bitmaskBuf.toString();
}
