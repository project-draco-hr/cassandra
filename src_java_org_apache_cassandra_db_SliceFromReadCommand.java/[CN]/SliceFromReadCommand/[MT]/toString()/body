{
  String bitmaskString=getBitmaskDescription(bitmasks);
  return "SliceFromReadCommand(" + "table='" + table + '\''+ ", key='"+ FBUtilities.bytesToHex(key)+ '\''+ ", column_parent='"+ queryPath+ '\''+ ", start='"+ getComparator().getString(start)+ '\''+ ", finish='"+ getComparator().getString(finish)+ '\''+ ", bitmasks="+ bitmaskString+ ", reversed="+ reversed+ ", count="+ count+ ')';
}
