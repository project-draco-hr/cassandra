{
switch (field) {
case COLUMN:
    if (value == null) {
      unsetColumn();
    }
 else {
      setColumn((Column)value);
    }
  break;
case SUPER_COLUMN:
if (value == null) {
  unsetSuper_column();
}
 else {
  setSuper_column((SuperColumn)value);
}
break;
case COUNTER_COLUMN:
if (value == null) {
unsetCounter_column();
}
 else {
setCounter_column((CounterColumn)value);
}
break;
case COUNTER_SUPER_COLUMN:
if (value == null) {
unsetCounter_super_column();
}
 else {
setCounter_super_column((CounterSuperColumn)value);
}
break;
}
}
