{
  ClientState state=new ClientState(false);
  state.setKeyspace(KEYSPACE);
  for (int i=0; i < 100; i++) {
    QueryProcessor.process(new StringBuilder().append("INSERT INTO ").append(COLUMN_FAMILY).append(" (id, name, value) VALUES ( 'key").append(i).append("', 'colb', 'value").append(i).append("' )").toString(),ConsistencyLevel.QUORUM,new QueryState(state));
    System.out.println("wrote key" + i);
  }
  System.out.println("Done writing.");
}
