{
  try {
    if (in.isEOF())     return endOfData();
    DecoratedKey key=SSTableReader.decodeKey(StorageService.getPartitioner(),desc,ByteBufferUtil.readWithShortLength(in));
    RowIndexEntry.serializer.skip(in,desc);
    return key;
  }
 catch (  IOException e) {
    throw new IOError(e);
  }
}
