{
switch (field) {
case COLUMN_NAME:
    if (value == null) {
      unsetColumn_name();
    }
 else {
      setColumn_name((byte[])value);
    }
  break;
case OP:
if (value == null) {
  unsetOp();
}
 else {
  setOp((IndexOperator)value);
}
break;
case VALUE:
if (value == null) {
unsetValue();
}
 else {
setValue((byte[])value);
}
break;
}
}
