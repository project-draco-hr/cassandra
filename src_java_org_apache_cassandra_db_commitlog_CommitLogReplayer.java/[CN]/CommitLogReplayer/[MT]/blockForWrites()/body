{
  for (  Map.Entry<UUID,AtomicInteger> entry : invalidMutations.entrySet())   logger.info(String.format("Skipped %d mutations from unknown (probably removed) CF with id %s",entry.getValue().intValue(),entry.getKey()));
  FBUtilities.waitOnFutures(futures);
  logger.debug("Finished waiting on mutations from recovery");
  futures.clear();
  for (  Keyspace keyspace : keyspacesRecovered)   futures.addAll(keyspace.flush());
  FBUtilities.waitOnFutures(futures);
  return replayedCount.get();
}
