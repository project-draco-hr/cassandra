{
  List<String> files=new ArrayList<String>();
  Set<InetAddress> existingDestinations=getStreamDestinations();
  InetAddress dest=InetAddress.getByName(host);
  if (!existingDestinations.contains(dest))   return files;
  for (  PendingFile f : StreamOutManager.getOutgoingFiles(dest))   files.add(String.format("%s",f.toString()));
  return files;
}
