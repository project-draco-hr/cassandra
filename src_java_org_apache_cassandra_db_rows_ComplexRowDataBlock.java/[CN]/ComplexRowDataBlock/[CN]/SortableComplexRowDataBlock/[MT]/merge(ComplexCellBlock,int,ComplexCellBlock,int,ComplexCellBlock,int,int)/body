{
  if (isCounter)   CellData.mergeCounterCell(b1.data,idx1,b2.data,idx2,mergedBlock.data,mergedIdx,nowInSec);
 else   CellData.mergeRegularCell(b1.data,idx1,b2.data,idx2,mergedBlock.data,mergedIdx,nowInSec);
  mergedBlock.ensureComplexPathsCapacity(mergedIdx);
  mergedBlock.complexPaths[mergedIdx]=b1.complexPaths[idx1];
}
