{
  long size=ObjectSizes.sizeOfArray(cellIdx) + data.unsharedHeapSizeExcludingData() + ObjectSizes.sizeOfArray(complexPaths);
  for (int i=0; i < complexPaths.length; i++)   if (complexPaths[i] != null)   size+=((MemtableRowData.BufferCellPath)complexPaths[i]).unsharedHeapSizeExcludingData();
  return size;
}
