{
  return String.format("%s Column Family: Column Range Query: \n" + "  Table Name:       %s\n" + "  Column Family:    %s\n"+ "  RowKey:           %s\n"+ "%s"+ "  Limit:            %d\n"+ "  Order By:         %s",cfMetaData_.columnType,cfMetaData_.tableName,cfMetaData_.cfName,rowKey_.explain(),(superColumnKey_ == null) ? "" : "  SuperColumnKey:   " + superColumnKey_.explain() + "\n",limit_,cfMetaData_.indexProperty_);
}
