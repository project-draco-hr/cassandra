{
  ksm.validate();
  if (Schema.instance.getTableDefinition(ksm.name) != null)   throw new ConfigurationException(String.format("Cannot add already existing keyspace '%s'.",ksm.name));
  logger.info(String.format("Create new Keyspace: %s",ksm));
  announce(ksm.toSchema(FBUtilities.timestampMicros()));
}
