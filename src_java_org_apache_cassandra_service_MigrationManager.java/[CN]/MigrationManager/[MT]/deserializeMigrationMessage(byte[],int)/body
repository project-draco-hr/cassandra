{
  if (version < MessagingService.VERSION_11)   throw new IOException("Can't accept schema migrations from Cassandra versions previous to 1.1, please update first.");
  Collection<RowMutation> schema=new ArrayList<RowMutation>();
  DataInputStream in=new DataInputStream(new FastByteArrayInputStream(data));
  int count=in.readInt();
  for (int i=0; i < count; i++)   schema.add(RowMutation.serializer().deserialize(in,version));
  return schema;
}
