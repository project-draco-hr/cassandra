{
  if (logger.isDebugEnabled())   logger.debug("Removing files for transaction {}",name());
  Throwable err=data.removeUnfinishedLeftovers(null);
  if (err != null) {
    logger.info("Failed deleting files for transaction {}, we'll retry after GC and on on server restart",name(),err);
    failedDeletions.add(this);
  }
 else {
    if (logger.isDebugEnabled())     logger.debug("Closing file transaction {}",name());
    data.close();
  }
}
