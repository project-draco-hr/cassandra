{
  boolean supported=false;
  if (numberOfValues() > 0)   supported|=index.supportsOperator(Operator.CONTAINS);
  if (numberOfKeys() > 0)   supported|=index.supportsOperator(Operator.CONTAINS_KEY);
  if (numberOfEntries() > 0)   supported|=index.supportsOperator(Operator.EQ);
  return supported;
}
