{
  boolean supported=false;
  if (numberOfValues() > 0)   supported|=index.supportsExpression(columnDef,Operator.CONTAINS);
  if (numberOfKeys() > 0)   supported|=index.supportsExpression(columnDef,Operator.CONTAINS_KEY);
  if (numberOfEntries() > 0)   supported|=index.supportsExpression(columnDef,Operator.EQ);
  return supported;
}
