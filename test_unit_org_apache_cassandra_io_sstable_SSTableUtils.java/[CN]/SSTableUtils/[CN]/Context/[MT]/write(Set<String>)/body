{
  Map<String,ColumnFamily> map=new HashMap<String,ColumnFamily>();
  for (  String key : keys) {
    ColumnFamily cf=ColumnFamily.create(ksname,cfname);
    cf.addColumn(new Column(ByteBuffer.wrap(key.getBytes()),ByteBuffer.wrap(key.getBytes()),0));
    map.put(key,cf);
  }
  return write(map);
}
