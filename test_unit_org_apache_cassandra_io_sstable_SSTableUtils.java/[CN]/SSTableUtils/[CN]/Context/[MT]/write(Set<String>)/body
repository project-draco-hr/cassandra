{
  Map<String,ColumnFamily> map=new HashMap<String,ColumnFamily>();
  for (  String key : keys) {
    ColumnFamily cf=TreeMapBackedSortedColumns.factory.create(ksname,cfname);
    cf.addColumn(new Column(Util.cellname(key),ByteBufferUtil.bytes(key),0));
    map.put(key,cf);
  }
  return write(map);
}
