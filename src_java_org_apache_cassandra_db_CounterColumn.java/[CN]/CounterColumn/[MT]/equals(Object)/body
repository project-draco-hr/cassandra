{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  CounterColumn column=(CounterColumn)o;
  if (timestamp != column.timestamp)   return false;
  if (timestampOfLastDelete != column.timestampOfLastDelete)   return false;
  if (!Arrays.equals(partitionedCounter,column.partitionedCounter))   return false;
  if (!name.equals(column.name))   return false;
  return value.equals(column.value);
}
