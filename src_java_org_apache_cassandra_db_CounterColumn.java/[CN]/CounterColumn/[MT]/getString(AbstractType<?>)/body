{
  StringBuilder sb=new StringBuilder();
  sb.append(comparator.getString(name));
  sb.append(":");
  sb.append(isMarkedForDelete());
  sb.append(":");
  sb.append(contextManager.toString(value));
  sb.append("@");
  sb.append(timestamp());
  sb.append("!");
  sb.append(timestampOfLastDelete);
  return sb.toString();
}
