{
  byte[] cleanPartitionedCounter=contextManager.cleanNodeCounts(partitionedCounter,node);
  if (cleanPartitionedCounter == partitionedCounter)   return this;
  if (0 == cleanPartitionedCounter.length)   return null;
  return new CounterColumn(name,ByteBuffer.wrap(contextManager.total(cleanPartitionedCounter)),timestamp,cleanPartitionedCounter,timestampOfLastDelete);
}
