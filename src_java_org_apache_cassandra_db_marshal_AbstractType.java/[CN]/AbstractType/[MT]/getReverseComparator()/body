{
  return new Comparator<ByteBuffer>(){
    public int compare(    ByteBuffer o1,    ByteBuffer o2){
      if (o1.remaining() == 0) {
        return o2.remaining() == 0 ? 0 : -1;
      }
      if (o2.remaining() == 0) {
        return 1;
      }
      return -AbstractType.this.compare(o1,o2);
    }
  }
;
}
