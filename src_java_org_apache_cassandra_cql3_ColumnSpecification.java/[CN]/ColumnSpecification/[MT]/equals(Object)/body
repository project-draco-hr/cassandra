{
  if (null == obj)   return false;
  if (!(obj instanceof ColumnSpecification))   return false;
  ColumnSpecification other=(ColumnSpecification)obj;
  return Objects.equal(ksName,other.ksName) && Objects.equal(cfName,other.cfName) && Objects.equal(name,other.name)&& Objects.equal(type,other.type);
}
