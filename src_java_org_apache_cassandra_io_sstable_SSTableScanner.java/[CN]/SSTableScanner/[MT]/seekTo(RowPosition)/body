{
  try {
    long position=sstable.getPosition(seekKey,SSTableReader.Operator.GE);
    if (position < 0) {
      exhausted=true;
      return;
    }
    file.seek(position);
    row=null;
  }
 catch (  IOException e) {
    sstable.markSuspect();
    throw new RuntimeException("corrupt sstable",e);
  }
}
