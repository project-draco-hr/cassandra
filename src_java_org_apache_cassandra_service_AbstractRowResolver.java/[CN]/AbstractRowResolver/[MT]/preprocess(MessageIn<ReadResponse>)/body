{
  MessageIn<ReadResponse> toReplace=null;
  for (  MessageIn<ReadResponse> reply : replies) {
    if (reply.from.equals(message.from)) {
      if (!message.payload.isDigestQuery())       toReplace=reply;
      break;
    }
  }
  if (toReplace != null) {
    replies.remove(toReplace);
    replies.add(message);
    return false;
  }
  replies.add(message);
  return true;
}
