{
  try {
    if (!hasExistingRoles()) {
      QueryProcessor.process(String.format("INSERT INTO %s.%s (role, is_superuser, can_login, salted_hash) " + "VALUES ('%s', true, true, '%s')",AuthKeyspace.NAME,AuthKeyspace.ROLES,DEFAULT_SUPERUSER_NAME,escape(hashpw(DEFAULT_SUPERUSER_PASSWORD))),consistencyForRole(DEFAULT_SUPERUSER_NAME));
      logger.info("Created default superuser role '{}'",DEFAULT_SUPERUSER_NAME);
    }
  }
 catch (  RequestExecutionException e) {
    logger.warn("CassandraRoleManager skipped default role setup: some nodes were not ready");
    throw e;
  }
}
