{
  if (!getRoles(revokee,false).contains(role))   throw new InvalidRequestException(String.format("%s is not a member of %s",revokee,role));
  modifyRoleMembership(revokee,role,"-");
  process(String.format("DELETE FROM %s.%s WHERE role = '%s' and member = '%s'",AuthKeyspace.NAME,AuthKeyspace.ROLE_MEMBERS,escape(role),escape(revokee)),consistencyForRole(role));
}
