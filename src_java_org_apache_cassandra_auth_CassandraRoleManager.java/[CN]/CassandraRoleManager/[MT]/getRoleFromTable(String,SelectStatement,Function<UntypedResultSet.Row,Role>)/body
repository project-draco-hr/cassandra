{
  ResultMessage.Rows rows=statement.execute(QueryState.forInternalCalls(),QueryOptions.forInternalCalls(consistencyForRole(name),Collections.singletonList(ByteBufferUtil.bytes(name))));
  if (rows.result.isEmpty())   return NULL_ROLE;
  return function.apply(UntypedResultSet.create(rows.result).one());
}
