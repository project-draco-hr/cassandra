{
  SessionInfo sessionInfo=session.getSessionInfo();
  logger.info("[Stream #{}] Prepare completed. Receiving {} files({} bytes), sending {} files({} bytes)",session.planId(),sessionInfo.getTotalFilesToReceive(),sessionInfo.getTotalSizeToReceive(),sessionInfo.getTotalFilesToSend(),sessionInfo.getTotalSizeToSend());
  StreamEvent.SessionPreparedEvent event=new StreamEvent.SessionPreparedEvent(planId,sessionInfo);
  sessionStates.put(sessionInfo.peer,sessionInfo);
  fireStreamEvent(event);
}
