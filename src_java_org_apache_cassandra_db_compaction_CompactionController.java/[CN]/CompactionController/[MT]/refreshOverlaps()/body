{
  if (this.overlappingSSTables != null)   overlappingSSTables.release();
  if (compacting == null)   overlappingSSTables=Refs.tryRef(Collections.<SSTableReader>emptyList());
 else   overlappingSSTables=cfs.getAndReferenceOverlappingSSTables(compacting);
  this.overlappingTree=DataTracker.buildIntervalTree(overlappingSSTables);
}
