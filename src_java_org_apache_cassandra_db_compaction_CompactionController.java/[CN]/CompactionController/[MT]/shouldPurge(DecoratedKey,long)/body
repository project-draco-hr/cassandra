{
  List<SSTableReader> filteredSSTables=overlappingTree.search(key);
  for (  SSTableReader sstable : filteredSSTables) {
    if (sstable.getMinTimestamp() <= maxDeletionTimestamp) {
      if (sstable.getBloomFilter() instanceof AlwaysPresentFilter && sstable.getPosition(key,SSTableReader.Operator.EQ) != null)       return false;
 else       if (sstable.getBloomFilter().isPresent(key.key))       return false;
    }
  }
  return true;
}
