def recv_truncate(self):
    (fname, mtype, rseqid) = self._iprot.readMessageBegin()
    if (mtype == TMessageType.EXCEPTION):
        x = TApplicationException()
        x.read(self._iprot)
        self._iprot.readMessageEnd()
        raise x
    result = truncate_result()
    result.read(self._iprot)
    self._iprot.readMessageEnd()
    if (result.ire != None):
        raise result.ire
    if (result.ue != None):
        raise result.ue
    return
