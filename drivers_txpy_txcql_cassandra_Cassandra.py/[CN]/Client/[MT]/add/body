def add(self, key, column_parent, column, consistency_level):
    '\n    Increment or decrement a counter.\n\n    Parameters:\n     - key\n     - column_parent\n     - column\n     - consistency_level\n    '
    self._seqid += 1
    d = self._reqs[self._seqid] = defer.Deferred()
    self.send_add(key, column_parent, column, consistency_level)
    return d
