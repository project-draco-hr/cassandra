def system_drop_keyspace(self, keyspace):
    '\n    drops a keyspace and any column families that are part of it. returns the new schema id.\n\n    Parameters:\n     - keyspace\n    '
    self._seqid += 1
    d = self._reqs[self._seqid] = defer.Deferred()
    self.send_system_drop_keyspace(keyspace)
    return d
