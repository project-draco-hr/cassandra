def send_login(self, auth_request):
    oprot = self._oprot_factory.getProtocol(self._transport)
    oprot.writeMessageBegin('login', TMessageType.CALL, self._seqid)
    args = login_args()
    args.auth_request = auth_request
    args.write(oprot)
    oprot.writeMessageEnd()
    oprot.trans.flush()
