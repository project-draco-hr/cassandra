{
  if (column.type.isMultiCell()) {
    if (value == null)     return;
    for (    ByteBuffer bb : ((Value)value).elements) {
      if (bb == ByteBufferUtil.UNSET_BYTE_BUFFER)       continue;
      params.addCell(clustering,column,writer,CellPath.create(bb),ByteBufferUtil.EMPTY_BYTE_BUFFER);
    }
  }
 else {
    if (value == null)     params.addTombstone(column,writer);
 else     params.addCell(clustering,column,writer,value.get(Server.CURRENT_VERSION));
  }
}
