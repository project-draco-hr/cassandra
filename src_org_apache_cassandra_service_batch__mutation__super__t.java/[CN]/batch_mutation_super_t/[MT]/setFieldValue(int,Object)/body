{
switch (fieldID) {
case TABLE:
    setTable((String)value);
  break;
case KEY:
setKey((String)value);
break;
case CFMAP:
setCfmap((Map<String,List<superColumn_t>>)value);
break;
case CFMAPDEL:
setCfmapdel((Map<String,List<superColumn_t>>)value);
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
