{
  if (cfType == null) {
    if (mutation.getColumn_or_supercolumn().isSetSuper_column() || mutation.getColumn_or_supercolumn().isSetCounter_super_column())     cfType=CFType.SUPER;
 else     cfType=CFType.NORMAL;
    if (mutation.getColumn_or_supercolumn().isSetCounter_column() || mutation.getColumn_or_supercolumn().isSetCounter_super_column())     colType=ColType.COUNTER;
 else     colType=ColType.NORMAL;
  }
}
