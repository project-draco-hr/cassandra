{
  super(name);
  this.properties=properties;
  this.ifNotExists=ifNotExists;
  this.staticColumns=staticColumns;
  if (!this.properties.hasProperty(CFPropDefs.KW_COMPRESSION) && CFMetaData.DEFAULT_COMPRESSOR != null)   this.properties.addProperty(CFPropDefs.KW_COMPRESSION,new HashMap<String,String>(){
{
      put(CompressionParameters.SSTABLE_COMPRESSION,CFMetaData.DEFAULT_COMPRESSOR);
    }
  }
);
}
