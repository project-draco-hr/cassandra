{
  List<ColumnDefinition> columnDefs=new ArrayList<>(columns.size());
  Integer componentIndex=comparator.isCompound() ? comparator.clusteringPrefixSize() : null;
  for (  Map.Entry<ColumnIdentifier,AbstractType> col : columns.entrySet())   columnDefs.add(ColumnDefinition.regularDef(cfm,col.getKey().bytes,col.getValue(),componentIndex));
  return columnDefs;
}
