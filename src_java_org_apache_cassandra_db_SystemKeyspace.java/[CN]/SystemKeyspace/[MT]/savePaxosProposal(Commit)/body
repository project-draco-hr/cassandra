{
  processInternal(String.format("UPDATE %s USING TIMESTAMP %d AND TTL %d SET proposal_ballot = %s, proposal = 0x%s WHERE row_key = 0x%s AND cf_id = %s",PAXOS_CF,UUIDGen.microsTimestamp(proposal.ballot),paxosTtl(proposal.update.metadata),proposal.ballot,ByteBufferUtil.bytesToHex(proposal.update.toBytes()),ByteBufferUtil.bytesToHex(proposal.key),proposal.update.id()));
}
