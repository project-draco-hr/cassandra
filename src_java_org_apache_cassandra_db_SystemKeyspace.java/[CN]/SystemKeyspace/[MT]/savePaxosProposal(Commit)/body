{
  executeInternal(String.format("UPDATE system.%s USING TIMESTAMP ? AND TTL ? SET proposal_ballot = ?, proposal = ? WHERE row_key = ? AND cf_id = ?",PAXOS_CF),UUIDGen.microsTimestamp(proposal.ballot),paxosTtl(proposal.update.metadata),proposal.ballot,proposal.update.toBytes(),proposal.key,proposal.update.id());
}
