{
  processInternal(String.format("UPDATE %s USING TIMESTAMP %d AND TTL %d SET in_progress_ballot = %s, proposal = 0x%s WHERE row_key = 0x%s AND cf_id = %s",PAXOS_CF,UUIDGen.microsTimestamp(commit.ballot),paxosTtl(commit.update.metadata),commit.ballot,ByteBufferUtil.bytesToHex(commit.update.toBytes()),ByteBufferUtil.bytesToHex(commit.key),commit.update.id()));
}
