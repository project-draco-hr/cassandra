{
  ColumnFamilyStore cfs=Keyspace.open(Keyspace.SYSTEM_KS).getColumnFamilyStore(INDEX_CF);
  QueryFilter filter=QueryFilter.getNamesFilter(decorate(ByteBufferUtil.bytes(keyspaceName)),INDEX_CF,FBUtilities.singleton(cfs.getComparator().makeCellName(indexName),cfs.getComparator()),System.currentTimeMillis());
  return ColumnFamilyStore.removeDeleted(cfs.getColumnFamily(filter),Integer.MAX_VALUE) != null;
}
