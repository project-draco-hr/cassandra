{
  setupVersion();
  migrateIndexInterval();
  migrateCachingOption();
  KSMetaData ksmd=Schema.instance.getKSMetaData(Keyspace.SYSTEM_KS);
  long timestamp=FBUtilities.timestampMicros();
  for (  String cfname : Arrays.asList(SystemKeyspace.SCHEMA_KEYSPACES_CF,SystemKeyspace.SCHEMA_COLUMNFAMILIES_CF,SystemKeyspace.SCHEMA_COLUMNS_CF,SystemKeyspace.SCHEMA_TRIGGERS_CF,SystemKeyspace.SCHEMA_USER_TYPES_CF)) {
    executeOnceInternal(String.format("DELETE FROM system.%s USING TIMESTAMP ? WHERE keyspace_name = ?",cfname),timestamp,ksmd.name);
  }
  ksmd.toSchema(timestamp + 1).apply();
}
