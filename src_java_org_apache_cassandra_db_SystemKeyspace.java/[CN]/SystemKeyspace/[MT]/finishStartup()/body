{
  setupVersion();
  copyAllAliasesToColumnsProper();
  KSMetaData ksmd=Schema.instance.getKSMetaData(Keyspace.SYSTEM_KS);
  for (  String cfname : Arrays.asList(SystemKeyspace.SCHEMA_KEYSPACES_CF,SystemKeyspace.SCHEMA_COLUMNFAMILIES_CF,SystemKeyspace.SCHEMA_COLUMNS_CF,SystemKeyspace.SCHEMA_TRIGGERS_CF))   processInternal(String.format("DELETE FROM system.%s WHERE keyspace_name = '%s'",cfname,ksmd.name));
  ksmd.toSchema(FBUtilities.timestampMicros() + 1).apply();
}
