{
  setupVersion();
  migrateIndexInterval();
  migrateCachingOption();
  for (  String ksname : Schema.systemKeyspaceNames) {
    KSMetaData ksmd=Schema.instance.getKSMetaData(ksname);
    for (    String cfname : Arrays.asList(SystemKeyspace.SCHEMA_KEYSPACES_CF,SystemKeyspace.SCHEMA_COLUMNFAMILIES_CF,SystemKeyspace.SCHEMA_COLUMNS_CF))     executeOnceInternal(String.format("DELETE FROM system.%s WHERE keyspace_name = ?",cfname),ksmd.name);
    ksmd.toSchema(FBUtilities.timestampMicros() + 1).apply();
  }
}
