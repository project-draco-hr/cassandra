{
  String cql="UPDATE %s USING TIMESTAMP %d AND TTL %d SET proposal_ballot = null, proposal = null, most_recent_commit_at = %s, most_recent_commit = 0x%s WHERE row_key = 0x%s AND cf_id = %s";
  processInternal(String.format(cql,PAXOS_CF,UUIDGen.microsTimestamp(commit.ballot),paxosTtl(commit.update.metadata),commit.ballot,ByteBufferUtil.bytesToHex(commit.update.toBytes()),ByteBufferUtil.bytesToHex(commit.key),commit.update.id()));
}
