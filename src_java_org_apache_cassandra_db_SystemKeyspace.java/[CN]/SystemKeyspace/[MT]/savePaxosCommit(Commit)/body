{
  String cql="UPDATE system.%s USING TIMESTAMP ? AND TTL ? SET proposal_ballot = null, proposal = null, most_recent_commit_at = ?, most_recent_commit = ? WHERE row_key = ? AND cf_id = ?";
  executeInternal(String.format(cql,PAXOS_TABLE),UUIDGen.microsTimestamp(commit.ballot),paxosTtl(commit.update.metadata),commit.ballot,commit.update.toBytes(),commit.key,commit.update.id());
}
