{
  String cql="SELECT * FROM %s WHERE keyspace_name='%s' and columnfamily_name='%s' and generation=%d";
  UntypedResultSet results=processInternal(String.format(cql,SSTABLE_ACTIVITY_CF,keyspace,table,generation));
  if (results.isEmpty())   return new RestorableMeter();
  UntypedResultSet.Row row=results.one();
  double m15rate=row.getDouble("rate_15m");
  double m120rate=row.getDouble("rate_120m");
  return new RestorableMeter(m15rate,m120rate);
}
