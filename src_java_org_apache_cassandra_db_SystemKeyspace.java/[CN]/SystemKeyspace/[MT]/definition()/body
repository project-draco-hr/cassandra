{
  Iterable<CFMetaData> tables=Iterables.concat(LegacySchemaTables.All,Arrays.asList(BuiltIndexes,Hints,Batchlog,Paxos,Local,Peers,PeerEvents,RangeXfers,CompactionsInProgress,CompactionHistory,SSTableActivity));
