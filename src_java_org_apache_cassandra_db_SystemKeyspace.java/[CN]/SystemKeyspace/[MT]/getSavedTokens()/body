{
  String req="SELECT tokens FROM system.%s WHERE key='%s'";
  UntypedResultSet result=executeInternal(String.format(req,LOCAL_CF,LOCAL_KEY));
  return result.isEmpty() || !result.one().has("tokens") ? Collections.<Token>emptyList() : deserializeTokens(result.one().<String>getSet("tokens",UTF8Type.instance));
}
