{
  if (count <= 0)   return;
  Long lastSize=sizes.peekLast();
  long newSize=calculateNewSize(count);
  sizes.offer(newSize);
  if (newSize > DataOutputBuffer.MAX_ARRAY_SIZE)   throw new RuntimeException();
  if (newSize < 0)   throw new AssertionError();
  if (lastSize != null && newSize <= lastSize)   throw new AssertionError();
}
