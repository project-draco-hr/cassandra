{
  if (isComplete)   return;
  currentKey=null;
  try {
    CountDownLatch latch=new CountDownLatch(indexes.size());
    for (    Index index : indexes.values())     index.complete(latch);
    Uninterruptibles.awaitUninterruptibly(latch);
  }
  finally {
    indexes.clear();
    isComplete=true;
  }
}
