{
  if (other.columns.length > columns.length)   return false;
  int j=0;
  int cmp=0;
  for (  ColumnDefinition def : other.columns) {
    while (j < columns.length && (cmp=columns[j].compareTo(def)) < 0)     j++;
    if (j >= columns.length || cmp > 0)     return false;
    j++;
  }
  return true;
}
