{
  return new Predicate<ColumnDefinition>(){
    private int i=0;
    public boolean test(    ColumnDefinition column){
      while (i < columns.length) {
        int cmp=column.compareTo(columns[i]);
        if (cmp < 0)         return false;
        i++;
        if (cmp == 0)         return true;
      }
      return false;
    }
  }
;
}
