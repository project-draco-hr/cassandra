{
  Refs<SSTableReader> sstables=getAndReferenceSSTables(cfId);
  for (  SSTableReader sstable : new ArrayList<>(sstables)) {
    if (new Bounds<>(sstable.first.getToken(),sstable.last.getToken()).intersects(Arrays.asList(range)))     sstables.add(sstable);
 else     sstables.release(sstable);
  }
  return sstables;
}
