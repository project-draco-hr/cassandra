{
  Collection<SSTableReader> allSSTables=getAndReferenceSSTables(cfId);
  Set<SSTableReader> sstables=new HashSet<>();
  for (  SSTableReader sstable : allSSTables) {
    if (new Bounds<>(sstable.first.getToken(),sstable.last.getToken()).intersects(Arrays.asList(range)))     sstables.add(sstable);
 else     sstable.releaseReference();
  }
  return sstables;
}
