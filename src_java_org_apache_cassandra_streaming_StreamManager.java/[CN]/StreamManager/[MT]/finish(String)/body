{
  File f=new File(file);
  if (logger_.isDebugEnabled())   logger_.debug("Deleting file " + file + " after streaming "+ f.length()+ "/"+ totalBytesToStream_+ " bytes.");
  FileUtils.delete(file);
  filesToStream_.remove(0);
  if (filesToStream_.size() > 0)   start();
 else {
synchronized (this) {
      if (logger_.isDebugEnabled())       logger_.debug("Signalling that streaming is done for " + to_);
      notifyAll();
    }
  }
}
