{
  StringBuilder sb=new StringBuilder();
  if (!required)   sb.append("[");
  sb.append(displayPrefix);
  if (displayPrefix.endsWith("="))   sb.append("?");
  if (displayPrefix.endsWith("<"))   sb.append("?");
  if (displayPrefix.endsWith(">"))   sb.append("?");
  if (!required)   sb.append("]");
  return sb.toString();
}
