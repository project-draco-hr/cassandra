{
  if (isTracing()) {
    if (callable instanceof TraceSessionFutureTask)     return (TraceSessionFutureTask<T>)callable;
    return new TraceSessionFutureTask<T>(callable,null);
  }
  if (callable instanceof java.util.concurrent.FutureTask)   return (FutureTask<T>)callable;
  return new FutureTask<>(callable);
}
