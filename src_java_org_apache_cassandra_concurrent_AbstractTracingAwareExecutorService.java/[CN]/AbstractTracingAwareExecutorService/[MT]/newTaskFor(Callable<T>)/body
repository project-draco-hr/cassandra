{
  if (isTracing()) {
    if (callable instanceof TraceSessionFutureTask)     return (TraceSessionFutureTask<T>)callable;
    return new TraceSessionFutureTask<T>(callable,Tracing.instance.get());
  }
  if (callable instanceof FutureTask)   return (FutureTask<T>)callable;
  return new FutureTask<>(callable);
}
