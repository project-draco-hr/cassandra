{
  if (traceState != null) {
    if (runnable instanceof TraceSessionFutureTask)     return (TraceSessionFutureTask<T>)runnable;
    return new TraceSessionFutureTask<T>(runnable,result,traceState);
  }
  if (runnable instanceof FutureTask)   return (FutureTask<T>)runnable;
  return new FutureTask<>(runnable,result);
}
