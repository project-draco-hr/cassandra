{
  while (currentSlice != null || idx < slices.length) {
    if (currentSlice == null) {
      ColumnSlice slice=slices[idx++];
      if (forwards)       currentSlice=slice(btree,comparator,slice.start,slice.finish,true);
 else       currentSlice=slice(btree,comparator,slice.finish,slice.start,false);
    }
    if (currentSlice.hasNext())     return currentSlice.next();
    currentSlice=null;
  }
  return endOfData();
}
