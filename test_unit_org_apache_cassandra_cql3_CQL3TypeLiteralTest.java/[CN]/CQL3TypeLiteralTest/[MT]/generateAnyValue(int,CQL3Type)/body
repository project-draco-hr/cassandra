{
  if (type instanceof CQL3Type.Native)   return generateNativeValue(type,false);
  if (type instanceof CQL3Type.Tuple)   return generateTupleValue(version,(TupleType)type.getType(),false);
  if (type instanceof CQL3Type.UserDefined)   return generateUserDefinedValue(version,(UserType)type.getType(),false);
  if (type instanceof CQL3Type.Collection)   return generateCollectionValue(version,(CollectionType)type.getType(),false);
  throw new AssertionError();
}
