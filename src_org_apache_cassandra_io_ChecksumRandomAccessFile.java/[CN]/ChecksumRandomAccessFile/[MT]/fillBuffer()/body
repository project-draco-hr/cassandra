{
  int cnt=0;
  int rem=this.buff_.length;
  int n=-1;
  while (rem > 0) {
    n=super.read(this.buff_,cnt,rem);
    if (n < 0)     break;
    cnt+=n;
    rem-=n;
  }
  if (this.hitEOF_=(cnt < this.buff_.length)) {
    Arrays.fill(this.buff_,cnt,this.buff_.length,(byte)0xff);
  }
  this.diskPos_+=cnt;
  return cnt;
}
