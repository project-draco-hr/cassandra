{
  if (offset < 0)   return false;
  int sPos=src.position() + offset;
  int pPos=prefix.position();
  if (src.remaining() - offset < prefix.remaining())   return false;
  int len=Math.min(src.remaining() - offset,prefix.remaining());
  while (len-- > 0) {
    if (src.get(sPos++) != prefix.get(pPos++))     return false;
  }
  return true;
}
