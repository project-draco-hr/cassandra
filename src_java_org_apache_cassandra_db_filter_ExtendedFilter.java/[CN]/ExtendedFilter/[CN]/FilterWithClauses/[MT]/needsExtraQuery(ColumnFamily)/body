{
  if (!(originalFilter instanceof SliceQueryFilter))   return false;
  SliceQueryFilter filter=(SliceQueryFilter)originalFilter;
  if (filter.start.equals(ByteBufferUtil.EMPTY_BYTE_BUFFER) && filter.finish.equals(ByteBufferUtil.EMPTY_BYTE_BUFFER) && filter.count == Integer.MAX_VALUE)   return false;
  for (  IndexExpression expr : clause) {
    if (data.getColumn(expr.column_name) == null) {
      logger.debug("adding extraFilter to cover additional expressions");
      return true;
    }
  }
  return false;
}
