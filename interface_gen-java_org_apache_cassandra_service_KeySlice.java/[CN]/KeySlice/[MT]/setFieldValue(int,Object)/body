{
switch (fieldID) {
case KEY:
    if (value == null) {
      unsetKey();
    }
 else {
      setKey((String)value);
    }
  break;
case COLUMNS:
if (value == null) {
  unsetColumns();
}
 else {
  setColumns((List<ColumnOrSuperColumn>)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
