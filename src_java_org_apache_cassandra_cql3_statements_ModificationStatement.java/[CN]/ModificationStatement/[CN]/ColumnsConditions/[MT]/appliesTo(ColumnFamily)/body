{
  if (current == null)   return false;
  for (  Column e : expected) {
    Column c=current.getColumn(e.name());
    if (e.isLive(now)) {
      if (c == null || !c.isLive(now) || !c.value().equals(e.value()))       return false;
    }
 else {
      if (c != null && c.isLive(now))       return false;
    }
  }
  return true;
}
