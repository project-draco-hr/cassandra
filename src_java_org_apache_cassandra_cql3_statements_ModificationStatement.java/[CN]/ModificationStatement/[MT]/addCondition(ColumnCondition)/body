{
  List<ColumnCondition> conds=null;
  if (cond.column.kind == CFDefinition.Name.Kind.STATIC) {
    if (staticConditions == null)     staticConditions=new ArrayList<ColumnCondition>();
    conds=staticConditions;
  }
 else {
    if (columnConditions == null)     columnConditions=new ArrayList<ColumnCondition>();
    conds=columnConditions;
  }
  conds.add(cond);
}
