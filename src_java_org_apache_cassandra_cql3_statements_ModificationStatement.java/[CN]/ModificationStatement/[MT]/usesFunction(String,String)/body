{
  if (attrs.usesFunction(ksName,functionName))   return true;
  for (  Restriction restriction : processedKeys.values())   if (restriction != null && restriction.usesFunction(ksName,functionName))   return true;
  for (  Operation operation : columnOperations)   if (operation != null && operation.usesFunction(ksName,functionName))   return true;
  for (  ColumnCondition condition : columnConditions)   if (condition != null && condition.usesFunction(ksName,functionName))   return true;
  for (  ColumnCondition condition : staticConditions)   if (condition != null && condition.usesFunction(ksName,functionName))   return true;
  return false;
}
