{
  if (ifNotExists) {
    request.addNotExist(clustering);
  }
 else   if (ifExists) {
    request.addExist(clustering);
  }
 else {
    if (columnConditions != null)     request.addConditions(clustering,columnConditions,options);
    if (staticConditions != null)     request.addConditions(Clustering.STATIC_CLUSTERING,staticConditions,options);
  }
}
