{
  if (clusterings.contains(Clustering.STATIC_CLUSTERING))   return makeUpdateParameters(keys,new ClusteringIndexSliceFilter(Slices.ALL,false),options,DataLimits.cqlLimits(1),local,now);
  return makeUpdateParameters(keys,new ClusteringIndexNamesFilter(clusterings,false),options,DataLimits.NONE,local,now);
}
