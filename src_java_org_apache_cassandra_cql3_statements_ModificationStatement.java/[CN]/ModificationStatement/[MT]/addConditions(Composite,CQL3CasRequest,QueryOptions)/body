{
  if (ifNotExists) {
    request.addNotExist(clusteringPrefix);
  }
 else   if (ifExists) {
    request.addExist(clusteringPrefix);
  }
 else {
    if (columnConditions != null)     request.addConditions(clusteringPrefix,columnConditions,options);
    if (staticConditions != null)     request.addConditions(cfm.comparator.staticPrefix(),staticConditions,options);
  }
}
