{
  boolean clean=true;
  for (  ColumnFamilyStore cfs : Iterables.concat(Collections.singleton(this),getIndexColumnFamilyStores()))   clean&=cfs.memtable.isClean();
  if (clean) {
    logger.debug("forceFlush requested but everything is clean");
    return null;
  }
  return maybeSwitchMemtable(memtable,true);
}
