{
  Future<?> future;
  Table.switchLock.writeLock().lock();
  try {
    future=postFlushExecutor.submit(new Runnable(){
      public void run(){
      }
    }
);
  }
  finally {
    Table.switchLock.writeLock().unlock();
  }
  try {
    future.get();
  }
 catch (  InterruptedException e) {
    throw new AssertionError(e);
  }
catch (  ExecutionException e) {
    throw new AssertionError(e);
  }
}
