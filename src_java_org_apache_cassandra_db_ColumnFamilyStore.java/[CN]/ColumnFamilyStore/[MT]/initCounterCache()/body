{
  if (!metadata.isCounter() || CacheService.instance.counterCache.getCapacity() == 0)   return;
  long start=System.nanoTime();
  int cachedShardsRead=CacheService.instance.counterCache.loadSaved(this);
  if (cachedShardsRead > 0)   logger.info("Completed loading ({} ms; {} shards) counter cache for {}.{}",TimeUnit.NANOSECONDS.toMillis(System.nanoTime() - start),cachedShardsRead,keyspace.getName(),name);
}
