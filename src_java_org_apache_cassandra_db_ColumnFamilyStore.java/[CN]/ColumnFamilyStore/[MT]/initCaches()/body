{
  long start=System.currentTimeMillis();
  for (  DecoratedKey key : rowCache.readSaved())   cacheRow(key);
  if (rowCache.size() > 0)   logger.info(String.format("completed loading (%d ms; %d keys) row cache for %s.%s",System.currentTimeMillis() - start,rowCache.size(),table.name,columnFamily));
  scheduleCacheSaving(metadata.getRowCacheSavePeriodInSeconds(),metadata.getKeyCacheSavePeriodInSeconds(),metadata.getRowCacheKeysToSave());
}
