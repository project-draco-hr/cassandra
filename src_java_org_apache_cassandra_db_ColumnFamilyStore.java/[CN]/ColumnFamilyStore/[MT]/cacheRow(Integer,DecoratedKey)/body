{
  RowCacheKey key=new RowCacheKey(cfId,decoratedKey);
  ColumnFamily cached;
  if ((cached=CacheService.instance.rowCache.get(key)) == null) {
    cached=getTopLevelColumns(QueryFilter.getIdentityFilter(decoratedKey,new QueryPath(columnFamily)),Integer.MIN_VALUE,true);
    if (cached == null)     return null;
    CacheService.instance.rowCache.put(key,cached);
  }
  return cached;
}
