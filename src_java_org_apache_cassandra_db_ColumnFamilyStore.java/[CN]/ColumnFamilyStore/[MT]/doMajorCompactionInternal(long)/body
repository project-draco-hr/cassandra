{
  List<String> filesInternal=new ArrayList<String>(ssTables_);
  List<String> files;
  if (skip > 0L) {
    files=new ArrayList<String>();
    for (    String file : filesInternal) {
      File f=new File(file);
      if (f.length() < skip * 1024L * 1024L* 1024L) {
        files.add(file);
      }
    }
  }
 else {
    files=filesInternal;
  }
  doFileCompaction(files,BUFSIZE);
}
