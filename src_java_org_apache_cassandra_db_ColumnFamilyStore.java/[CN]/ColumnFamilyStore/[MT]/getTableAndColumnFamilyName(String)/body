{
  StringTokenizer st=new StringTokenizer(filename,"-");
  String[] values=new String[2];
  int i=0;
  while (st.hasMoreElements()) {
    if (i == 0) {
      values[i]=(String)st.nextElement();
    }
 else     if (i == 1) {
      values[i]=(String)st.nextElement();
      break;
    }
    ++i;
  }
  return values;
}
