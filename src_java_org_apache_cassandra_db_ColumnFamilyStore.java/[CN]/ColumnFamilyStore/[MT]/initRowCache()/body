{
  if (!isRowCacheEnabled())   return;
  long start=System.nanoTime();
  int cachedRowsRead=CacheService.instance.rowCache.loadSaved(this);
  if (cachedRowsRead > 0)   logger.info("Completed loading ({} ms; {} keys) row cache for {}.{}",TimeUnit.NANOSECONDS.toMillis(System.nanoTime() - start),cachedRowsRead,keyspace.getName(),name);
}
