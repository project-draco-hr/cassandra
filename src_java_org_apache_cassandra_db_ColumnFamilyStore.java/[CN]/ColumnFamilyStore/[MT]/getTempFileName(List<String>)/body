{
  int lowestIndex;
  int index;
  Collections.sort(files,new FileNameComparator(FileNameComparator.Ascending));
  if (files.size() <= 1) {
    return null;
  }
  lowestIndex=getIndexFromFileName(files.get(0));
  index=lowestIndex + 1;
  return String.format("%s-%s-%s-Data.db",columnFamily_,SSTable.TEMPFILE_MARKER,index);
}
