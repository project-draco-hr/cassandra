{
  PriorityQueue<FileStruct> pq=new PriorityQueue<FileStruct>();
  if (sstables.size() > 1 || (ranges != null && sstables.size() > 0)) {
    FileStruct fs=null;
    for (    SSTableReader sstable : sstables) {
      fs=sstable.getFileStruct();
      fs.advance(true);
      if (fs.isExhausted()) {
        continue;
      }
      pq.add(fs);
    }
  }
  return pq;
}
