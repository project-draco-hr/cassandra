{
  for (  RowCacheKey key : CacheService.instance.rowCache.getKeySet())   if (key.cfId == metadata.cfId)   invalidateCachedRow(key);
  if (metadata.isCounter())   for (  CounterCacheKey key : CacheService.instance.counterCache.getKeySet())   if (key.cfId == metadata.cfId)   CacheService.instance.counterCache.remove(key);
}
