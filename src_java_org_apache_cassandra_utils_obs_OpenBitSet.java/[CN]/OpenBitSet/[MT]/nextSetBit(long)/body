{
  int i=(int)(index >>> 6);
  if (i >= wlen)   return -1;
  int subIndex=(int)index & 0x3f;
  long word=bits[i / PAGE_SIZE][i % PAGE_SIZE] >>> subIndex;
  if (word != 0) {
    return (((long)i) << 6) + (subIndex + BitUtil.ntz(word));
  }
  while (++i < wlen) {
    word=bits[i / PAGE_SIZE][i % PAGE_SIZE];
    if (word != 0)     return (((long)i) << 6) + BitUtil.ntz(word);
  }
  return -1;
}
