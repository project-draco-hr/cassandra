{
  int bitLength=getNumWords();
  int pageSize=getPageSize();
  int pageCount=getPageCount();
  long size=type.sizeof(bitLength);
  for (int p=0; p < pageCount; p++) {
    long[] bits=getPage(p);
    for (int i=0; i < pageSize && bitLength-- > 0; i++)     size+=type.sizeof(bits[i]);
  }
  return size;
}
