{
  if (del.predicate != null && del.predicate.column_names != null) {
    for (    byte[] c : del.predicate.column_names) {
      if (del.super_column == null && DatabaseDescriptor.getColumnFamilyType(rm.table_,cfName) == ColumnFamilyType.Super)       rm.delete(new QueryPath(cfName,c),new TimestampClock(del.timestamp));
 else       rm.delete(new QueryPath(cfName,del.super_column,c),new TimestampClock(del.timestamp));
    }
  }
 else {
    rm.delete(new QueryPath(cfName,del.super_column),new TimestampClock(del.timestamp));
  }
}
