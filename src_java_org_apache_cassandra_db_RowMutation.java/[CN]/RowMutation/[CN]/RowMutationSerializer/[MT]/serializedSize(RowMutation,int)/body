{
  TypeSizes sizes=TypeSizes.NATIVE;
  int size=0;
  if (version < MessagingService.VERSION_20)   size+=sizes.sizeof(rm.getTable());
  int keySize=rm.key().remaining();
  size+=sizes.sizeof((short)keySize) + keySize;
  size+=sizes.sizeof(rm.modifications.size());
  for (  Map.Entry<UUID,ColumnFamily> entry : rm.modifications.entrySet())   size+=ColumnFamily.serializer.serializedSize(entry.getValue(),TypeSizes.NATIVE,version);
  return size;
}
