{
  if (rows == null)   return;
  try (OpOrder.Group opGroup=Keyspace.writeOrder.start()){
    for (    Index index : indexes) {
      Index.Indexer indexer=index.indexerFor(key,nowInSec,opGroup,Type.COMPACTION);
      indexer.begin();
      for (      Row row : rows)       if (row != null)       indexer.removeRow(row);
      indexer.finish();
    }
  }
 }
