{
  return new WrappingUnfilteredRowIterator(iterator){
    public Unfiltered next(){
      Unfiltered next=super.next();
      try {
        next.validateData(metadata());
        return next;
      }
 catch (      MarshalException me) {
        throw new CorruptSSTableException(me,filename);
      }
    }
  }
;
}
