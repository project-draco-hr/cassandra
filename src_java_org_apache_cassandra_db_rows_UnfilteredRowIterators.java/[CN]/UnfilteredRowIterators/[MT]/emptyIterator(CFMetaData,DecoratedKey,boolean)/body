      return DeletionTime.LIVE;
    }
    public Row staticRow(){
      return Rows.EMPTY_STATIC_ROW;
    }
    public RowStats stats(){
      return RowStats.NO_STATS;
    }
    public boolean hasNext(){
      return false;
    }
    public Unfiltered next(){
      throw new NoSuchElementException();
    }
    public void remove(){
    }
    public void close(){
    }
  }
;
}
