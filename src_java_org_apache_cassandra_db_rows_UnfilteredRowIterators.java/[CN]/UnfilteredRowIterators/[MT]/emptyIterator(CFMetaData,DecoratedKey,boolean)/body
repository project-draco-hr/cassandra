{
  return new UnfilteredRowIterator(){
    public CFMetaData metadata(){
      return cfm;
    }
    public boolean isReverseOrder(){
      return isReverseOrder;
    }
    public PartitionColumns columns(){
      return PartitionColumns.NONE;
    }
