{
  super(metadata,iterators.get(0).partitionKey(),partitionDeletion,columns,mergeStaticRows(metadata,iterators,columns.statics,nowInSec,listener,partitionDeletion),reversed,mergeStats(iterators));
  this.listener=listener;
  this.mergeIterator=MergeIterator.get(iterators,reversed ? metadata.comparator.reversed() : metadata.comparator,new MergeReducer(metadata,iterators.size(),reversed,nowInSec));
}
