{
  List<TriggerDef> thriftDefs=new ArrayList<>(triggers.size());
  for (  TriggerDefinition def : triggers) {
    TriggerDef td=new TriggerDef();
    td.setName(def.name);
    td.setOptions(Collections.singletonMap(TriggerDefinition.CLASS,def.classOption));
    thriftDefs.add(td);
  }
  return thriftDefs;
}
