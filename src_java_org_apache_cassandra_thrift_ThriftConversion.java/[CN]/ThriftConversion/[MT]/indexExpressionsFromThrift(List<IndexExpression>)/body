{
  if (exprs == null)   return null;
  if (exprs.isEmpty())   return Collections.emptyList();
  List<org.apache.cassandra.db.IndexExpression> converted=new ArrayList<>(exprs.size());
  for (  IndexExpression expr : exprs) {
    converted.add(new org.apache.cassandra.db.IndexExpression(expr.column_name,Operator.valueOf(expr.op.name()),expr.value));
  }
  return converted;
}
