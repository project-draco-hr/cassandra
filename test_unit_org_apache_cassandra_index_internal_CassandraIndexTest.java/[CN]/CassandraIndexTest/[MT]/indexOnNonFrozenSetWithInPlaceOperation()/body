{
  new TestScript().tableDefinition("CREATE TABLE %s (k int, c int, s set<int>, PRIMARY KEY (k, c));").target("s").indexName("s_index2").withFirstRow(row(0,0,Sets.newHashSet(10,20,30))).withSecondRow(row(1,1,Sets.newHashSet(11,21,31))).missingIndexMessage(StatementRestrictions.REQUIRES_ALLOW_FILTERING_MESSAGE).firstQueryExpression("s CONTAINS 10").secondQueryExpression("s CONTAINS 11").updateExpression("SET s = s - {10}").postUpdateQueryExpression("s CONTAINS 20").run();
}
