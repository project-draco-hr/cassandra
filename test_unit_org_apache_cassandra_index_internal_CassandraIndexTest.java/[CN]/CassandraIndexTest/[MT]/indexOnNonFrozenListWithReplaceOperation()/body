{
  new TestScript().tableDefinition("CREATE TABLE %s (k int, c int, l list<int>, PRIMARY KEY (k, c));").target("l").indexName("l_index1").withFirstRow(row(0,0,Lists.newArrayList(10,20,30))).withSecondRow(row(1,1,Lists.newArrayList(11,21,31))).missingIndexMessage(StatementRestrictions.REQUIRES_ALLOW_FILTERING_MESSAGE).firstQueryExpression("l CONTAINS 10").secondQueryExpression("l CONTAINS 11").updateExpression("SET l = [40, 50, 60]").postUpdateQueryExpression("l CONTAINS 40").run();
}
