{
  int c=0;
  while (c == 0) {
    for (    Row row : partition.iterator(randomBuffer.length,false).next()) {
      Object[] randomRow=randomBuffer[c++];
      for (int i=0; i < argumentIndex.length; i++)       randomRow[i]=row.get(argumentIndex[i]);
      if (c >= randomBuffer.length)       break;
    }
  }
  return c;
}
